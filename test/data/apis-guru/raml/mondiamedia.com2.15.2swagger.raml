        (oas-tags):
          - mmplaylist
types:
  ArticleIdModel:
    properties:
      articleId:
        type: number
  ArticleIdPreviewModel:
    properties:
      articleId:
        type: number
      preview:
        type: boolean
        required: false
  ArticleReport:
    properties:
      date:
        description: 'Date when the event was happen (Use the format "yyyy-MM-ddTHH:mm:ss.SSSZ"'
        type: string
      maxUsageDuration:
        description: Maximal onblock duration in seconds (for usage-Event)
        type: number
        required: false
      offline:
        description: Information if the Track was listend offline (for usage-event)
        type: boolean
        required: false
      realm:
        default: app
        description: Realm/Client of the event
        enum:
          - app
          - web
        type: string
        required: false
      source:
        description: 'Key of the playlist, where this event occurs - Special: MyTracks for personal Playlist'
        type: string
        required: false
      trigger:
        default: playlist
        description: 'Did the user trigger the play of this item (Play, Skip) (for usage-Event)'
        enum:
          - user
          - playlist
        type: string
        required: false
      type:
        description: 'Type of the status event (downloaded, removed, listened)'
        enum:
          - downloaded
          - removed
          - listened
        type: string
      usageDuration:
        description: Total duration in seconds (for usage-Event)
        type: number
        required: false
  ArticleReport_2:
    properties:
      date:
        type: string
      maxUsageDuration:
        type: number
        required: false
      offline:
        type: boolean
        required: false
      realm:
        default: app
        enum:
          - app
          - web
        type: string
        required: false
      source:
        type: string
        required: false
      trigger:
        default: playlist
        enum:
          - user
          - playlist
        type: string
        required: false
      type:
        enum:
          - downloaded
          - removed
          - listened
        type: string
      usageDuration:
        type: number
        required: false
  Config_keyClient_template_keyLifecycleAPIPagesModel:
    properties:
      client_template_key:
        description: Corresponding Client Template Key
        type: string
        required: false
      config_key:
        type: string
        required: false
      lifecycleAPI:
        description: Endpoint for the Lifecycle Service
        type: string
        required: false
      pages:
        items:
          type: KeyPageTypeTitlesContentModel
        type: array
        required: false
  EmptyModel:
    properties: {}
  IdModel:
    properties:
      id:
        type: string
        required: false
  IdModel_2:
    properties:
      id:
        description: Id of the item
        maximum: 2147483647
        minimum: 1
        type: number
  IdsModel:
    properties:
      ids:
        description: Ids of the items
        items:
          type: number
        type: array
  ItemsModel:
    properties:
      items:
        items:
          type: IdModel_2
        type: array
        required: false
  KeyPageTypeTitlesContentModel:
    properties:
      content:
        type: EmptyModel
        required: false
      key:
        type: string
      pageType:
        type: string
      titles:
        type: EmptyModel
        required: false
  PlaylistKeyPlaylistNamePlaylistDescriptionCoverUrlEntitiesModel:
    properties:
      coverUrl:
        type: string
        required: false
      entities:
        items:
          type: TypeSongModel
        type: array
        required: false
      playlistDescription:
        type: string
        required: false
      playlistKey:
        type: string
      playlistName:
        type: string
  TypeSongModel:
    properties:
      song:
        type: EmptyModel
        required: false
      type:
        type: string
        required: false
  UrlTicketPreviewEncryptionModel:
    properties:
      encryption:
        description: used encryption for this song
        type: string
        required: false
      preview:
        type: boolean
        required: false
      ticket:
        type: string
        required: false
      url:
        type: string
        required: false
  UrlTicketPreviewModel:
    properties:
      preview:
        type: boolean
        required: false
      ticket:
        type: string
        required: false
      url:
        type: string
        required: false
