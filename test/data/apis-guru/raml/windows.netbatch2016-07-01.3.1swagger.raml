        required: false
        (oas-x-ms-enum):
          modelAsString: false
          name: CertificateFormat
      data:
        type: string
        (oas-property-title): The base64-encoded contents of the certificate. The maximum size is 10KB.
      password:
        description: This is required if the certificate format is pfx. It should be omitted if the certificate format is cer.
        type: string
        (oas-property-title): The password to access the certificate's private key.
        required: false
