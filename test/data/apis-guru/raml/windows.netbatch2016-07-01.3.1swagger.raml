      exitOptions:
        (oas-property-title): How the Batch service should respond if the task exits with an exit code in the range start to end (inclusive).
        type: ExitOptions
      start:
        format: int32
        type: integer
        (oas-property-title): The first exit code in the range.
    (oas-schema-title): A range of exit codes and how the Batch service should respond to exit codes within that range.
  ExitConditions:
    properties:
      default:
        description: 'Note that the default condition does not include exit code 0. If you want non-default behaviour on exit code 0, you must list it explicitly using the exitCodes or exitCodeRanges collection.'
        (oas-property-title): 'How the Batch service should respond if the task fails with an exit condition not covered by any of the other properties – that is, any nonzero exit code not listed in the exitCodes or exitCodeRanges collection, or a scheduling error if the schedulingError property is not present.'
        type: ExitOptions
        required: false
      exitCodeRanges:
        items:
          type: ExitCodeRangeMapping
        type: array
        (oas-property-title): A list of task exit code ranges and how the Batch service should respond to them.
        required: false
      exitCodes:
        items:
          type: ExitCodeMapping
        type: array
        (oas-property-title): A list of individual task exit codes and how the Batch service should respond to them.
        required: false
      schedulingError:
        (oas-property-title): How the Batch service should respond if the task fails with a scheduling error.
        type: ExitOptions
        required: false
    (oas-schema-title): Specifies how the Batch service should respond when the task completes.
  ExitOptions:
    properties:
      jobAction:
        description: 'The default is none for exit code 0 and terminate for all other exit conditions. It is an error to specify this if the job''s onTaskFailed is noaction. The add task request fails with an invalid property value error; if you are calling the REST API directly, the HTTP status code is 400 (Bad Request).'
        enum:
          - none
          - disable
          - terminate
        type: string
        (oas-property-title): 'An action to take on the job containing the task, if the task completes with the given exit condition and the job’s onTaskFailed property is ''performexitoptionsjobaction''.'
        required: false
        (oas-x-ms-enum):
          modelAsString: false
          name: JobAction
    (oas-schema-title): How the Batch service should respond to a particular exit condition.
  FileProperties:
