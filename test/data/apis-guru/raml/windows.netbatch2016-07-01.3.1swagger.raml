      exitOptions:
        (oas-property-title): How the Batch service should respond if the task exits with this exit code.
        type: ExitOptions
    (oas-schema-title): How the Batch service should respond if a task exits with a particular exit code.
  ExitCodeRangeMapping:
    properties:
