        required: false
      thumbprint:
        type: string
        (oas-property-title): The thumbprint of the certificate.
      thumbprintAlgorithm:
        type: string
        (oas-property-title): The algorithm with which the thumbprint is associated. This must be sha1.
      visibility:
        description: The default is all accounts.
        items:
          enum:
            - starttask
            - task
            - remoteuser
            - unmapped
          type: string
          (oas-x-ms-enum):
            modelAsString: false
            name: CertificateVisibility
