        (oas-property-title): The list of applications available in the account.
        required: false
    (oas-schema-title): The result of listing the applications available in an account.
  ApplicationPackageReference:
    properties:
      applicationId:
        type: string
        (oas-property-title): The ID of the application to deploy.
      version:
        description: 'If this is omitted, and no default version is specified for this application, the request fails with the error code InvalidApplicationPackageReferences. If you are calling the REST API directly, the HTTP status code is 409.'
        type: string
        (oas-property-title): 'The version of the application to deploy. If omitted, the default version is deployed.'
        required: false
