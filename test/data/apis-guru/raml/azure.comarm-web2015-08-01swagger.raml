          - apiVersionParameter
        responses:
          '200':
            body:
              application/json:
                type: SourceControlCollection
            description: OK
        (oas-tags):
          - Provider
      '/{sourceControlType}':
        displayName: '{sourceControlType}'
        uriParameters:
          sourceControlType:
            description: Type of source control
            required: true
            type: string
        put:
          displayName: Provider_UpdateSourceControl
          (oas-summary): Updates source control token
          is:
            - apiVersionParameter
          body:
            application/json:
              type: SourceControl
              (oas-body-name): requestMessage
              description: Source control token information
          responses:
            '200':
              body:
                application/json:
                  type: SourceControl
              description: OK
          (oas-tags):
            - Provider
        get:
          displayName: Provider_GetSourceControl
          (oas-summary): Gets source control token
          is:
            - apiVersionParameter
          responses:
            '200':
              body:
                application/json:
                  type: SourceControl
              description: OK
          (oas-tags):
            - Provider
