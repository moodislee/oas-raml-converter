                  required: false
                results:
                  items:
                    properties:
                      display_name:
                        type: string
                        required: false
                      list_name:
                        type: string
                        required: false
                      list_name_encoded:
                        type: string
                        required: false
                      newest_published_date:
                        type: string
                        required: false
                      oldest_published_date:
                        type: string
                        required: false
                      updated:
                        type: string
                        required: false
                    type: object
                  type: array
                  required: false
                status:
                  type: string
                  required: false
              type: object
              example:
                copyright: Copyright (c) 2016 The New York Times Company.  All Rights Reserved.
                num_results: 53
                results:
                  - display_name: Combined Print & E-Book Fiction
                    list_name: Combined Print and E-Book Fiction
                    list_name_encoded: combined-print-and-e-book-fiction
                    newest_published_date: '2016-03-20'
                    oldest_published_date: '2011-02-13'
                    updated: WEEKLY
                status: OK
      queryParameters:
        api-key:
          type: string
      securedBy:
        - api-key
