      '/{domain_name}.json':
        displayName: '{domain_name}.json'
        uriParameters:
          domain_name:
            required: true
            type: string
        delete:
          displayName: DELETE-api--version-domains--domain_name---format-
          (oas-summary): Delete the specified domain
          responses:
            '200':
              description: No response was specified
          (oas-tags):
            - domains
        put:
          displayName: PUT-api--version-domains--domain_name---format-
          (oas-summary): Update the specified domain
          responses:
            '200':
              description: No response was specified
          (oas-tags):
            - domains
        get:
          displayName: GET-api--version-domains--domain_name---format-
