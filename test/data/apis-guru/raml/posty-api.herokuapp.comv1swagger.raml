      '/{domain_name}':
        displayName: '{domain_name}'
        /users:
          displayName: users
          '/{user_name}.json':
            displayName: '{user_name}.json'
            uriParameters:
              user_name:
                required: true
                type: string
            put:
              displayName: PUT-api--version-domains--domain_name-users--user_name---format-
              (oas-summary): Update the specified user
              responses:
                '200':
                  description: No response was specified
              (oas-tags):
                - domains
            delete:
              displayName: DELETE-api--version-domains--domain_name-users--user_name---format-
              (oas-summary): Delete the specified user
              responses:
                '200':
                  description: No response was specified
              (oas-tags):
                - domains
            get:
              displayName: GET-api--version-domains--domain_name-users--user_name---format-
              (oas-summary): Returns the information to the specified user
              responses:
                '200':
                  description: No response was specified
              (oas-tags):
                - domains
          '/{user_name}':
            displayName: '{user_name}'
            /aliases.json:
              displayName: aliases.json
              get:
                displayName: GET-api--version-domains--domain_name-users--user_name-aliases---format-
                (oas-summary): Returns all aliases for the specified user
                responses:
                  '200':
                    description: No response was specified
                (oas-tags):
                  - domains
              post:
                displayName: POST-api--version-domains--domain_name-users--user_name-aliases---format-
                (oas-summary): Create new user alias
                responses:
                  '200':
                    description: No response was specified
                (oas-tags):
                  - domains
            /aliases:
              displayName: aliases
              '/{alias_name}.json':
                displayName: '{alias_name}.json'
                uriParameters:
                  alias_name:
                    required: true
                    type: string
                delete:
                  displayName: DELETE-api--version-domains--domain_name-users--user_name-aliases--alias_name---format-
                  (oas-summary): Delete the specified user alias
                  responses:
                    '200':
                      description: No response was specified
                  (oas-tags):
                    - domains
                get:
                  displayName: GET-api--version-domains--domain_name-users--user_name-aliases--alias_name---format-
                  (oas-summary): Returns the information to the specified user alias
                  responses:
                    '200':
                      description: No response was specified
                  (oas-tags):
                    - domains
                put:
                  displayName: PUT-api--version-domains--domain_name-users--user_name-aliases--alias_name---format-
                  (oas-summary): Update the specified user alias
                  responses:
                    '200':
                      description: No response was specified
                  (oas-tags):
                    - domains
        /aliases.json:
          displayName: aliases.json
          post:
            displayName: POST-api--version-domains--domain_name-aliases---format-
            (oas-summary): Create new domain alias
            responses:
              '200':
                description: No response was specified
            (oas-tags):
              - domains
          get:
            displayName: GET-api--version-domains--domain_name-aliases---format-
            (oas-summary): Returns all aliases for the specified domain
            responses:
              '200':
                description: No response was specified
            (oas-tags):
              - domains
        /aliases:
          displayName: aliases
          '/{alias_name}.json':
            displayName: '{alias_name}.json'
            uriParameters:
              alias_name:
                required: true
                type: string
            get:
              displayName: GET-api--version-domains--domain_name-aliases--alias_name---format-
              (oas-summary): Returns the information to the specified domain alias
              responses:
                '200':
                  description: No response was specified
              (oas-tags):
                - domains
            delete:
              displayName: DELETE-api--version-domains--domain_name-aliases--alias_name---format-
              (oas-summary): Delete the specified domain alias
              responses:
