(oas-info):
  (oas-x-apisguru-categories):
    - analytics
    - ecommerce
  (oas-x-logo):
    backgroundColor: '#444444'
    url: 'https://api.apis.guru/v2/cache/logo/https_www.indix.com_wp-content_themes_indix2015_images_logo.png'
  (oas-x-origin):
    format: swagger
    url: 'https://developer.indix.com/swagger/spec/INDIX_API_V2.json'
    version: '2.0'
  (oas-x-preferred): true
  (oas-x-providerName): indix.com
  (oas-x-tags):
    - DaaS
    - Product information
annotationTypes:
  oas-x-apisguru-categories: any
