            required: true
            type: string
        get:
          displayName: GET_v2-offersPremium-products-mpid
          description: <p>This operation returns the offers premium information for the specified product across all the stores where it is sold at. This consumes 4 credits.
          (oas-summary): 'Product Details - Returns offers premium information for a product - Credits: 　4'
          responses:
