      /anticipatedpayments:
        displayName: anticipatedpayments
        get:
          displayName: GET_v1-merchant-merchantKey-anticipatedpayments
          description: 'Anticipated payments are returned sorted into descending order by create date, and are paged (with 25 per page)'
          (oas-summary): Retrieve anticipated payments for a merchant
          responses:
            '200':
              body:
                application/hal+json:
                  type: AnticipatedPaymentListRepresentation
              description: Request was successful
