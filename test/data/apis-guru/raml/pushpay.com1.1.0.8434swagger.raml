#%RAML 1.0
title: Pushpay
version: 1.1.0.8434
baseUri: 'https://api.pushpay.com/'
protocols:
  - HTTPS
description: PushpayÂ® improves sales and business processes by making payments simple.
(oas-tags-definition):
  - description: The API for viewing & querying a merchant's community of payers
    name: community
  - description: Supporting API for diagnostics and testing of the platform
    name: diagnostics
  - description: Supporting API for 3rd party system integrations
    name: integrations
  - description: The API for managing merchants and merchant-related data in the pushpay playform
    name: merchants
  - description: Metadata about the various features exposed in the Pushpay API
    name: meta
  - description: The API for managing payments in the pushpay playform (including anticipated payments)
    name: payments
  - description: The API for viewing & querying settlement information
    name: settlements
annotationTypes:
  oas-tags-definition:
    type: array
    items:
      properties:
        name: string
        description?: string
        externalDocs?:
          properties:
            url: string
            description?: string
    allowedTargets: API
  oas-x-logo: any
  oas-x-origin: any
  oas-x-preferred: any
  oas-x-providerName: any
  oas-tags:
    type: 'string[]'
    allowedTargets: Method
  oas-summary:
    type: string
    allowedTargets: Method
  oas-externalDocs:
    properties:
      description?: string
      url: string
    allowedTargets:
      - API
      - Method
      - TypeDeclaration
  oas-info:
    properties:
      termsOfService?: string
      contact?:
        properties:
          name?: string
          url?: string
          email?: string
      license?:
        properties:
          name?: string
          url?: string
    allowedTargets: API
  oas-body-name:
    type: string
    allowedTargets: TypeDeclaration
(oas-info):
  (oas-x-logo):
    url: 'https://api.apis.guru/v2/cache/logo/https_upload.wikimedia.org_wikipedia_en_f_f7_Pushpay_company_logo.png'
  (oas-x-origin):
    format: swagger
    url: 'https://api.pushpay.com/swagger'
    version: '1.2'
  (oas-x-preferred): true
  (oas-x-providerName): pushpay.com
(oas-externalDocs):
  url: 'https://pushpay.io/'
securitySchemes:
  oauth2_accessCode:
    type: OAuth 2.0
    settings:
      authorizationUri: 'https://auth.pushpay.com/oauth/authorize'
      accessTokenUri: 'https://auth.pushpay.com/oauth/token'
      authorizationGrants:
        - authorization_code
      scopes:
        - create_anticipated_payment
        - list_anticipated_payments
        - list_my_merchants
        - manage_webhooks
        - 'merchant:download_file'
        - 'merchant:make_ach_payment'
        - 'merchant:make_card_payment'
        - 'merchant:make_cash_payment'
        - 'merchant:manage_community_members'
        - 'merchant:manage_recurring_payment'
        - 'merchant:upload_file'
        - 'merchant:view_community_members'
        - 'merchant:view_payments'
        - 'merchant:view_recurring_payments'
        - read
  oauth2_implicit:
    type: OAuth 2.0
    settings:
      authorizationUri: 'https://auth.pushpay.com/oauth/authorize'
      accessTokenUri: ''
      authorizationGrants:
        - implicit
      scopes:
        - create_anticipated_payment
        - list_anticipated_payments
        - list_my_merchants
        - manage_webhooks
        - 'merchant:download_file'
        - 'merchant:make_ach_payment'
        - 'merchant:make_card_payment'
        - 'merchant:make_cash_payment'
        - 'merchant:manage_community_members'
        - 'merchant:manage_recurring_payment'
        - 'merchant:upload_file'
        - 'merchant:view_community_members'
        - 'merchant:view_payments'
        - 'merchant:view_recurring_payments'
        - read
/v1:
  displayName: v1
  /merchant:
    displayName: merchant
    '/{merchantKey}':
      displayName: '{merchantKey}'
      /recurringpayment:
        displayName: recurringpayment
        '/{token}':
          displayName: '{token}'
          uriParameters:
            token:
              description: The token is a unique identifier for this resource - it is case-sensitive series of characters
              required: true
              type: string
          get:
            displayName: GET_v1-merchant-merchantKey-recurringpayment-token
            description: 'Retrieves a single recurring payment record, given the keys for the payer and the '
            (oas-summary): Get a person's recurring payment
            responses:
              '200':
                body:
                  application/hal+json:
                    type: RecurringPaymentRepresentation
                description: Request was successful
              '401':
                description: Request failed because the client was not authorized to perform this action
              '404':
                description: Could not find resource
              '429':
                description: Request was throttled due to too many requests from this client or IP address
            securedBy:
              - oauth2_implicit:
                  scopes:
                    - 'merchant:view_recurring_payments'
                    - read
            (oas-tags):
              - payments
        '/{paymentToken}':
          displayName: '{paymentToken}'
          /payments:
            displayName: payments
            get:
              displayName: GET_v1-merchant-merchantKey-recurringpayment-paymentToken-payments
              (oas-summary): Get payment for a recurring payment
              responses:
                '200':
                  body:
                    application/hal+json:
                      type: PaymentsRepresentation
                  description: Request was successful
                '401':
                  description: Request failed because the client was not authorized to perform this action
                '404':
                  description: Could not find resource
                '429':
                  description: Request was throttled due to too many requests from this client or IP address
              queryParameters:
                from:
                  description: Only include payments after a date/time (UTC)
                  format: rfc3339
                  required: false
                  type: datetime
                to:
                  description: Only include payments before a date/time (UTC)
                  format: rfc3339
                  required: false
                  type: datetime
                createdFrom:
                  description: Only include payments after a date/time (UTC)
                  format: rfc3339
                  required: false
                  type: datetime
                createdTo:
                  description: Only include payments before a date/time (UTC)
                  format: rfc3339
                  required: false
                  type: datetime
                updatedFrom:
                  description: Only include payments changed after a date/time (UTC)
                  format: rfc3339
                  required: false
                  type: datetime
                updatedTo:
                  description: Only include payments changed before a date/time (UTC)
                  format: rfc3339
                  required: false
                  type: datetime
                orderby:
                  description: Order data by a particular property in ascending or descending order.  Uses format 'PropertyName ASC' or 'PropertyName DESC'
                  required: false
                  type: string
                type:
                  description: 'Only include payments made with the specified type of payment method (ACH, CreditCard, or NzBankAccount)'
                  enum:
                    - ACH
                    - AchCheck
                    - Cash
                    - CreditCard
                    - DepositedCheck
                    - NzBankAccount
                  required: false
                  type: string
                status:
                  description: 'Only include payments with the specified status (Success, Failed, or Processing)'
                  enum:
                    - Failed
                    - Processing
                    - Success
                  required: false
                  type: string
                fields:
                  description: 'Only include payments that contain a specified value within a custom field. Uses format ''fields[FieldNumber]'''
                  items:
                    type: string
                  required: false
                  type: array
                page:
                  description: 'The page parameter (Note: page numbering starts at 0)'
                  format: int32
                  required: true
                  type: integer
              securedBy:
                - oauth2_implicit:
                    scopes:
                      - 'merchant:view_recurring_payments'
                      - read
              (oas-tags):
                - payments
      /recurringpayments:
        displayName: recurringpayments
        get:
          displayName: GET_v1-merchant-merchantKey-recurringpayments
          description: Retrieves all recurring payment records associated with a merchant
          (oas-summary): Get all recurring payments associated with a merchant
          responses:
            '200':
              body:
                application/hal+json:
                  type: PagedRecurringPaymentsRepresentation
              description: Request was successful
            '401':
              description: Request failed because the client was not authorized to perform this action
            '404':
              description: Could not find resource
            '429':
              description: Request was throttled due to too many requests from this client or IP address
          queryParameters:
            updatedFrom:
              description: Only include payments changed after a date/time (UTC)
              format: rfc3339
              required: false
              type: datetime
            updatedTo:
              description: Only include payments changed before a date/time (UTC)
              format: rfc3339
              required: false
              type: datetime
            createdFrom:
              description: Only include payments after a date/time (UTC)
              format: rfc3339
              required: false
              type: datetime
            createdTo:
              description: Only include payments before a date/time (UTC)
              format: rfc3339
              required: false
              type: datetime
            status:
              description: 'Filter recurring payments by status, acceptable values are any combination of (Active, Cancelled, Paused)'
              items:
                enum:
                  - Active
                  - Cancelled
                  - Completed
                  - Paused
                type: string
              required: true
              type: array
            orderBy:
              description: Order data by a particular property in ascending or descending order.  Uses format 'PropertyName ASC' or 'PropertyName DESC'
              required: false
              type: string
            page:
              description: 'The page parameter (Note: page numbering starts at 0)'
              format: int32
              required: true
              type: integer
          securedBy:
            - oauth2_implicit:
                scopes:
                  - 'merchant:view_recurring_payments'
          (oas-tags):
            - payments
      /settlements:
        displayName: settlements
        get:
          displayName: GET_v1-merchant-merchantKey-settlements
          description: Retrieves all settlements that belong to the in scope merchant & which pass the specified filters
          (oas-summary): Retrieve all settlements that belong to a merchant
          responses:
            '200':
              body:
                application/hal+json:
                  type: SettlementsRepresentation
              description: Request was successful
            '401':
              description: Request failed because the client was not authorized to perform this action
            '404':
              description: Could not find resource
            '429':
              description: Request was throttled due to too many requests from this client or IP address
          queryParameters:
            orderBy:
              description: Order data by a particular property in ascending or descending order.  Uses format 'PropertyName ASC' or 'PropertyName DESC'
              required: false
              type: string
            isReconciled:
              description: Only include settlements which have or have not been reconciled by the user
              required: false
              type: boolean
            name:
              description: 'Only include settlements which contain the following substring, wildcharacters implicitly prepended and appended'
              required: false
              type: string
            type:
              description: Only include settlements which are of the specified type (ACH or CreditCard)
              enum:
                - ACH
                - CreditCard
              required: false
              type: string
            depositFrom:
              description: Only include settlements which have been deposited after the specified date (UTC)
              format: rfc3339
              required: false
              type: datetime
            depositTo:
              description: Only include settlements which have been deposited before the specified date (UTC)
              format: rfc3339
              required: false
              type: datetime
            page:
              description: 'The page parameter (Note: page numbering starts at 0)'
              format: int32
              required: true
              type: integer
          securedBy:
            - oauth2_implicit:
                scopes:
                  - 'merchant:view_payments'
          (oas-tags):
            - settlements
      /webhook:
        displayName: webhook
        '/{token}':
