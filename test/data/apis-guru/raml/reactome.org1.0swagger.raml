#%RAML 1.0
title: Pathway Analysis Service
version: '1.0'
baseUri: 'http://www.reactome.org/'
protocols:
  - HTTP
description: Provides an API for pathway over-representation and expression analysis as well as species comparison tool
(oas-tags-definition):
  - description: download
    name: download
  - description: identifier
    name: identifier
  - description: identifiers
    name: identifiers
  - description: species
    name: species
  - description: token
    name: token
annotationTypes:
  oas-tags-definition:
    type: array
    items:
      properties:
        name: string
        description?: string
        externalDocs?:
          properties:
            url: string
            description?: string
    allowedTargets: API
  oas-x-logo: any
  oas-x-origin: any
  oas-x-preferred: any
  oas-x-providerName: any
  oas-tags:
    type: 'string[]'
    allowedTargets: Method
  oas-summary:
    type: string
    allowedTargets: Method
  oas-info:
    properties:
      termsOfService?: string
      contact?:
        properties:
          name?: string
          url?: string
          email?: string
      license?:
        properties:
          name?: string
          url?: string
    allowedTargets: API
  oas-body-name:
    type: string
    allowedTargets: TypeDeclaration
(oas-info):
  (oas-x-logo):
    backgroundColor: '#191D64'
    url: 'https://api.apis.guru/v2/cache/logo/http_www.reactome.org_wordpress_wp-content_themes_HS_OICR_2013_images_ReactomeLogo.png'
  (oas-x-origin):
    format: swagger
    url: 'http://www.reactome.org/AnalysisService/api-docs'
    version: '1.2'
  (oas-x-preferred): true
  (oas-x-providerName): reactome.org
  contact:
    email: help@reactome.org
  termsOfService: /pages/analysisservice/terms
  license:
    name: Creative Commons Attribution 3.0 Unported License
    url: 'http://creativecommons.org/licenses/by/3.0/legalcode'
/species:
  displayName: species
  /homoSapiens:
    displayName: homoSapiens
    '/{species}':
      displayName: '{species}'
      uriParameters:
        species:
          description: The dbId of the species to compare to
          format: int64
          required: true
          type: integer
      get:
        displayName: GET_species-homoSapiens-species
        description: Use page and pageSize to reduce the amount of data retrieved. Use sortBy and order to sort the result by your preferred option. The resource field will filter the results to show only those corresponding to the preferred molecule type (TOTAL includes all the different molecules type)
        (oas-summary): Compares Homo sapiens to the specified species
        responses:
          '200':
            body:
              application/json:
                type: AnalysisResult
            description: No response was specified
          '404':
            description: Species identifier does not match with any of the species in the current data
        queryParameters:
          pageSize:
            default: 20
            description: pathways per page
            format: int32
            required: false
            type: integer
          page:
            default: 1
            description: page number
            format: int32
            required: false
            type: integer
          sortBy:
            default: ENTITIES_PVALUE
            description: how to sort the result
            enum:
              - NAME
              - TOTAL_ENTITIES
              - TOTAL_INTERACTORS
              - TOTAL_REACTIONS
              - FOUND_ENTITIES
              - FOUND_INTERACTORS
              - FOUND_REACTIONS
              - ENTITIES_RATIO
              - ENTITIES_PVALUE
              - ENTITIES_FDR
              - REACTIONS_RATIO
            required: false
            type: string
          order:
            default: ASC
            description: specifies the order
            enum:
              - ASC
              - DESC
            required: false
            type: string
          resource:
            default: TOTAL
            description: the resource to sort
            enum:
              - TOTAL
              - UNIPROT
              - ENSEMBL
              - CHEBI
              - MIRBASE
              - NCBI_PROTEIN
              - EMBL
              - COMPOUND
            required: false
            type: string
        (oas-tags):
          - species
/token:
  displayName: token
  '/{token}':
    displayName: '{token}'
    /resources:
      displayName: resources
      get:
        displayName: GET_token-token-resources
        description: A summary of the molecules type associated to the submitted data.
        (oas-summary): Returns the resources summary associated with the token
        responses:
          '200':
            body:
              application/json:
                items:
                  type: ResourceSummary
                type: array
            description: No response was specified
          '404':
            description: No result corresponding to the token was found
          '410':
            description: Result deleted due to a new data release
        (oas-tags):
          - token
    uriParameters:
      token:
        description: The token associated with the data to query
        required: true
        type: string
    get:
      displayName: GET_token-token
      description: Use page and pageSize to reduce the amount of data retrieved. Use sortBy and order to sort the result by your preferred option. The resource field will filter the results to show only those corresponding to the preferred molecule type (TOTAL includes all the different molecules type)
      (oas-summary): Returns the result associated with the token
      responses:
        '200':
          body:
            application/json:
              type: AnalysisResult
          description: No response was specified
        '404':
          description: No result corresponding to the token was found
        '410':
          description: Result deleted due to a new data release
      queryParameters:
        pageSize:
          default: 20
          description: pathways per page
          format: int32
          required: false
          type: integer
        page:
          default: 1
          description: page number
          format: int32
          required: false
          type: integer
        sortBy:
          default: ENTITIES_PVALUE
          description: how to sort the result
          enum:
            - NAME
            - TOTAL_ENTITIES
            - TOTAL_INTERACTORS
            - TOTAL_REACTIONS
            - FOUND_ENTITIES
            - FOUND_INTERACTORS
            - FOUND_REACTIONS
            - ENTITIES_RATIO
            - ENTITIES_PVALUE
            - ENTITIES_FDR
            - REACTIONS_RATIO
          required: false
          type: string
        order:
          default: ASC
          description: specifies the order
          enum:
            - ASC
            - DESC
          required: false
          type: string
        resource:
          default: TOTAL
          description: the resource to sort
          enum:
            - TOTAL
            - UNIPROT
            - ENSEMBL
            - CHEBI
            - MIRBASE
            - NCBI_PROTEIN
            - EMBL
            - COMPOUND
          required: false
          type: string
      (oas-tags):
        - token
    /filter:
      displayName: filter
      /pathways:
        displayName: pathways
        post:
          displayName: POST_token-token-filter-pathways
          description: For a given list of pathway identifiers (dbId) it will retrieve a list containing those that are present in the result (with the results for the indicated molecule type)
          (oas-summary): Returns the result for the pathway ids sent by post (when they are present in the original result)
          body:
            application/json:
              type: string
              (oas-body-name): input
              description: 'A comma separated list with the identifiers of the pathways of interest (NOTE: is plain text, not json)'
          responses:
            '200':
              body:
                application/json:
                  items:
                    type: PathwaySummary
                  type: array
              description: No response was specified
            '404':
              description: No result corresponding to the token was found
            '410':
              description: Result deleted due to a new data release
          queryParameters:
            resource:
              default: TOTAL
              description: the resource to sort
              enum:
                - TOTAL
                - UNIPROT
                - ENSEMBL
                - CHEBI
                - MIRBASE
                - NCBI_PROTEIN
                - EMBL
                - COMPOUND
              required: false
              type: string
          (oas-tags):
            - token
      /species:
        displayName: species
        '/{species}':
          displayName: '{species}'
          uriParameters:
            species:
              description: The dbId of the species to filter the result
              format: int64
              required: true
              type: integer
          get:
            displayName: GET_token-token-filter-species-species
            description: filterBySpecies
            (oas-summary): Filters the result by species
            responses:
              '200':
                body:
                  application/json:
                    type: SpeciesFilteredResult
                description: No response was specified
              '404':
                description: No result corresponding to the token was found
              '410':
                description: Result deleted due to a new data release
            queryParameters:
              resource:
                default: TOTAL
                description: the resource to sort
                enum:
                  - TOTAL
                  - UNIPROT
                  - ENSEMBL
                  - CHEBI
                  - MIRBASE
                  - NCBI_PROTEIN
                  - EMBL
                  - COMPOUND
                required: false
                type: string
            (oas-tags):
              - token
    /found:
      displayName: found
      /all:
        displayName: all
        '/{pathway}':
          displayName: '{pathway}'
          uriParameters:
            pathway:
              description: The dbId of the pathway of interest
              format: int64
              required: true
              type: integer
          get:
            displayName: GET_token-token-found-all-pathway
            description: The identifiers submitted by the user that have a match in Reactome database. It also retrieves the mapping to the main identifiers for those that have been found.
            (oas-summary): Returns a summary of the contained identifiers for a pathway in the result for a given token
            responses:
              '200':
                body:
                  application/json:
                    type: FoundElements
                description: No response was specified
              '404':
                description: No result corresponding to the token was found
              '410':
                description: Result deleted due to a new data release
            queryParameters:
              resource:
                default: TOTAL
                description: the resource to sort
                enum:
                  - TOTAL
                  - UNIPROT
                  - ENSEMBL
                  - CHEBI
                  - MIRBASE
                  - NCBI_PROTEIN
                  - EMBL
                  - COMPOUND
                required: false
                type: string
            (oas-tags):
              - token
      /entities:
        displayName: entities
        '/{pathway}':
          displayName: '{pathway}'
          uriParameters:
            pathway:
              description: The dbId of the pathway of interest
              format: int64
              required: true
              type: integer
          get:
            displayName: GET_token-token-found-entities-pathway
            description: The identifiers submitted by the user that have a match in Reactome database. It also retrieves the mapping to the main identifiers for those that have been found.
            (oas-summary): Returns a summary of the contained identifiers for a pathway in the result for a given token
            responses:
              '200':
                body:
                  application/json:
                    type: FoundEntities
                description: No response was specified
              '404':
                description: No result corresponding to the token was found
              '410':
                description: Result deleted due to a new data release
            queryParameters:
              page:
                default: 1
                description: page number
                format: int32
                required: false
                type: integer
              pageSize:
                default: 20
                description: identifiers per page
                format: int32
                required: false
                type: integer
              resource:
                default: TOTAL
                description: the resource to sort
                enum:
                  - TOTAL
                  - UNIPROT
                  - ENSEMBL
                  - CHEBI
                  - MIRBASE
                  - NCBI_PROTEIN
                  - EMBL
                  - COMPOUND
                required: false
                type: string
            (oas-tags):
              - token
      /interactors:
        displayName: interactors
        '/{pathway}':
          displayName: '{pathway}'
          uriParameters:
            pathway:
              description: The dbId of the pathway of interest
              format: int64
              required: true
              type: integer
          get:
            displayName: GET_token-token-found-interactors-pathway
            description: The identifiers submitted by the user that have a match with an interactor in Reactome database. It also retrieves the mapping to the main identifiers (the one interacting with) for those that have been found.
            (oas-summary): Returns a summary of the contained interactors for a pathway in the result for a given token
            responses:
              '200':
                body:
                  application/json:
                    type: FoundInteractors
                description: No response was specified
              '404':
                description: No result corresponding to the token was found
              '410':
                description: Result deleted due to a new data release
            queryParameters:
              page:
                default: 1
                description: page number
                format: int32
                required: false
                type: integer
              pageSize:
                default: 20
                description: identifiers per page
                format: int32
                required: false
                type: integer
              resource:
                default: TOTAL
                description: the resource to sort
                enum:
                  - TOTAL
                  - UNIPROT
                  - ENSEMBL
                  - CHEBI
                  - MIRBASE
                  - NCBI_PROTEIN
                  - EMBL
                  - COMPOUND
                required: false
                type: string
            (oas-tags):
              - token
    /reactions:
      displayName: reactions
      '/{pathway}':
        displayName: '{pathway}'
        uriParameters:
          pathway:
            description: The database identifier of the pathway of interest
            format: int64
            required: true
            type: integer
        get:
          displayName: GET_token-token-reactions-pathway
          description: For a given pathway it returns the identifiers (dbId) of the reactions in the pathway that have been hit with the sample taking into account their participating molecules.
          (oas-summary): Returns the reaction ids of the provided pathway id that are present in the original result
          responses:
            '200':
              body:
                application/json:
                  items: {}
                  type: array
                  uniqueItems: true
              description: No response was specified
            '404':
              description: No result corresponding to the token was found
            '410':
              description: Result deleted due to a new data release
          queryParameters:
            resource:
              default: TOTAL
              description: the resource to sort
              enum:
                - TOTAL
                - UNIPROT
                - ENSEMBL
                - CHEBI
                - MIRBASE
                - NCBI_PROTEIN
                - EMBL
                - COMPOUND
              required: false
              type: string
          (oas-tags):
            - token
      /pathways:
        displayName: pathways
        post:
          displayName: POST_token-token-reactions-pathways
          description: It filters the submitted list and retrieves back only those that at least one of the participating molecules has been hit with the user submitted data.
          (oas-summary): Returns the reaction ids of the pathway ids sent by post that are present in the original result
          body:
            application/json:
              type: string
              (oas-body-name): input
              description: 'A comma separated list with the identifiers of the pathways of interest (NOTE: is plain text, not json)'
          responses:
            '200':
              body:
                application/json:
                  items: {}
                  type: array
                  uniqueItems: true
              description: No response was specified
            '404':
              description: No result corresponding to the token was found
            '410':
              description: Result deleted due to a new data release
          queryParameters:
            resource:
              default: TOTAL
              description: the resource to sort
              enum:
                - TOTAL
                - UNIPROT
                - ENSEMBL
                - CHEBI
                - MIRBASE
                - NCBI_PROTEIN
                - EMBL
                - COMPOUND
              required: false
              type: string
          (oas-tags):
            - token
    /page:
      displayName: page
      '/{pathway}':
        displayName: '{pathway}'
        uriParameters:
          pathway:
            description: The database identifier of the pathway of interest
            format: int64
            required: true
            type: integer
        get:
          displayName: GET_token-token-page-pathway
          description: Useful when implementing UI with tables showing the results in a page way and the user needs to know in which page a certain pathway is present for a given set of sorting and filtering options.
          (oas-summary): Returns the page where the corresponding pathway is taking into account the passed parameters
          responses:
            '200':
              body:
                application/json:
                  format: int32
                  type: integer
              description: No response was specified
            '404':
              description: No result corresponding to the token was found
            '410':
              description: Result deleted due to a new data release
          queryParameters:
            pageSize:
              default: 20
              description: pathways per page
              format: int32
              required: false
              type: integer
            sortBy:
              default: ENTITIES_PVALUE
              description: how to sort the result
              enum:
                - NAME
                - TOTAL_ENTITIES
                - TOTAL_INTERACTORS
                - TOTAL_REACTIONS
                - FOUND_ENTITIES
                - FOUND_INTERACTORS
                - FOUND_REACTIONS
                - ENTITIES_RATIO
                - ENTITIES_PVALUE
                - ENTITIES_FDR
                - REACTIONS_RATIO
              required: false
              type: string
            order:
              default: ASC
              description: specifies the order
              enum:
                - ASC
                - DESC
              required: false
              type: string
            resource:
              default: TOTAL
              description: the resource to sort
              enum:
                - TOTAL
                - UNIPROT
                - ENSEMBL
                - CHEBI
                - MIRBASE
                - NCBI_PROTEIN
                - EMBL
                - COMPOUND
              required: false
              type: string
          (oas-tags):
            - token
    /notFound:
      displayName: notFound
      get:
        displayName: GET_token-token-notFound
        description: Those identifiers that have not been found in the Reactome database
        (oas-summary): Returns a list of the identifiers not found for a given token
        responses:
          '200':
            body:
              application/json:
                items:
                  type: IdentifierSummary
                type: array
            description: No response was specified
          '404':
            description: No result corresponding to the token was found
          '410':
            description: Result deleted due to a new data release
        queryParameters:
          pageSize:
            default: 40
            description: identifiers per page
            format: int32
            required: false
            type: integer
          page:
            default: 1
            description: page number
            format: int32
            required: false
            type: integer
        (oas-tags):
          - token
/download:
  displayName: download
  '/{token}':
    displayName: '{token}'
    /entities:
      displayName: entities
      /found:
        displayName: found
        '/{resource}':
          displayName: '{resource}'
          '/{filename}.csv':
            displayName: '{filename}.csv'
            uriParameters:
              filename:
                default: result
                description: the file name for the downloaded information
                required: true
                type: string
            get:
              displayName: GET_download-token-entities-found-resource-filename-csv
              description: The identifiers are filtered by the selected resource. The filename is the one to be suggested in the download window.
              (oas-summary): Downloads those identifiers found for a given analysis and a certain resource
              responses:
                '200':
                  body:
                    text/csv:
                      type: FileSystemResource
                  description: No response was specified
                '404':
                  description: No result corresponding to the token was found
                '410':
                  description: Result deleted due to a new data release
              (oas-tags):
                - download
      /notfound:
        displayName: notfound
        '/{filename}.csv':
          displayName: '{filename}.csv'
          uriParameters:
            filename:
              default: result
              description: the file name for the downloaded information
              required: true
              type: string
          get:
            displayName: GET_download-token-entities-notfound-filename-csv
            description: Those identifiers from the user sample that are not present up to the current data version. The filename is the one to be suggested in the download window.
            (oas-summary): Downloads a list of the not found identifiers
            responses:
              '200':
                body:
                  text/csv:
                    type: FileSystemResource
                description: No response was specified
              '404':
                description: No result corresponding to the token was found
              '410':
                description: Result deleted due to a new data release
            (oas-tags):
              - download
    /pathways:
      displayName: pathways
      '/{resource}':
        displayName: '{resource}'
        '/{filename}.csv':
          displayName: '{filename}.csv'
          uriParameters:
            filename:
              default: result
              description: the file name for the downloaded information
              required: true
              type: string
          get:
            displayName: GET_download-token-pathways-resource-filename-csv
            description: The results are filtered by the selected resource. The filename is the one to be suggested in the download window.
            (oas-summary): Downloads all hit pathways for a given analysis
            responses:
              '200':
                body:
                  text/csv:
                    type: FileSystemResource
                description: No response was specified
              '404':
                description: No result corresponding to the token was found
              '410':
                description: Result deleted due to a new data release
            (oas-tags):
              - download
