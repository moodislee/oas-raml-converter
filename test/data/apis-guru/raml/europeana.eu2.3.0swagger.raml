          description: Unauthorized
        '403':
          description: Forbidden
        '404':
          description: Not Found
      queryParameters:
        wskey:
          description: wskey
          required: false
          type: string
        callback:
          description: callback
          required: false
          type: string
        edmDatasetName:
          description: edmDatasetName
          required: false
          type: string
        countryCode:
          description: countryCode
          required: false
          type: string
        status:
          description: status
          required: false
          type: string
        offset:
          description: offset
          required: false
          type: string
        pagesize:
          description: pagesize
          required: false
          type: string
      (oas-tags):
        - Providers and Datasets
  /providers.json:
    displayName: providers.json
    get:
      displayName: listProviders
      (oas-summary): get the list of Europeana data providers
      responses:
        '200':
          body:
            application/json:
              type: ModelAndView
          description: OK
