#%RAML 1.0
title: Google Play Game Services
version: v1
baseUri: 'https://www.googleapis.com/games/v1'
protocols:
  - HTTPS
description: The API for Google Play Game Services.
(oas-tags-definition):
  - name: achievementDefinitions
  - name: achievements
  - name: applications
  - name: events
  - name: leaderboards
  - name: metagame
  - name: players
  - name: pushtokens
  - name: questMilestones
  - name: quests
  - name: revisions
  - name: rooms
  - name: scores
  - name: snapshots
  - name: turnBasedMatches
annotationTypes:
  oas-tags-definition:
    type: array
    items:
      properties:
        name: string
        description?: string
        externalDocs?:
          properties:
            url: string
            description?: string
    allowedTargets: API
  oas-x-apiClientRegistration: any
  oas-x-logo: any
  oas-x-origin: any
  oas-x-preferred: any
  oas-x-providerName: any
  oas-x-serviceName: any
  oas-tags:
    type: 'string[]'
    allowedTargets: Method
  oas-externalDocs:
    properties:
      description?: string
      url: string
    allowedTargets:
      - API
      - Method
      - TypeDeclaration
  oas-info:
    properties:
      termsOfService?: string
      contact?:
        properties:
          name?: string
          url?: string
          email?: string
      license?:
        properties:
          name?: string
          url?: string
    allowedTargets: API
  oas-body-name:
    type: string
    allowedTargets: TypeDeclaration
(oas-info):
  (oas-x-apiClientRegistration):
    url: 'https://console.developers.google.com'
  (oas-x-logo):
    url: 'https://api.apis.guru/v2/cache/logo/https_www.gstatic.com_images_icons_material_product_2x_play_games_64dp.png'
  (oas-x-origin):
    format: google
    url: 'https://www.googleapis.com/discovery/v1/apis/games/v1/rest'
    version: v1
  (oas-x-preferred): true
  (oas-x-providerName): googleapis.com
  (oas-x-serviceName): games
  contact:
    name: Google
    url: 'https://google.com'
(oas-externalDocs):
  url: 'https://developers.google.com/games/services/'
securitySchemes:
  Oauth2:
    type: OAuth 2.0
    settings:
      authorizationUri: 'https://accounts.google.com/o/oauth2/auth'
      accessTokenUri: ''
      authorizationGrants:
        - implicit
      scopes:
        - 'https://www.googleapis.com/auth/drive.appdata'
        - 'https://www.googleapis.com/auth/games'
        - 'https://www.googleapis.com/auth/plus.login'
    description: Oauth 2.0 authentication
/pushtokens:
  displayName: pushtokens
  put:
    displayName: games.pushtokens.update
    description: Registers a push token for the current user and application.
    body:
      application/json:
        type: PushToken
        (oas-body-name): body
    responses:
      '200':
        description: Successful response
    queryParameters:
      consistencyToken:
        description: The last-seen mutation timestamp.
        type: string
    securedBy:
      - Oauth2:
          scopes:
            - 'https://www.googleapis.com/auth/plus.login'
    (oas-tags):
      - pushtokens
  is:
    - alt
    - fields
    - key
    - oauthToken
    - prettyPrint
    - quotaUser
    - userIp
  /remove:
    displayName: remove
    post:
      displayName: games.pushtokens.remove
      description: Removes a push token for the current user and application. Removing a non-existent push token will report success.
      body:
        application/json:
          type: PushTokenId
          (oas-body-name): body
      responses:
        '200':
          description: Successful response
      queryParameters:
        consistencyToken:
          description: The last-seen mutation timestamp.
          type: string
      securedBy:
        - Oauth2:
            scopes:
              - 'https://www.googleapis.com/auth/plus.login'
      (oas-tags):
        - pushtokens
    is:
      - alt
      - fields
      - key
      - oauthToken
      - prettyPrint
      - quotaUser
      - userIp
/turnbasedmatches:
  displayName: turnbasedmatches
  '/{matchId}':
    displayName: '{matchId}'
    /turn:
      displayName: turn
      put:
        displayName: games.turnBasedMatches.takeTurn
        description: Commit the results of a player turn.
        body:
          application/json:
            type: TurnBasedMatchTurn
            (oas-body-name): body
        responses:
          '200':
            body:
              application/json:
                type: TurnBasedMatch
            description: Successful response
        queryParameters:
          consistencyToken:
            description: The last-seen mutation timestamp.
            type: string
          language:
            description: The preferred language to use for strings returned by this method.
            type: string
        securedBy:
          - Oauth2:
              scopes:
                - 'https://www.googleapis.com/auth/plus.login'
        (oas-tags):
          - turnBasedMatches
      is:
        - alt
        - fields
        - key
        - oauthToken
        - prettyPrint
        - quotaUser
        - userIp
    uriParameters:
      matchId:
        description: The ID of the match.
        required: true
        type: string
    get:
      displayName: games.turnBasedMatches.get
      description: Get the data for a turn-based match.
      responses:
        '200':
          body:
            application/json:
              type: TurnBasedMatch
          description: Successful response
      queryParameters:
        consistencyToken:
          description: The last-seen mutation timestamp.
          type: string
        includeMatchData:
          description: Get match data along with metadata.
          type: boolean
        language:
          description: The preferred language to use for strings returned by this method.
          type: string
      securedBy:
        - Oauth2:
            scopes:
              - 'https://www.googleapis.com/auth/plus.login'
      (oas-tags):
        - turnBasedMatches
    is:
      - alt
      - fields
      - key
      - oauthToken
      - prettyPrint
      - quotaUser
      - userIp
    /cancel:
      displayName: cancel
      put:
        displayName: games.turnBasedMatches.cancel
        description: Cancel a turn-based match.
        responses:
          '200':
            description: Successful response
        queryParameters:
          consistencyToken:
            description: The last-seen mutation timestamp.
            type: string
        securedBy:
          - Oauth2:
              scopes:
                - 'https://www.googleapis.com/auth/plus.login'
        (oas-tags):
          - turnBasedMatches
      is:
        - alt
        - fields
        - key
        - oauthToken
        - prettyPrint
        - quotaUser
        - userIp
    /decline:
      displayName: decline
      put:
        displayName: games.turnBasedMatches.decline
        description: Decline an invitation to play a turn-based match.
        responses:
          '200':
            body:
              application/json:
                type: TurnBasedMatch
            description: Successful response
        queryParameters:
          consistencyToken:
            description: The last-seen mutation timestamp.
            type: string
          language:
            description: The preferred language to use for strings returned by this method.
            type: string
        securedBy:
          - Oauth2:
              scopes:
                - 'https://www.googleapis.com/auth/plus.login'
        (oas-tags):
          - turnBasedMatches
      is:
        - alt
        - fields
        - key
        - oauthToken
        - prettyPrint
        - quotaUser
        - userIp
    /dismiss:
      displayName: dismiss
      put:
        displayName: games.turnBasedMatches.dismiss
        description: Dismiss a turn-based match from the match list. The match will no longer show up in the list and will not generate notifications.
        responses:
          '200':
            description: Successful response
        queryParameters:
          consistencyToken:
            description: The last-seen mutation timestamp.
            type: string
        securedBy:
          - Oauth2:
              scopes:
                - 'https://www.googleapis.com/auth/plus.login'
        (oas-tags):
          - turnBasedMatches
      is:
        - alt
        - fields
        - key
        - oauthToken
        - prettyPrint
        - quotaUser
        - userIp
    /finish:
      displayName: finish
      put:
        displayName: games.turnBasedMatches.finish
        description: 'Finish a turn-based match. Each player should make this call once, after all results are in. Only the player whose turn it is may make the first call to Finish, and can pass in the final match state.'
        body:
          application/json:
            type: TurnBasedMatchResults
            (oas-body-name): body
        responses:
          '200':
            body:
              application/json:
                type: TurnBasedMatch
            description: Successful response
        queryParameters:
          consistencyToken:
            description: The last-seen mutation timestamp.
            type: string
          language:
            description: The preferred language to use for strings returned by this method.
            type: string
        securedBy:
          - Oauth2:
              scopes:
                - 'https://www.googleapis.com/auth/plus.login'
        (oas-tags):
          - turnBasedMatches
      is:
        - alt
        - fields
        - key
        - oauthToken
        - prettyPrint
        - quotaUser
        - userIp
    /rematch:
      displayName: rematch
      post:
        displayName: games.turnBasedMatches.rematch
        description: 'Create a rematch of a match that was previously completed, with the same participants. This can be called by only one player on a match still in their list; the player must have called Finish first. Returns the newly created match; it will be the caller''s turn.'
        responses:
          '200':
            body:
              application/json:
                type: TurnBasedMatchRematch
            description: Successful response
        queryParameters:
          consistencyToken:
            description: The last-seen mutation timestamp.
            type: string
          language:
            description: The preferred language to use for strings returned by this method.
            type: string
          requestId:
            description: A randomly generated numeric ID for each request specified by the caller. This number is used at the server to ensure that the request is handled correctly across retries.
            type: string
        securedBy:
          - Oauth2:
              scopes:
                - 'https://www.googleapis.com/auth/plus.login'
        (oas-tags):
          - turnBasedMatches
      is:
        - alt
        - fields
        - key
        - oauthToken
        - prettyPrint
        - quotaUser
        - userIp
    /leaveTurn:
      displayName: leaveTurn
      put:
        displayName: games.turnBasedMatches.leaveTurn
        description: 'Leave a turn-based match during the current player''s turn, without canceling the match.'
        responses:
          '200':
            body:
              application/json:
                type: TurnBasedMatch
            description: Successful response
        queryParameters:
          matchVersion:
            description: The version of the match being updated.
            required: true
            type: integer
          consistencyToken:
            description: The last-seen mutation timestamp.
            type: string
          language:
            description: The preferred language to use for strings returned by this method.
            type: string
          pendingParticipantId:
            description: 'The ID of another participant who should take their turn next. If not set, the match will wait for other player(s) to join via automatching; this is only valid if automatch criteria is set on the match with remaining slots for automatched players.'
            type: string
        securedBy:
          - Oauth2:
              scopes:
                - 'https://www.googleapis.com/auth/plus.login'
        (oas-tags):
          - turnBasedMatches
      is:
        - alt
        - fields
        - key
        - oauthToken
        - prettyPrint
        - quotaUser
        - userIp
    /leave:
      displayName: leave
      put:
        displayName: games.turnBasedMatches.leave
        description: 'Leave a turn-based match when it is not the current player''s turn, without canceling the match.'
        responses:
          '200':
            body:
              application/json:
                type: TurnBasedMatch
            description: Successful response
        queryParameters:
          consistencyToken:
            description: The last-seen mutation timestamp.
            type: string
          language:
            description: The preferred language to use for strings returned by this method.
            type: string
        securedBy:
          - Oauth2:
              scopes:
                - 'https://www.googleapis.com/auth/plus.login'
        (oas-tags):
          - turnBasedMatches
      is:
        - alt
        - fields
        - key
        - oauthToken
        - prettyPrint
        - quotaUser
        - userIp
    /join:
      displayName: join
      put:
        displayName: games.turnBasedMatches.join
        description: Join a turn-based match.
        responses:
          '200':
            body:
              application/json:
                type: TurnBasedMatch
            description: Successful response
        queryParameters:
          consistencyToken:
            description: The last-seen mutation timestamp.
            type: string
          language:
            description: The preferred language to use for strings returned by this method.
            type: string
        securedBy:
          - Oauth2:
              scopes:
                - 'https://www.googleapis.com/auth/plus.login'
        (oas-tags):
          - turnBasedMatches
      is:
        - alt
        - fields
        - key
        - oauthToken
        - prettyPrint
        - quotaUser
        - userIp
  get:
    displayName: games.turnBasedMatches.list
    description: Returns turn-based matches the player is or was involved in.
    responses:
      '200':
        body:
          application/json:
            type: TurnBasedMatchList
        description: Successful response
    queryParameters:
      consistencyToken:
        description: The last-seen mutation timestamp.
        type: string
      includeMatchData:
        description: True if match data should be returned in the response. Note that not all data will necessarily be returned if include_match_data is true; the server may decide to only return data for some of the matches to limit download size for the client. The remainder of the data for these matches will be retrievable on request.
        type: boolean
      language:
        description: The preferred language to use for strings returned by this method.
        type: string
      maxCompletedMatches:
        description: 'The maximum number of completed or canceled matches to return in the response. If not set, all matches returned could be completed or canceled.'
        maximum: 500
        minimum: 0
        type: integer
      maxResults:
        description: 'The maximum number of matches to return in the response, used for paging. For any response, the actual number of matches to return may be less than the specified maxResults.'
        maximum: 500
        minimum: 1
        type: integer
      pageToken:
        description: The token returned by the previous request.
        type: string
    securedBy:
      - Oauth2:
          scopes:
            - 'https://www.googleapis.com/auth/plus.login'
    (oas-tags):
      - turnBasedMatches
  is:
    - alt
    - fields
    - key
    - oauthToken
    - prettyPrint
    - quotaUser
    - userIp
  /create:
    displayName: create
    post:
      displayName: games.turnBasedMatches.create
      description: Create a turn-based match.
      body:
        application/json:
          type: TurnBasedMatchCreateRequest
          (oas-body-name): body
      responses:
        '200':
          body:
            application/json:
              type: TurnBasedMatch
          description: Successful response
      queryParameters:
        consistencyToken:
          description: The last-seen mutation timestamp.
          type: string
        language:
          description: The preferred language to use for strings returned by this method.
          type: string
      securedBy:
        - Oauth2:
            scopes:
              - 'https://www.googleapis.com/auth/plus.login'
      (oas-tags):
        - turnBasedMatches
    is:
      - alt
      - fields
      - key
      - oauthToken
      - prettyPrint
      - quotaUser
      - userIp
  /sync:
    displayName: sync
    get:
      displayName: games.turnBasedMatches.sync
      description: 'Returns turn-based matches the player is or was involved in that changed since the last sync call, with the least recent changes coming first. Matches that should be removed from the local cache will have a status of MATCH_DELETED.'
      responses:
        '200':
          body:
            application/json:
              type: TurnBasedMatchSync
          description: Successful response
      queryParameters:
        consistencyToken:
          description: The last-seen mutation timestamp.
          type: string
        includeMatchData:
          description: True if match data should be returned in the response. Note that not all data will necessarily be returned if include_match_data is true; the server may decide to only return data for some of the matches to limit download size for the client. The remainder of the data for these matches will be retrievable on request.
          type: boolean
        language:
          description: The preferred language to use for strings returned by this method.
          type: string
        maxCompletedMatches:
          description: 'The maximum number of completed or canceled matches to return in the response. If not set, all matches returned could be completed or canceled.'
          maximum: 500
          minimum: 0
          type: integer
        maxResults:
          description: 'The maximum number of matches to return in the response, used for paging. For any response, the actual number of matches to return may be less than the specified maxResults.'
          maximum: 500
          minimum: 1
          type: integer
        pageToken:
          description: The token returned by the previous request.
          type: string
      securedBy:
        - Oauth2:
            scopes:
              - 'https://www.googleapis.com/auth/plus.login'
      (oas-tags):
        - turnBasedMatches
    is:
      - alt
      - fields
      - key
      - oauthToken
      - prettyPrint
      - quotaUser
      - userIp
/achievements:
  displayName: achievements
  get:
    displayName: games.achievementDefinitions.list
    description: Lists all the achievement definitions for your application.
    responses:
      '200':
        body:
          application/json:
            type: AchievementDefinitionsListResponse
        description: Successful response
    queryParameters:
      consistencyToken:
        description: The last-seen mutation timestamp.
        type: string
      language:
        description: The preferred language to use for strings returned by this method.
        type: string
      maxResults:
        description: 'The maximum number of achievement resources to return in the response, used for paging. For any response, the actual number of achievement resources returned may be less than the specified maxResults.'
        maximum: 200
        minimum: 1
        type: integer
      pageToken:
        description: The token returned by the previous request.
        type: string
    securedBy:
      - Oauth2:
          scopes:
            - 'https://www.googleapis.com/auth/plus.login'
    (oas-tags):
      - achievementDefinitions
  is:
    - alt
    - fields
    - key
    - oauthToken
    - prettyPrint
    - quotaUser
    - userIp
  '/{achievementId}':
    displayName: '{achievementId}'
    /increment:
      displayName: increment
      post:
        displayName: games.achievements.increment
        description: Increments the steps of the achievement with the given ID for the currently authenticated player.
        responses:
          '200':
            body:
              application/json:
                type: AchievementIncrementResponse
            description: Successful response
        queryParameters:
          stepsToIncrement:
            description: The number of steps to increment.
            minimum: 1
            required: true
            type: integer
          consistencyToken:
            description: The last-seen mutation timestamp.
            type: string
          requestId:
            description: A randomly generated numeric ID for each request specified by the caller. This number is used at the server to ensure that the request is handled correctly across retries.
            type: string
        securedBy:
          - Oauth2:
              scopes:
                - 'https://www.googleapis.com/auth/plus.login'
        (oas-tags):
          - achievements
      is:
        - alt
        - fields
        - key
        - oauthToken
        - prettyPrint
        - quotaUser
        - userIp
    /setStepsAtLeast:
      displayName: setStepsAtLeast
      post:
        displayName: games.achievements.setStepsAtLeast
        description: 'Sets the steps for the currently authenticated player towards unlocking an achievement. If the steps parameter is less than the current number of steps that the player already gained for the achievement, the achievement is not modified.'
        responses:
          '200':
            body:
              application/json:
                type: AchievementSetStepsAtLeastResponse
            description: Successful response
        queryParameters:
          steps:
            description: The minimum value to set the steps to.
            minimum: 1
            required: true
            type: integer
          consistencyToken:
            description: The last-seen mutation timestamp.
            type: string
        securedBy:
          - Oauth2:
              scopes:
                - 'https://www.googleapis.com/auth/plus.login'
        (oas-tags):
          - achievements
      is:
        - alt
        - fields
        - key
        - oauthToken
        - prettyPrint
        - quotaUser
        - userIp
    /unlock:
      displayName: unlock
      post:
        displayName: games.achievements.unlock
        description: Unlocks this achievement for the currently authenticated player.
        responses:
          '200':
            body:
              application/json:
                type: AchievementUnlockResponse
            description: Successful response
        queryParameters:
          consistencyToken:
            description: The last-seen mutation timestamp.
            type: string
        securedBy:
          - Oauth2:
              scopes:
                - 'https://www.googleapis.com/auth/plus.login'
        (oas-tags):
          - achievements
      is:
        - alt
        - fields
        - key
        - oauthToken
        - prettyPrint
        - quotaUser
        - userIp
    /reveal:
      displayName: reveal
      post:
        displayName: games.achievements.reveal
        description: Sets the state of the achievement with the given ID to REVEALED for the currently authenticated player.
        responses:
          '200':
            body:
              application/json:
                type: AchievementRevealResponse
            description: Successful response
        queryParameters:
          consistencyToken:
            description: The last-seen mutation timestamp.
            type: string
        securedBy:
          - Oauth2:
              scopes:
                - 'https://www.googleapis.com/auth/plus.login'
        (oas-tags):
          - achievements
      is:
        - alt
        - fields
        - key
        - oauthToken
        - prettyPrint
        - quotaUser
        - userIp
  /updateMultiple:
    displayName: updateMultiple
    post:
      displayName: games.achievements.updateMultiple
      description: Updates multiple achievements for the currently authenticated player.
      body:
        application/json:
          type: AchievementUpdateMultipleRequest
          (oas-body-name): body
      responses:
        '200':
          body:
            application/json:
              type: AchievementUpdateMultipleResponse
          description: Successful response
      queryParameters:
        consistencyToken:
          description: The last-seen mutation timestamp.
          type: string
      securedBy:
        - Oauth2:
            scopes:
              - 'https://www.googleapis.com/auth/plus.login'
      (oas-tags):
        - achievements
    is:
      - alt
      - fields
      - key
      - oauthToken
      - prettyPrint
      - quotaUser
      - userIp
/quests:
  displayName: quests
  '/{questId}':
    displayName: '{questId}'
    /accept:
      displayName: accept
      post:
        displayName: games.quests.accept
        description: Indicates that the currently authorized user will participate in the quest.
        responses:
          '200':
            body:
              application/json:
                type: Quest
            description: Successful response
        queryParameters:
          consistencyToken:
            description: The last-seen mutation timestamp.
            type: string
          language:
            description: The preferred language to use for strings returned by this method.
            type: string
        securedBy:
          - Oauth2:
              scopes:
                - 'https://www.googleapis.com/auth/plus.login'
        (oas-tags):
          - quests
      is:
        - alt
        - fields
        - key
        - oauthToken
        - prettyPrint
        - quotaUser
        - userIp
    /milestones:
      displayName: milestones
      '/{milestoneId}':
        displayName: '{milestoneId}'
        /claim:
          displayName: claim
          put:
            displayName: games.questMilestones.claim
            description: Report that a reward for the milestone corresponding to milestoneId for the quest corresponding to questId has been claimed by the currently authorized user.
            responses:
              '200':
                description: Successful response
            queryParameters:
              requestId:
                description: A numeric ID to ensure that the request is handled correctly across retries. Your client application must generate this ID randomly.
                required: true
                type: string
              consistencyToken:
                description: The last-seen mutation timestamp.
                type: string
            securedBy:
              - Oauth2:
                  scopes:
                    - 'https://www.googleapis.com/auth/plus.login'
            (oas-tags):
              - questMilestones
          is:
            - alt
            - fields
            - key
            - oauthToken
            - prettyPrint
            - quotaUser
            - userIp
/revisions:
  displayName: revisions
  /check:
    displayName: check
    get:
      displayName: games.revisions.check
      description: Checks whether the games client is out of date.
      responses:
        '200':
          body:
            application/json:
              type: RevisionCheckResponse
          description: Successful response
      queryParameters:
        clientRevision:
          description: |-
            The revision of the client SDK used by your application. Format:
            [PLATFORM_TYPE]:[VERSION_NUMBER]. Possible values of PLATFORM_TYPE are:
             
            - "ANDROID" - Client is running the Android SDK. 
            - "IOS" - Client is running the iOS SDK. 
            - "WEB_APP" - Client is running as a Web App.
          required: true
          type: string
        consistencyToken:
          description: The last-seen mutation timestamp.
          type: string
      securedBy:
        - Oauth2:
            scopes:
              - 'https://www.googleapis.com/auth/plus.login'
      (oas-tags):
        - revisions
    is:
      - alt
      - fields
      - key
      - oauthToken
      - prettyPrint
      - quotaUser
      - userIp
/applications:
  displayName: applications
  /played:
    displayName: played
    post:
      displayName: games.applications.played
      description: Indicate that the the currently authenticated user is playing your application.
      responses:
        '200':
          description: Successful response
