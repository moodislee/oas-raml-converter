#%RAML 1.0
title: Resource Views
version: v1beta1
baseUri: 'https://www.googleapis.com/resourceviews/v1beta1/projects'
protocols:
  - HTTPS
description: The Resource View API allows users to create and manage logical sets of Google Compute Engine instances.
(oas-tags-definition):
  - name: regionViews
  - name: zoneViews
annotationTypes:
  oas-tags-definition:
    type: array
    items:
      properties:
        name: string
        description?: string
        externalDocs?:
          properties:
            url: string
            description?: string
    allowedTargets: API
  oas-x-apiClientRegistration: any
  oas-x-logo: any
  oas-x-origin: any
  oas-x-preferred: any
  oas-x-providerName: any
  oas-x-serviceName: any
  oas-tags:
    type: 'string[]'
    allowedTargets: Method
  oas-externalDocs:
    properties:
      description?: string
      url: string
    allowedTargets:
      - API
      - Method
      - TypeDeclaration
  oas-info:
    properties:
      termsOfService?: string
      contact?:
        properties:
          name?: string
          url?: string
          email?: string
      license?:
        properties:
          name?: string
          url?: string
    allowedTargets: API
  oas-body-name:
    type: string
    allowedTargets: TypeDeclaration
(oas-info):
  (oas-x-apiClientRegistration):
    url: 'https://console.developers.google.com'
  (oas-x-logo):
    url: 'https://api.apis.guru/v2/cache/logo/https_www.google.com_images_branding_googlelogo_2x_googlelogo_color_272x92dp.png'
  (oas-x-origin):
    format: google
    url: 'https://www.googleapis.com/discovery/v1/apis/resourceviews/v1beta1/rest'
    version: v1
  (oas-x-preferred): false
  (oas-x-providerName): googleapis.com
  (oas-x-serviceName): resourceviews
  contact:
    name: Google
    url: 'https://google.com'
(oas-externalDocs):
  url: 'https://developers.google.com/compute/'
securitySchemes:
  Oauth2:
    type: OAuth 2.0
    settings:
      authorizationUri: 'https://accounts.google.com/o/oauth2/auth'
      accessTokenUri: ''
      authorizationGrants:
        - implicit
      scopes:
        - 'https://www.googleapis.com/auth/cloud-platform'
        - 'https://www.googleapis.com/auth/cloud-platform.read-only'
        - 'https://www.googleapis.com/auth/compute'
        - 'https://www.googleapis.com/auth/compute.readonly'
        - 'https://www.googleapis.com/auth/ndev.cloudman'
        - 'https://www.googleapis.com/auth/ndev.cloudman.readonly'
    description: Oauth 2.0 authentication
'/{projectName}':
  displayName: '{projectName}'
  /zones:
    displayName: zones
    '/{zone}':
      displayName: '{zone}'
      /resourceViews:
        displayName: resourceViews
        get:
          displayName: resourceviews.zoneViews.list
          description: List resource views.
          responses:
            '200':
              body:
                application/json:
                  type: ZoneViewsListResponse
              description: Successful response
          queryParameters:
            maxResults:
              default: 5000
              description: 'Maximum count of results to be returned. Acceptable values are 0 to 5000, inclusive. (Default: 5000)'
              maximum: 5000
              minimum: 0
              type: integer
            pageToken:
              description: Specifies a nextPageToken returned by a previous list request. This token can be used to request the next page of results from a previous list request.
              type: string
          securedBy:
            - Oauth2:
                scopes:
                  - 'https://www.googleapis.com/auth/ndev.cloudman.readonly'
          (oas-tags):
            - zoneViews
        is:
          - alt
          - fields
          - key
          - oauthToken
          - prettyPrint
          - quotaUser
          - userIp
        '/{resourceViewName}':
          displayName: '{resourceViewName}'
          /resources:
            displayName: resources
            post:
              displayName: resourceviews.zoneViews.listresources
              description: List the resources of the resource view.
              responses:
                '200':
                  body:
                    application/json:
                      type: ZoneViewsListResourcesResponse
                  description: Successful response
              queryParameters:
                maxResults:
                  default: 5000
                  description: 'Maximum count of results to be returned. Acceptable values are 0 to 5000, inclusive. (Default: 5000)'
                  maximum: 5000
                  minimum: 0
                  type: integer
                pageToken:
                  description: Specifies a nextPageToken returned by a previous list request. This token can be used to request the next page of results from a previous list request.
                  type: string
              securedBy:
                - Oauth2:
                    scopes:
                      - 'https://www.googleapis.com/auth/ndev.cloudman.readonly'
              (oas-tags):
                - zoneViews
            is:
              - alt
              - fields
              - key
              - oauthToken
              - prettyPrint
              - quotaUser
              - userIp
          /removeResources:
            displayName: removeResources
            post:
              displayName: resourceviews.zoneViews.removeresources
              description: Remove resources from the view.
              body:
                application/json:
                  type: ZoneViewsRemoveResourcesRequest
                  (oas-body-name): body
              responses:
                '200':
                  description: Successful response
              securedBy:
                - Oauth2:
                    scopes:
                      - 'https://www.googleapis.com/auth/ndev.cloudman'
              (oas-tags):
                - zoneViews
            is:
              - alt
              - fields
              - key
              - oauthToken
              - prettyPrint
              - quotaUser
              - userIp
          /addResources:
            displayName: addResources
            post:
              displayName: resourceviews.zoneViews.addresources
              description: Add resources to the view.
              body:
                application/json:
                  type: ZoneViewsAddResourcesRequest
                  (oas-body-name): body
              responses:
                '200':
                  description: Successful response
              securedBy:
                - Oauth2:
                    scopes:
                      - 'https://www.googleapis.com/auth/ndev.cloudman'
              (oas-tags):
                - zoneViews
            is:
              - alt
              - fields
              - key
              - oauthToken
              - prettyPrint
              - quotaUser
              - userIp
          uriParameters:
            resourceViewName:
              description: The name of the resource view.
              required: true
              type: string
          get:
            displayName: resourceviews.zoneViews.get
            description: Get the information of a zonal resource view.
            responses:
              '200':
                body:
                  application/json:
                    type: ResourceView
                description: Successful response
            securedBy:
              - Oauth2:
                  scopes:
                    - 'https://www.googleapis.com/auth/ndev.cloudman.readonly'
            (oas-tags):
              - zoneViews
          is:
            - alt
            - fields
            - key
            - oauthToken
            - prettyPrint
            - quotaUser
            - userIp
          delete:
            displayName: resourceviews.zoneViews.delete
            description: Delete a resource view.
            responses:
              '200':
                description: Successful response
            securedBy:
              - Oauth2:
                  scopes:
                    - 'https://www.googleapis.com/auth/ndev.cloudman'
            (oas-tags):
              - zoneViews
        post:
          displayName: resourceviews.zoneViews.insert
          description: Create a resource view.
          body:
            application/json:
              type: ResourceView
              (oas-body-name): body
          responses:
            '200':
              body:
                application/json:
                  type: ZoneViewsInsertResponse
              description: Successful response
          securedBy:
            - Oauth2:
                scopes:
                  - 'https://www.googleapis.com/auth/ndev.cloudman'
          (oas-tags):
            - zoneViews
  /regions:
    displayName: regions
    '/{region}':
      displayName: '{region}'
      /resourceViews:
        displayName: resourceViews
        get:
          displayName: resourceviews.regionViews.list
          description: List resource views.
          responses:
            '200':
              body:
                application/json:
                  type: RegionViewsListResponse
              description: Successful response
          queryParameters:
            maxResults:
              default: 5000
              description: 'Maximum count of results to be returned. Acceptable values are 0 to 5000, inclusive. (Default: 5000)'
              maximum: 5000
              minimum: 0
              type: integer
            pageToken:
              description: Specifies a nextPageToken returned by a previous list request. This token can be used to request the next page of results from a previous list request.
              type: string
          securedBy:
            - Oauth2:
                scopes:
