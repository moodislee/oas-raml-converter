            (oas-tags):
              - regionViews
          /resources:
            displayName: resources
            post:
              displayName: resourceviews.regionViews.listresources
              description: List the resources in the view.
              responses:
                '200':
                  body:
                    application/json:
                      type: RegionViewsListResourcesResponse
                  description: Successful response
              queryParameters:
