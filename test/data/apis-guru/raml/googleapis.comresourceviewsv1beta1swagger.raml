            - oauthToken
            - prettyPrint
            - quotaUser
            - userIp
          get:
            displayName: resourceviews.regionViews.get
            description: Get the information of a resource view.
            responses:
              '200':
                body:
                  application/json:
                    type: ResourceView
                description: Successful response
            securedBy:
              - Oauth2:
                  scopes:
