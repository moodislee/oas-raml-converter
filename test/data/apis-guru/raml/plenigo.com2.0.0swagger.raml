#%RAML 1.0
title: Plenigo
version: 2.0.0
baseUri: 'https://api.plenigo.com/api/v2'
protocols:
  - HTTPS
description: This is the API Documentation for the public plenigo server interfaces
(oas-info):
  (oas-x-logo):
    url: 'https://api.apis.guru/v2/cache/logo/https_static.plenigo.com_assets_logo.png'
  (oas-x-origin):
    format: swagger
    url: 'https://api.plenigo.com/swagger.json'
    version: '2.0'
  (oas-x-preferred): true
  (oas-x-providerName): plenigo.com
annotationTypes:
  oas-x-logo: any
  oas-x-origin: any
  oas-x-preferred: any
  oas-x-providerName: any
  oas-tags:
    type: 'string[]'
    allowedTargets: Method
  oas-summary:
    type: string
    allowedTargets: Method
  oas-externalDocs:
    properties:
      description?: string
      url: string
    allowedTargets:
      - API
      - Method
      - TypeDeclaration
  oas-info:
    properties:
      termsOfService?: string
      contact?:
        properties:
          name?: string
          url?: string
          email?: string
      license?:
        properties:
          name?: string
          url?: string
    allowedTargets: API
  oas-body-name:
    type: string
    allowedTargets: TypeDeclaration
(oas-externalDocs):
  url: 'https://developer.plenigo.com/'
/company:
  displayName: company
  /users:
    displayName: users
    /select:
      displayName: select
      get:
        displayName: getCompanyUsersFromUserIds
        description: Returns a list of users of the specified company
        (oas-summary): Query company users by user ids
        headers:
          plenigoToken:
            type: string
            description: Plenigo authentication token
            enum:
              - JWT
            required: true
        responses:
          '200':
            body:
              application/json:
                items:
                  type: CompanyUserItem
                type: array
            description: Company Users queried
          '400':
            description: If the given parameters were incorrect
          '401':
            description: If the provided company secret and/or company Id is incorrect
          '500':
            description: Server error
        queryParameters:
          userIds:
            description: 'List of ids, a comma separated value list expected e.g. 1,2,3,5,10,11'
            required: true
            type: string
        (oas-tags):
          - company
    /failed:
      displayName: failed
      /payments:
        displayName: payments
        get:
          displayName: getCompanyFailedPayments
          description: Returns a list of failed payments of the specified company
          (oas-summary): Query failed payments
          headers:
            plenigoToken:
              type: string
              description: Plenigo authentication token
              enum:
                - JWT
              required: true
          responses:
            '200':
              body:
                application/json:
                  type: FailedPaymentList
              description: Failed Payments queried
            '400':
              description: If the given parameters were incorrect
            '401':
              description: If the provided company secret and/or company Id is incorrect
            '500':
              description: Server error
          queryParameters:
            failedPaymentStatus:
              description: Payment status where the failed payment is.
              enum:
                - FAILED
                - FIXED
                - FIXED_MANUALLY
              required: false
              type: string
            size:
              default: 10
              description: Size of the page - must be between 10 and 100
              format: int32
              maximum: 100
              minimum: 10
              required: false
              type: integer
            page:
              default: 0
              description: Number of the page
              format: int32
              minimum: 0
              required: false
              type: integer
          (oas-tags):
            - company
    get:
      displayName: getCompanyUsers
      description: Returns a list of users of the specified company
      (oas-summary): Query company users
      headers:
        plenigoToken:
          type: string
          description: Plenigo authentication token
          enum:
            - JWT
          required: true
      responses:
        '200':
          body:
            application/json:
              type: CompanyUserList
          description: Company Users queried
        '400':
          description: If the given parameters were incorrect
        '401':
          description: If the provided company secret and/or company Id is incorrect
        '500':
          description: Server error
      queryParameters:
        size:
          default: 10
          description: Size of the page - must be between 10 and 100
          format: int32
          maximum: 100
          minimum: 10
          required: false
          type: integer
        page:
          default: 0
          description: Number of the page
          format: int32
          minimum: 0
          required: false
          type: integer
      (oas-tags):
        - company
/user:
  displayName: user
  '/{customerId}':
    displayName: '{customerId}'
    /products:
      displayName: products
      get:
        displayName: getProductsBought
        description: Returns the products the user has bought only including active subscriptions
        (oas-summary): Get products a user has bought
        headers:
          plenigoToken:
            type: string
            description: Plenigo authentication token
            enum:
              - JWT
            required: true
        responses:
          '200':
            body:
              application/json:
                type: CustomerProductsList
            description: Correct parameters
          '400':
            description: If the given parameters were incorrect
          '401':
            description: If the provided company secret and/or company Id is incorrect
          '500':
            description: Server error
        queryParameters:
          testMode:
            description: Flag indicating if products from test mode should be returned
            required: true
            type: boolean
        (oas-tags):
          - user
  /profile:
    displayName: profile
    get:
      displayName: getUserProfile
      description: Returns the user profile information that belongs to the token
      (oas-summary): Get user profile information
      headers:
        plenigoToken:
          type: string
          description: Plenigo authentication token
          enum:
            - JWT
          required: true
      responses:
        '200':
          body:
            application/json:
              type: User
          description: Correct parameters
        '400':
          body:
            application/json:
              type: OAuthError
          description: If the given parameters were incorrect
        '500':
          description: Server error
      queryParameters:
        token:
          description: OAuth2 access token
          required: true
          type: string
      (oas-tags):
        - user
  /product:
    displayName: product
    get:
      displayName: hasBoughtProduct
      (oas-summary): Find out if a product is accessible for a specified customer
      headers:
        plenigoToken:
          type: string
          description: Plenigo authentication token
          enum:
            - JWT
          required: true
      responses:
        '200':
          description: Access is allowed
        '400':
          description: If the given parameters were incorrect
        '401':
          description: If the provided company secret and/or company Id is incorrect
        '403':
          description: Customer is not allowed to access the product
      queryParameters:
        customerId:
          description: Id of the customer to test if product is currently paid
          required: true
          type: string
        productId:
          description: Id of the product that wants to be queried
          required: true
          type: string
        testMode:
          description: Flag indicating if this product was bought in test mode
          required: true
          type: boolean
      (oas-tags):
        - user
/externalUser:
  displayName: externalUser
  /register:
    displayName: register
    post:
      displayName: registerExternalUser
      description: Registers a new user bound to the company that registers the user. This functionality is only available for companies with closed user groups.
      (oas-summary): Registers a new user
      body:
        application/json:
          type: ExternalUserRegistration
          (oas-body-name): body
          description: User registration data
      headers:
        plenigoToken:
          type: string
          description: Plenigo authentication token
          enum:
            - JWT
          required: true
      responses:
        '201':
          body:
            application/json:
              type: CustomerId
          description: Id of the customer created
        '400':
          description: If the given parameters were incorrect
        '401':
          description: If the provided company secret and/or company Id is incorrect
        '403':
          description: If company is not qualified for a closed user group
        '500':
          description: Server error
      (oas-tags):
        - external user management
  '/{customerId}':
    displayName: '{customerId}'
    /changeEmail:
      displayName: changeEmail
      put:
        displayName: changeExternalUserEmail
        description: Change email address of an existing user. This functionality is only available for companies with closed user groups.
        (oas-summary): Change email address of a user
        body:
          application/json:
            type: EmailData
            (oas-body-name): body
            description: New User email data
        headers:
          plenigoToken:
            type: string
            description: Plenigo authentication token
            enum:
              - JWT
            required: true
        responses:
          '204':
            description: Email address changed
          '400':
            description: If the given parameters were incorrect or email address already exists
          '401':
            description: If the provided company secret and/or company Id is incorrect
          '403':
            description: If company is not qualified for a closed user group
          '404':
            description: If a user for the provided customer id does not exist
          '500':
            description: Server error
        (oas-tags):
          - external user management
    /createLoginToken:
      displayName: createLoginToken
      post:
        displayName: createLoginToken
        description: Create a login token for an existing user. This functionality is only available for companies with closed user groups.
        (oas-summary): Create a login token for a user
        headers:
          plenigoToken:
            type: string
            description: Plenigo authentication token
            enum:
              - JWT
            required: true
        responses:
          '200':
            body:
              application/json:
                type: ExternalLoginToken
            description: Login token successful created
          '400':
            description: If the given parameters were incorrect
          '401':
            description: If the provided company secret and/or company Id is incorrect
          '403':
            description: If company is not qualified for a closed user group
          '404':
            description: If a user for the provided customer id does not exist
          '500':
            description: Server error
        (oas-tags):
          - external user management
/access:
  displayName: access
  /app:
    displayName: app
    /token:
      displayName: token
      '/{customerId}':
        displayName: '{customerId}'
        uriParameters:
          customerId:
            description: Unique Id of the customer to get products for
            required: true
            type: string
        post:
          displayName: requestAppAccessToken
          description: Returns the app access token and the customer id necessary for requesting an customer app id
          (oas-summary): Request an app access token
          body:
            application/json:
              type: AppAccessTokenRequest
              (oas-body-name): body
              description: App access token request data
          headers:
            plenigoToken:
              type: string
              description: Plenigo authentication token
              enum:
                - JWT
              required: true
          responses:
            '200':
              body:
                application/json:
                  type: AppAccessToken
              description: Correct parameters
            '400':
              description: If the given parameters were incorrect
            '401':
              description: If the provided company secret and/or company Id is incorrect
            '412':
              description: If product is not owned by the user
            '423':
              description: If the user has reached the limit of parallel app accesses
            '500':
              description: Server error
          (oas-tags):
            - app management
    '/{customerId}':
      displayName: '{customerId}'
      uriParameters:
        customerId:
          description: Unique Id of the customer to get products for
          required: true
          type: string
      post:
        displayName: requestCustomerAppId
        description: Returns the customer app id
        (oas-summary): Request a customer app id
        body:
          application/json:
            type: AppAccessRequest
            (oas-body-name): body
            description: App access request data
        headers:
          plenigoToken:
            type: string
            description: Plenigo authentication token
            enum:
              - JWT
            required: true
        responses:
          '200':
            body:
              application/json:
                type: AppAccessData
            description: App access data
          '400':
            description: If the given parameters were incorrect
          '401':
            description: If the provided company secret and/or company Id is incorrect
          '403':
            description: If access token is not valid
          '423':
            description: If the user has reached the limit of parallel app accesses
          '500':
            description: Server error
        (oas-tags):
          - app management
      '/{productId}':
        displayName: '{productId}'
        '/{customerAppId}':
          displayName: '{customerAppId}'
          uriParameters:
            customerAppId:
              description: Customer app id that shall grant access
              required: true
              type: string
          get:
            displayName: verifyCustomerAppAccess
            description: Verify if customer app is having access to a specific product
            (oas-summary): Verify customer app access
            headers:
              plenigoToken:
                type: string
                description: Plenigo authentication token
                enum:
                  - JWT
                required: true
            responses:
              '204':
                description: Access is granted
              '400':
                description: If the given parameters were incorrect
              '401':
                description: If the provided company secret and/or company Id is incorrect
              '403':
                description: Access is not allowed
              '500':
                description: Server error
            queryParameters:
              testMode:
                description: Flag indicating if test mode should be used
                required: true
                type: boolean
            (oas-tags):
              - app management
      '/{customerAppId}':
        displayName: '{customerAppId}'
        uriParameters:
          customerAppId:
            description: Customer app id that shall grant access
            required: true
            type: string
        delete:
          displayName: deleteCustomerApp
          description: Deletes a customer app id to allow access with another app
          (oas-summary): Deletes a customer app id
          headers:
            plenigoToken:
              type: string
              description: Plenigo authentication token
              enum:
                - JWT
              required: true
          responses:
            '204':
              description: App id successfully deleted
            '400':
              description: If the given parameters were incorrect
            '401':
              description: If the provided company secret and/or company Id is incorrect
            '500':
              description: Server error
          queryParameters:
            testMode:
              description: Flag indicating if test mode should be used
              required: true
              type: boolean
          (oas-tags):
            - app management
      get:
        displayName: getCustomerApps
        description: Returns all customer apps
        (oas-summary): Get all customer apps
        headers:
          plenigoToken:
            type: string
            description: Plenigo authentication token
            enum:
              - JWT
            required: true
        responses:
          '200':
            body:
              application/json:
                type: AppAccessList
            description: List with all customer apps
          '400':
            description: If the given parameters were incorrect
          '401':
            description: If the provided company secret and/or company Id is incorrect
          '500':
            description: Server error
        queryParameters:
          testMode:
            description: Flag indicating if test mode should be used
            required: true
            type: boolean
        (oas-tags):
          - app management
  /mobileSecret:
    displayName: mobileSecret
    '/{customerId}':
      displayName: '{customerId}'
      uriParameters:
        customerId:
          description: Unique Id of the customer to get mobile secret for
          required: true
          type: string
      delete:
        displayName: deleteMobileSecret
        description: Deletes a mobile secret from the customer
        (oas-summary): Deletes the mobile secret for a customer
        headers:
          plenigoToken:
            type: string
            description: Plenigo authentication token
            enum:
              - JWT
            required: true
        responses:
          '204':
            description: Mobile secret successfully deleted
          '400':
            description: If the given parameters were incorrect
          '401':
            description: If the provided company secret and/or company Id is incorrect
          '500':
            description: Server error
        (oas-tags):
          - mobile secret
      post:
        displayName: createMobileSecret
        description: Returns the mobile secret
        (oas-summary): Create or recreate mobile secret
        body:
          application/json:
            type: CreateMobileSecretRequest
            (oas-body-name): body
            description: App access request data
        headers:
          plenigoToken:
            type: string
            description: Plenigo authentication token
            enum:
              - JWT
            required: true
        responses:
          '200':
            body:
              application/json:
                type: MobileSecretData
            description: Mobile secret
          '400':
            description: If the given parameters were incorrect
          '401':
            description: If the provided company secret and/or company Id is incorrect
          '500':
            description: Server error
        (oas-tags):
          - mobile secret
      get:
        displayName: getMobileSecret
        description: Returns customer mobile secret
        (oas-summary): Get customer mobile secret
        headers:
          plenigoToken:
            type: string
            description: Plenigo authentication token
            enum:
              - JWT
            required: true
        responses:
          '200':
            body:
              application/json:
                type: MobileSecretData
            description: Mobile secret
          '400':
            description: If the given parameters were incorrect
          '401':
            description: If the provided company secret and/or company Id is incorrect
          '404':
            description: No mobile secret created yet
          '500':
            description: Server error
        (oas-tags):
          - mobile secret
    /verify:
      displayName: verify
      get:
        displayName: verifyMobileSecret
        (oas-summary): Test if user mobile secret is correct.
        headers:
          plenigoToken:
            type: string
            description: Plenigo authentication token
            enum:
              - JWT
            required: true
        responses:
          '200':
            body:
              application/json:
                type: MobileSecretVerificationResult
            description: Mobile secret is correct
          '400':
            description: If the given parameters were incorrect
          '401':
            description: If the provided company secret and/or company Id is incorrect
          '403':
            description: Mobile secret is not correct
          '500':
            description: Server error
        queryParameters:
          email:
            description: Email of the user
            required: true
            type: string
          mobileSecret:
            description: Mobile secret entered by user
            required: true
            type: string
        (oas-tags):
          - mobile secret
/import:
