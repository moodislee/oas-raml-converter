          required: false
          type: boolean
        size:
          default: 10
          description: Size of the page - must be between 10 and 100
          format: int32
          maximum: 100
          minimum: 10
          required: false
          type: integer
        page:
          default: 0
          description: Number of the page
          format: int32
          minimum: 0
          required: false
