                  displayName: Profiles_CreateOrUpdate
                  description: Create or update a Traffic Manager profile.
                  is:
                    - apiVersionParameter
                  body:
                    application/json:
                      type: Profile
                      (oas-body-name): parameters
                      description: The Traffic Manager profile parameters supplied to the CreateOrUpdate operation.
                  responses:
                    '200':
                      body:
                        application/json:
                          type: Profile
                      description: The created or updated Traffic Manager profile.
                    '201':
                      body:
                        application/json:
                          type: Profile
                      description: The created or updated Traffic Manager profile.
                  (oas-tags):
                    - Profiles
                get:
                  displayName: Profiles_Get
                  description: Gets a Traffic Manager profile.
                  is:
                    - apiVersionParameter
                  responses:
                    '200':
                      body:
                        application/json:
                          type: Profile
                      description: The Traffic Manager profile.
                  (oas-tags):
                    - Profiles
                delete:
                  displayName: Profiles_Delete
                  description: Deletes a Traffic Manager profile.
                  is:
                    - apiVersionParameter
                  responses:
                    '200': {}
                    '204': {}
                  (oas-tags):
                    - Profiles
              get:
                (oas-x-ms-pageable): {}
                displayName: Profiles_ListAllInResourceGroup
                description: Lists all Traffic Manager profiles within a resource group.
                is:
