/subscriptions:
  displayName: subscriptions
  '/{subscriptionId}':
    displayName: '{subscriptionId}'
    /resourceGroups:
      displayName: resourceGroups
      '/{resourceGroupName}':
        displayName: '{resourceGroupName}'
        /providers:
          displayName: providers
          /Microsoft.Network:
            displayName: Microsoft.Network
            /trafficmanagerprofiles:
              displayName: trafficmanagerprofiles
              '/{profileName}':
                displayName: '{profileName}'
                uriParameters:
                  profileName:
                    description: The name of the Traffic Manager profile to be deleted.
                    required: true
                    type: string
                patch:
                  displayName: Profiles_Update
                  description: Update a Traffic Manager profile.
                  is:
                    - apiVersionParameter
                  body:
                    application/json:
                      type: Profile
                      (oas-body-name): parameters
                      description: The Traffic Manager profile parameters supplied to the Update operation.
                  responses:
                    '200':
                      body:
                        application/json:
                          type: Profile
                      description: The updated Traffic Manager profile.
                  (oas-tags):
                    - Profiles
                '/{endpointType}':
                  displayName: '{endpointType}'
                  '/{endpointName}':
                    displayName: '{endpointName}'
                    uriParameters:
                      endpointName:
                        description: The name of the Traffic Manager endpoint to be deleted.
                        required: true
                        type: string
                    put:
                      displayName: Endpoints_CreateOrUpdate
                      description: Create or update a Traffic Manager endpoint.
                      is:
                        - apiVersionParameter
                      body:
                        application/json:
                          type: Endpoint
                          (oas-body-name): parameters
                          description: The Traffic Manager endpoint parameters supplied to the CreateOrUpdate operation.
                      responses:
                        '200':
                          body:
                            application/json:
                              type: Endpoint
                          description: The created or updated Endpoint.
                        '201':
                          body:
                            application/json:
                              type: Endpoint
                          description: The created or updated Endpoint.
                      (oas-tags):
                        - Endpoints
                    patch:
                      displayName: Endpoints_Update
                      description: Update a Traffic Manager endpoint.
                      is:
                        - apiVersionParameter
                      body:
                        application/json:
                          type: Endpoint
                          (oas-body-name): parameters
                          description: The Traffic Manager endpoint parameters supplied to the Update operation.
                      responses:
                        '200':
                          body:
                            application/json:
                              type: Endpoint
                          description: The updated Traffic Manager endpoint.
                      (oas-tags):
                        - Endpoints
                    get:
                      displayName: Endpoints_Get
                      description: Gets a Traffic Manager endpoint.
                      is:
                        - apiVersionParameter
                      responses:
                        '200':
                          body:
                            application/json:
                              type: Endpoint
                          description: The Traffic Manager endpoint.
                      (oas-tags):
                        - Endpoints
                    delete:
                      displayName: Endpoints_Delete
                      description: Deletes a Traffic Manager endpoint.
                      is:
                        - apiVersionParameter
                      responses:
                        '200': {}
                        '204': {}
                      (oas-tags):
                        - Endpoints
