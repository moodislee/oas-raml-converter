      /Microsoft.Network:
        displayName: Microsoft.Network
        /trafficmanagerprofiles:
          displayName: trafficmanagerprofiles
          get:
            (oas-x-ms-pageable): {}
            displayName: Profiles_ListAll
            description: Lists all Traffic Manager profiles within a subscription.
            is:
              - apiVersionParameter
            responses:
              '200':
                body:
                  application/json:
                    type: ProfileListResult
            (oas-tags):
