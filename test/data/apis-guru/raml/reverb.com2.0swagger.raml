                  required: false
              type: object
              (oas-body-name): body
              description: the content of the request
          (oas-responses-default):
            description: Unexpected error
          (oas-tags):
            - my
      /decline:
        displayName: decline
        post:
          displayName: POST_my-negotiations-id-decline
          description: Decline an offer
          (oas-summary): Decline an offer
          body:
            application/json:
