                  type: string
              description: Internal Server Error
          queryParameters:
            max:
              description: The maximum number of records to return
              format: int32
              required: false
              type: integer
            offset:
              description: The offset of the records set to return for pagination.
              format: int32
              required: false
              type: integer
            sort:
              description: Which field to sort the records by.
              required: false
              type: string
