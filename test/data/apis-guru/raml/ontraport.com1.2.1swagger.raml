#%RAML 1.0
title: ONTRAPORT
version: 1.2.1
baseUri: 'https://api.ontraport.com/1'
mediaType: application/json
protocols:
  - HTTPS
description: '<p>Enter your App ID and API Key above. If you do not have an App ID or API Key login to your ONTRAPORT account and navigate <a href="https://app.ontraport.com/#!/api_settings/listAll">here</a>. Authentication parameters must be sent in the request header as <strong>Api-Appid</strong> and <strong>Api-Key</strong>. Each ONTRAPORT account is allowed up to 180 requests per minute.</p>'
(oas-tags-definition):
  - description: 'Create, update, fetch, and delete Contacts or other Objects'
    name: object
  - description: Fetch Form content
    name: form
  - description: 'Create, update, or fetch Email, SMS, or other Message content'
    name: message
  - description: Cancel and complete Tasks
    name: task
  - description: Everything to do with payment Transactions
    name: transaction
  - description: Fetch Landing Page content
    name: landingPage
annotationTypes:
  oas-tags-definition:
    type: array
    items:
      properties:
        name: string
        description?: string
        externalDocs?:
          properties:
            url: string
            description?: string
    allowedTargets: API
  oas-x-logo: any
  oas-x-origin: any
  oas-x-preferred: any
  oas-x-providerName: any
  oas-responses: any
  oas-tags:
    type: 'string[]'
    allowedTargets: Method
  oas-summary:
    type: string
    allowedTargets: Method
  oas-externalDocs:
    properties:
      description?: string
      url: string
    allowedTargets:
      - API
      - Method
      - TypeDeclaration
  oas-info:
    properties:
      termsOfService?: string
      contact?:
        properties:
          name?: string
          url?: string
          email?: string
      license?:
        properties:
          name?: string
          url?: string
    allowedTargets: API
  oas-body-name:
    type: string
    allowedTargets: TypeDeclaration
(oas-info):
  (oas-x-logo):
    url: 'https://api.apis.guru/v2/cache/logo/https_ontraport.com_assets_images_ONTRAPORT_logo_150px.png'
  (oas-x-origin):
    format: swagger
    url: 'https://api.ontraport.com/1/api-docs/swagger.json'
    version: '2.0'
  (oas-x-preferred): true
  (oas-x-providerName): ontraport.com
  contact:
    name: The ONTRAPORT Engineering Team
    url: 'http://support.ontraport.com/'
    email: support@ontraport.com
  termsOfService: 'https://ontraport.com/terms-of-service'
(oas-externalDocs):
  url: 'https://ontraport.com/ontraport-api'
/objects:
  displayName: objects
  /tag:
    displayName: tag
    put:
      displayName: addTag
      description: 'This will add a Tag to an Object. when adding or removing Tags, either <b>ids</b> or <b>group_ids</b> must be given.'
      (oas-summary): This will add an Object Tag
      is:
        - objectIdParamFormData
        - addListParamFormData
        - idsParamFormData
        - startParamFormData
        - rangeParamFormData
        - sortParamFormData
        - sortDirParamFormData
        - conditionParamFormData
        - searchParamFormData
        - searchNotesParamFormData
        - dateRangeParamFormData
        - groupIdsParamFormData
        - externsParamFormData
        - listFieldsParamFormData
      responses:
        '200':
          body:
            application/json:
              type: Response
          description: Successfully added Tag(s)
      (oas-tags):
        - objects
    delete:
      displayName: removeTag
      description: 'This will remove a Tag from an Object using a Tag ID. When adding or removing Tags, either <b>ids</b> or <b>group_ids</b> must be given.'
      (oas-summary): This will remove an Object Tag
      is:
        - objectIdParamFormData
        - removeListParamFormData
        - idsParamFormData
        - startParamFormData
        - rangeParamFormData
        - sortParamFormData
        - sortDirParamFormData
        - conditionParamFormData
        - searchParamFormData
        - searchNotesParamFormData
        - dateRangeParamFormData
        - groupIdsParamFormData
        - externsParamFormData
        - listFieldsParamFormData
      responses:
        '200':
          body:
            application/json:
              type: Response
          description: Successfully removed Tag(s)
      (oas-tags):
        - objects
  /unpause:
    displayName: unpause
    post:
      displayName: unpause
      description: This will upause a Sequence. Either <b>ids</b> or <b>group_ids</b> must be given.
      (oas-summary): This will unpause a Sequence
      is:
        - objectIdParamFormData
        - idsParamFormData
        - startParamFormData
        - rangeParamFormData
        - sortParamFormData
        - sortDirParamFormData
        - conditionParamFormData
        - searchParamFormData
        - dateRangeParamFormData
        - groupIdsParamFormData
        - externsParamFormData
        - listFieldsParamFormData
      responses:
        '200':
          body:
            application/json:
              type: Response
          description: Successfully unpaused Sequence(s)
      (oas-tags):
        - objects
  get:
    displayName: getObjects
    (oas-summary): This will get a list of Objects
    is:
      - objectIdParam
      - idsParam
      - startParam
      - rangeParam
      - sortParam
      - sortDirParam
      - conditionParam
      - searchParam
      - searchNotesParam
      - dateRangeParam
      - groupIdsParam
      - externsParam
      - listFieldsParam
    responses:
      '200':
        body:
          application/json:
            type: Response
        description: Successful object query
      '400':
        description: Invalid objectID
    (oas-tags):
      - objects
  put:
    displayName: updateObjects
    description: 'This will update an Object''s data. The Object type and ID and required parameters, but other parameters are dependent upon the object type. We have provided optional parameters as examples for updating a Contact. When using this in your application, all invalid parameters will be ignored.'
    (oas-summary): This will update an Object's data
    is:
      - objectIdParamFormData
      - idParamFormData
    body:
      multipart/form-data:
        properties:
          firstname:
            description: New first name for the Object.
            type: string
            required: false
          lastname:
            description: New last name for the Object.
            type: string
            required: false
          email:
            description: New email address for the Object.
            type: string
            required: false
    responses:
      '200':
        body:
          application/json:
            type: Response
        description: Successfully updated object
      '404':
        description: Object not found
    (oas-tags):
      - objects
  /meta:
    displayName: meta
    get:
      displayName: getMeta
      description: This will get a list of existing Object types and their corresponding field names. Use the <b>objectID</b> parameter to This will get a single Object type.
      (oas-summary): This will get a list of existing Object types
      responses:
        '200':
          body:
            application/json:
              type: Response
          description: Successful object query
      queryParameters:
        format:
          default: byId
          description: Choose to get the list indexed by Object class name or integer ID.
          enum:
            - byId
            - byName
          type: string
        objectID:
          default: 0
          description: Object Type ID.
          format: int32
          type: integer
      (oas-tags):
        - objects
  /saveorupdate:
    displayName: saveorupdate
    post:
      displayName: saveorupdateObject
      description: 'This will create an Object with the given data or will merge with an Object in the database if the unique field matches another Objectâ€™s. Object type is a required parameter, but other parameters are optional and depend on the Object type. We have provided optional parameters as examples for updating a Contact. When using this in your application, all invalid parameters will be ignored.'
      (oas-summary): Create or Merge an Object
      is:
        - objectIdParamFormData
      body:
        multipart/form-data:
          properties:
            firstname:
              description: First name for the Object.
              type: string
              required: false
            lastname:
              description: Last name for the Object.
              type: string
              required: false
            email:
              description: Email address for the Object.
              type: string
              required: false
      responses:
        '200':
          body:
            application/json:
              type: Response
          description: Successful object query
        '400':
          description: Invalid objectID
      (oas-tags):
        - objects
  post:
    displayName: createObject
    description: 'This will create an Object with the given data. Object type is a required parameters, but other parameters are optional and dependent upon the object type. We have provided optional parameters as examples for updating a Contact. When using this in your application, all invalid parameters will be ignored.'
    (oas-summary): This will create an Object
    is:
      - objectIdParamFormData
    body:
      multipart/form-data:
        properties:
          firstname:
            description: New first name for the Object.
            type: string
            required: false
          lastname:
            description: New last name for the Object.
            type: string
            required: false
          email:
            description: New email address for the Object.
            type: string
            required: false
    responses:
      '200':
        body:
          application/json:
            type: Response
        description: Successful object query
      '400':
        description: Invalid objectID
    (oas-tags):
      - objects
  /pause:
    displayName: pause
    post:
      displayName: pause
      description: This will pause a Sequence. Either <b>ids</b> or <b>group_ids</b> must be given.
      (oas-summary): This will pause a Sequence
      is:
        - objectIdParamFormData
        - idsParamFormData
        - startParamFormData
        - rangeParamFormData
        - sortParamFormData
        - sortDirParamFormData
        - conditionParamFormData
        - searchParamFormData
        - dateRangeParamFormData
        - groupIdsParamFormData
        - externsParamFormData
        - listFieldsParamFormData
      responses:
        '200':
          body:
            application/json:
              type: Response
          description: Successfully pause Sequence(s)
      (oas-tags):
        - objects
  /getInfo:
    displayName: getInfo
    get:
      displayName: getObjectsInfo
      description: 'Get information about an Objects Collection, such as the number of Objects that match the given criteria.'
      (oas-summary): Get information about an Objects Collection
      is:
        - objectIdParam
        - conditionParam
        - searchParam
        - searchNotesParam
        - dateRangeParam
        - groupIdsParam
      responses:
        '200':
          body:
            application/json:
              type: Response
          description: Successful object query
        '400':
          description: Invalid objectID
      (oas-tags):
        - objects
  delete:
    displayName: deleteObjects
    (oas-summary): This will delete a list of Objects
    is:
      - objectIdParam
      - idsParam
      - startParam
      - rangeParam
      - sortParam
      - sortDirParam
      - conditionParam
      - searchParam
      - searchNotesParam
      - dateRangeParam
      - groupIdsParam
      - externsParam
      - listFieldsParam
    responses:
      '200':
        body:
          application/json:
            type: Response
        description: Successful object query
      '400':
        description: Invalid objectID
    (oas-tags):
      - objects
/transaction:
  displayName: transaction
  /writeOff:
    displayName: writeOff
    put:
      displayName: writeOff
      (oas-summary): Write-off a Transaction
      is:
        - idParamFormData
      responses:
        '200':
          body:
            application/json:
              type: Response
          description: Successfully wrote off transaction
        '400':
          description: Error writing off transaction
        '404':
          description: Object not found
      (oas-tags):
        - transactions
  /convertToCollections:
    displayName: convertToCollections
    put:
      displayName: convertToCollections
      (oas-summary): Convert the status of one or more Transactions to Collections
      is:
        - idParamFormData
      responses:
        '200':
          body:
            application/json:
              type: Response
          description: Successfully converted transaction
        '400':
          description: Error converting transaction
        '404':
          description: Object not found
      (oas-tags):
        - transactions
  /convertToDecline:
    displayName: convertToDecline
    put:
      displayName: convertToDecline
      (oas-summary): Convert the status of one or more Transactions to Declined
      is:
        - idParamFormData
      responses:
        '200':
          body:
            application/json:
              type: Response
          description: Successfully converted transaction
        '400':
          description: Error converting transaction
        '404':
          description: Object not found
      (oas-tags):
        - transactions
  /markPaid:
    displayName: markPaid
    put:
      displayName: markPaid
      (oas-summary): Mark a Transaction as Paid
      is:
        - idParamFormData
      responses:
        '200':
          body:
            application/json:
              type: Response
          description: Successfully marked as paid
        '400':
          description: Error marking Transaction as paid
        '404':
          description: Object not found
      (oas-tags):
        - transactions
  /order:
    displayName: order
    get:
      displayName: getOrder
      (oas-summary): Get full information about an order
      responses:
        '200':
          body:
            application/json:
              type: Response
          description: Success fetching Order
        '400':
          description: Error fetching Order
      queryParameters:
        id:
          description: Order ID.
          required: true
          type: integer
      (oas-tags):
        - transactions
    put:
      displayName: setOrder
      description: 'Update information about an Order. Please note that if offer data is omitted, this Order will be deleted.'
      (oas-summary): Update information about an Order
      body:
        application/json:
          type: Order
          (oas-body-name): order
          description: Order Object.
      responses:
        '200':
          body:
            application/json:
              type: Response
          description: Successfully updated Order information
        '400':
          description: Error updating Order information
      (oas-tags):
        - transactions
  /processManual:
    displayName: processManual
    post:
      displayName: processManual
      description: 'This will create a Transaction for a Contact. Please note that this request requires valid parameters for all associated members of the Transaction or the request will fail. If you have doubled-checked all parameters and the request is still failing, check that the payload JSON format is correct by using <a href="http://jsonlint.com/">JSONLint</a>. If credit card and billing information already exist for this contact, you may omit <b>billing_address</b> and <b>payer</b> from the request. Note that <b>trans_date</b> is given in milliseconds elapsed since midnight on 1/1/1970.'
      (oas-summary): This will create a new Transaction
      body:
        application/json:
          type: Transaction
          (oas-body-name): transaction
          description: Definition of the Transaction.
      responses:
        '200':
          body:
            application/json:
              type: Response
          description: Successfully created transaction
        '400':
          description: Error processing transaction
      (oas-tags):
        - transactions
  /refund:
    displayName: refund
    put:
      displayName: refund
      (oas-summary): Refund one or more Transactions
      body:
        application/json:
          type: Criteria
          (oas-body-name): criteria
          description: Criteria of Transactions to refund.
      responses:
        '200':
          body:
            application/json:
              type: Response
          description: Successfully refunded transaction
        '400':
          description: Error refunding transaction
      (oas-tags):
        - transactions
  /rerun:
    displayName: rerun
    post:
      displayName: rerun
      description: Re-run a single Transaction or a Group of Transactions that are currently in Collections.
      (oas-summary): Re-run a Transaction
      body:
        application/json:
          type: Criteria
          (oas-body-name): criteria
          description: Criteria of Transactions to rerun.
      responses:
        '200':
          body:
            application/json:
              type: Response
          description: Successfully reran transaction
        '400':
          description: Error rerunning transaction
      (oas-tags):
        - transactions
  /voidPurchase:
    displayName: voidPurchase
    put:
      displayName: voidPurchase
      (oas-summary): Void a purchase
      is:
        - idParamFormData
      responses:
        '200':
          body:
            application/json:
              type: Response
          description: Successfully voided purchase
        '400':
          description: Error voiding purchase
        '404':
          description: Object not found
      (oas-tags):
        - transactions
  /void:
    displayName: void
    put:
      displayName: voidTransaction
      (oas-summary): Void a Transaction
      body:
        application/json:
          type: Criteria
          (oas-body-name): criteria
          description: Criteria of Transactions to void.
      responses:
        '200':
          body:
            application/json:
              type: Response
          description: Successfully voided transaction
        '400':
          description: Error voiding transaction
      (oas-tags):
        - transactions
  /resendInvoice:
    displayName: resendInvoice
    post:
      displayName: resendInvoice
      (oas-summary): Re-send a Transaction invoice
      body:
        application/json:
          type: Criteria
          (oas-body-name): criteria
          description: Criteria of Transactions to resend invoices.
      responses:
        '200':
          body:
            application/json:
              type: Response
          description: Successfully created transaction
        '400':
          description: Error processing transaction
      (oas-tags):
        - transactions
  /rerunCommission:
    displayName: rerunCommission
    put:
      displayName: rerunCommission
      (oas-summary): Re-run a Partner commission
      body:
        application/json:
          type: Criteria
          (oas-body-name): criteria
          description: Criteria of Transactions to rerun commissions.
      responses:
        '200':
          body:
            application/json:
              type: Response
          description: Successfully reran commission
        '400':
          description: Error rerunning commission
      (oas-tags):
        - transactions
/form:
  displayName: form
  get:
    displayName: getForm
    description: 'This will fetch a Form''s HTML for a given ID.<br/><b>Note</b>: HTML is returned in the <code>data</code> member of the JSON response. As a result, the HTML is escaped and you will have to unescape the HTML before using it.'
    (oas-summary): This will get an existing Form
    is:
      - idParam
    responses:
      '200':
        body:
          application/json:
            type: Response
        description: Successful object query
      '404':
        description: Form not found
    (oas-tags):
      - forms
/task:
  displayName: task
  /cancel:
    displayName: cancel
    post:
      displayName: taskCancel
      description: 'To affect a single Task or list of specific Tasks, use the <strong>ids</strong> array in the <strong>criteria</strong> parameter. Otherwise, you should use <strong>performAll</strong> and other criteria to select a Group of Tasks to cancel.'
      (oas-summary): Cancel a set of Tasks
      body:
        application/json:
          type: Criteria
          (oas-body-name): criteria
          description: Criteria of the Tasks to cancel.
      responses:
        '200':
          body:
            application/json:
              type: Response
          description: Successfully canceled tasks
      (oas-tags):
        - tasks
  /complete:
    displayName: complete
    post:
      displayName: taskComplete
      description: 'This will mark one or many Tasks as complete. As an option, you can assign a new Task for further follow-up. To affect a single Task or list of specific Tasks, use the <strong>ids</strong> array in the <strong>criteria</strong> parameter. Otherwise, you should use <strong>performAll</strong> and other criteria to select a Group of Tasks to complete.'
      (oas-summary): Complete a set of Tasks
      body:
        application/json:
          type: TaskCompleteParams
          (oas-body-name): body
          description: Criteria and data of the Tasks to complete.
      responses:
        '200':
          body:
            application/json:
              type: Response
          description: Successfully canceled tasks
      (oas-tags):
        - tasks
/message:
  displayName: message
  get:
    displayName: getMessage
    description: This will fetch data from an existing Message.
    (oas-summary): This will get an existing Message
    is:
      - idParam
    responses:
      '200':
        body:
          application/json:
            type: Response
        description: Successful object query
      '404':
        description: Message not found
    (oas-tags):
      - messages
  put:
    displayName: updateMessage
    (oas-summary): This will update an existing Message
    body:
      multipart/form-data:
        properties:
          id:
            description: ID of the Message to be updated.
            format: int32
            required: true
            type: integer
          objectID:
            description: Object Type ID.
            enum:
              - 7
            format: int32
            type: integer
            required: false
          alias:
            description: Message alias.
            type: string
            required: false
          name:
            description: Name of your Message.
            type: string
            required: false
          subject:
            description: Subject to be sent with the Message.
            type: string
            required: false
          from:
            description: 'Who will this Message come from? If not <b>owner</b> or <b>custom</b>, this field can contain the Staff user ID to send the Message from.'
            enum:
              - owner
              - custom
              - <staff user ID>
            type: string
            required: false
          send_out_name:
            default: owner
            description: Name to be used in the 'Message From' field when the <b>from</b> field is set to <b>custom</b>.
            type: string
            required: false
          reply_to_email:
            default: Contact owner email
            description: The email address to use as the reply-to in the Message.
            type: string
            required: false
          plaintext:
            description: 'The plain text version of your Message (for Email messages). For SMS Messages, this is the content that will be sent.'
            type: string
            required: false
          send_from:
            description: 'Custom Send-from email address. Leave this empty to use your default email address. Otherwise, this must be a validated send-from email address.'
            type: string
            required: false
          send_to:
            default: email
            description: Only used with SMS Messages or Custom Objects. Select the field to use when there may be a parent or child relationship to the Object type given in <b>object_type_id</b>. The default for SMS Message is <b>sms_number.</b>
            type: string
            required: false
          message_body:
            description: Use this for Legacy Email Messages only. This is the HTML content of the Message.
            type: string
            required: false
          resource:
            description: 'For ONTRAmail Messages only; this is the content of the message. Note that this can be extremely complex, so you should start by inspecting the resource that is sent when saving a Message from inside the App. For SMS Messages, use the plaintext field for content.'
            type: string
            required: false
          email_title:
            description: Short title that will appear as the first line in email clients.
            type: string
            required: false
          task_data:
            description: Use this only for Task Messages. This is the content of the Message.
            type: string
            required: false
          due_date:
            description: Use this only for Task Messages. This is the number of days after assignment this Task is due.
            format: int32
            type: integer
            required: false
          task_owner:
            description: Use this only for Task Messages. This is the Staff user ID of the Task owner.
            format: int32
            type: integer
            required: false
          task_form:
            description: Use this only for Task Messages. This is the optional Form ID to be filled when the Task is completed. This form must already exist.
            format: int32
            type: integer
            required: false
    responses:
      '200':
        body:
          application/json:
            type: Response
        description: Message successfully updated
    (oas-tags):
      - messages
  post:
    displayName: saveMessage
    (oas-summary): This will create a new Message
    body:
      multipart/form-data:
        properties:
          objectID:
            description: Object Type ID.
            enum:
              - 7
            format: int32
            type: integer
            required: false
          alias:
            description: Message alias.
            type: string
            required: false
          name:
            description: Name of your Message.
            type: string
            required: false
          subject:
            description: Subject to be sent with the Message.
            type: string
            required: false
          type:
            description: 'Message type: use <b>template</b> for ONTRAmail, <b>e-mail</b> for Legacy Email, <b>sms</b> for SMS Messages, <b>task</b> for Task Messages.'
            enum:
              - template
              - e-mail
              - sms
              - task
            type: string
            required: false
          object_type_id:
            description: Object type to associate with this Message. This will be zero (0) for Contacts. You should only change this if you are using Custom Objects.
            format: int32
            type: integer
            required: false
          from:
            description: 'Who will this Message come from? If not <b>owner</b> or <b>custom</b>, this field can contain the Staff user ID to send the Message from.'
            enum:
              - owner
              - custom
              - <staff user ID>
            type: string
            required: false
          send_out_name:
            default: owner
            description: Name to be used in the 'Message From' field when the <b>from</b> field is set to <b>custom</b>.
            type: string
            required: false
          reply_to_email:
            default: Contact owner email
            description: The email address to use as the reply-to in the Message.
            type: string
            required: false
          plaintext:
            description: 'The plain text version of your Message (for Email messages). For SMS Messages, this is the content that will be sent.'
            type: string
            required: false
          send_from:
            description: 'Custom Send-from email address. Leave this empty to use your default email address. Otherwise, this must be a validated send-from email address.'
            type: string
            required: false
          send_to:
            default: email
            description: Only used with SMS Messages or Custom Objects. Select the field to use when there may be a parent or child relationship to the Object type given in <b>object_type_id</b>. The default for SMS Message is <b>sms_number.</b>
            type: string
            required: false
          message_body:
            description: Use this for Legacy Email Messages only. This is the HTML content of the Message.
            type: string
            required: false
          resource:
            description: 'For ONTRAmail Messages only; this is the content of the message. Note that this can be extremely complex, so you should start by inspecting the resource that is sent when saving a Message from inside the App. For SMS Messages, use the plaintext field for content.'
            type: string
            required: false
          email_title:
            description: Short title that will appear as the first line in email clients.
            type: string
            required: false
          task_data:
            description: Use this only for Task Messages. This is the content of the Message.
            type: string
            required: false
          due_date:
            description: Use this only for Task Messages. This is the number of days after assignment this Task is due.
            format: int32
            type: integer
            required: false
          task_owner:
            description: Use this only for Task Messages. This is the Staff user ID of the Task owner.
            format: int32
            type: integer
            required: false
          task_form:
            description: Use this only for Task Messages. This is the optional Form ID to be filled when the Task is completed. This form must already exist.
            format: int32
            type: integer
            required: false
    responses:
      '200':
        body:
          application/json:
            type: Response
        description: Message successfully created
    (oas-tags):
      - messages
/object:
  displayName: object
  get:
    displayName: getObject
    description: This will fetch data for a given Object type and ID.
    (oas-summary): This will get an Object's data
    is:
      - objectIdParam
      - idParam
    responses:
      '200':
        body:
          application/json:
            type: Response
        description: Successful object query
      '400':
        description: Invalid objectID
      '404':
        description: Object not found
    (oas-tags):
      - objects
  delete:
    displayName: deleteObject
    description: This will delete an Object with the given Object type and ID.
    (oas-summary): This will delete an Object
    is:
      - objectIdParam
      - idParam
    responses:
      '200':
        body:
          application/json:
            type: Response
        description: Successful object query
      '400':
        description: Invalid objectID
      '404':
        description: Object not found
    (oas-tags):
      - objects
/landingPage:
