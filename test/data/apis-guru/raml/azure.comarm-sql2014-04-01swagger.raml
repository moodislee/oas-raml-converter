                    (oas-x-ms-pageable): {}
                    displayName: ElasticPools_ListByServer
                    description: Returns information about Azure SQL elastic pools.
                    is:
                      - apiVersionParameter
                    responses:
                      '200':
                        body:
                          application/json:
                            type: ElasticPoolListResult
                        description: OK
                    (oas-tags):
                      - ElasticPools
                  '/{elasticPoolName}':
                    displayName: '{elasticPoolName}'
                    uriParameters:
                      elasticPoolName:
                        description: The name of the Azure SQL Elastic Pool to be operated on (Updated or created).
                        required: true
                        type: string
                    delete:
                      displayName: ElasticPools_Delete
                      description: Deletes the Azure SQL elastic pool.
                      is:
                        - apiVersionParameter
                      responses:
                        '200':
                          description: OK
                        '204':
                          description: Accepted
                      (oas-tags):
                        - ElasticPools
                    get:
                      displayName: ElasticPools_Get
                      description: Gets information about an Azure SQL elastic pool.
                      is:
                        - apiVersionParameter
                      responses:
                        '200':
                          body:
                            application/json:
                              type: ElasticPool
                          description: OK
                      (oas-tags):
                        - ElasticPools
                    put:
                      (oas-x-ms-long-running-operation): true
                      displayName: ElasticPools_CreateOrUpdate
                      description: Creates a new Azure SQL elastic pool or updates an existing Azure SQL elastic pool.
                      is:
                        - apiVersionParameter
                      body:
                        application/json:
                          type: ElasticPool
                          (oas-body-name): parameters
                          description: The required parameters for creating or updating an Elastic Pool.
                      responses:
                        '200':
                          body:
                            application/json:
                              type: ElasticPool
                          description: OK
                        '201':
                          body:
                            application/json:
                              type: ElasticPool
                          description: Created
                        '202':
                          body:
                            application/json:
                              type: ElasticPool
                          description: Accepted
                      (oas-tags):
                        - ElasticPools
                    /databases:
                      displayName: databases
                      get:
                        (oas-x-ms-pageable): {}
                        displayName: ElasticPools_ListDatabases
                        description: Returns information about an Azure SQL database inside of an Azure SQL elastic pool.
                        is:
                          - apiVersionParameter
                        responses:
                          '200':
                            body:
                              application/json:
                                type: DatabaseListResult
                            description: OK
                        (oas-tags):
                          - ElasticPools
                      '/{databaseName}':
                        displayName: '{databaseName}'
                        uriParameters:
                          databaseName:
                            description: The name of the Azure SQL database to be retrieved.
                            required: true
                            type: string
                        get:
                          displayName: ElasticPools_GetDatabase
                          description: Gets information about an Azure SQL database inside of an Azure SQL elastic pool.
                          is:
                            - apiVersionParameter
                          responses:
                            '200':
                              body:
                                application/json:
                                  type: Database
                              description: OK
                          (oas-tags):
                            - ElasticPools
                    /elasticPoolActivity:
                      displayName: elasticPoolActivity
                      get:
                        (oas-x-ms-pageable): {}
                        displayName: ElasticPools_ListActivity
                        description: Returns information about Azure SQL elastic pool activities.
                        is:
                          - apiVersionParameter
                        responses:
                          '200':
                            body:
                              application/json:
                                type: ElasticPoolActivityListResult
                            description: OK
                        (oas-tags):
                          - ElasticPools
                    /elasticPoolDatabaseActivity:
                      displayName: elasticPoolDatabaseActivity
                      get:
                        (oas-x-ms-pageable): {}
                        displayName: ElasticPools_ListDatabaseActivity
                        description: Returns information about activity on Azure SQL databases inside of an Azure SQL elastic pool.
                        is:
                          - apiVersionParameter
                        responses:
                          '200':
                            body:
                              application/json:
                                type: ElasticPoolDatabaseActivityListResult
                            description: OK
                        (oas-tags):
                          - ElasticPools
                uriParameters:
                  serverName:
                    description: The name of the Azure SQL server.
                    required: true
                    type: string
                delete:
