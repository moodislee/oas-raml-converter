                        - apiVersionParameter
                      body:
                        application/json:
                          type: Database
                          (oas-body-name): parameters
                          description: The required parameters for creating or updating a database.
                      responses:
                        '200':
                          body:
                            application/json:
                              type: Database
                          description: OK
                        '201':
                          body:
                            application/json:
                              type: Database
                          description: Created
                        '202':
                          body:
                            application/json:
                              type: Database
                          description: Accepted
                      (oas-tags):
                        - Databases
                    /restorePoints:
                      displayName: restorePoints
                      get:
                        (oas-x-ms-pageable): {}
                        displayName: Databases_ListRestorePoints
