                          - apiVersionParameter
                        responses:
                          '200':
                            body:
                              application/json:
                                type: ServiceTierAdvisorListResult
                            description: OK
                        (oas-tags):
                          - ServiceTierAdvisors
                      '/{serviceTierAdvisorName}':
                        displayName: '{serviceTierAdvisorName}'
