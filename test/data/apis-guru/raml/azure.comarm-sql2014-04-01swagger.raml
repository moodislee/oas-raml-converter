                        description: Pause an Azure SQL Data Warehouse database.
                        is:
                          - apiVersionParameter
                        responses:
                          '200':
                            description: OK
                          '202':
                            description: Accepted
                        (oas-tags):
                          - DatabaseActivation
                  get:
                    (oas-x-ms-pageable): {}
                    displayName: Databases_ListByServer
                    description: Returns information about an Azure SQL database.
                    is:
                      - apiVersionParameter
                    responses:
                      '200':
                        body:
                          application/json:
                            type: DatabaseListResult
                        description: OK
                    (oas-tags):
                      - Databases
                /usages:
