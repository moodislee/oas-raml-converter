                            - apiVersionParameter
                          responses:
                            '200':
                              body:
                                application/json:
                                  type: TransparentDataEncryption
                              description: OK
                          (oas-tags):
                            - TransparentDataEncryption
                        /operationResults:
                          displayName: operationResults
                          get:
                            (oas-x-ms-pageable): {}
                            displayName: Databases_ListTransparentDataEncryptionActivity
                            description: Returns an Azure SQL Database Transparent Data Encryption Activity Response.
                            is:
                              - apiVersionParameter
                            responses:
                              '200':
                                body:
                                  application/json:
                                    type: TransparentDataEncryptionActivityListResult
                                description: OK
                            (oas-tags):
                              - TransparentDataEncryption
                        put:
                          displayName: Databases_CreateOrUpdateTransparentDataEncryptionConfiguration
                          description: Creates or updates an Azure SQL Database Transparent Data Encryption Operation.
