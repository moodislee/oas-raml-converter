                  is:
                    - apiVersionParameter
                  body:
                    application/json:
                      type: Server
                      (oas-body-name): parameters
                      description: The required parameters for creating or updating a server.
                  responses:
                    '200':
                      body:
                        application/json:
                          type: Server
                      description: OK
                    '201':
                      body:
                        application/json:
                          type: Server
                      description: Created
                  (oas-tags):
                    - Servers
                /recommendedElasticPools:
                  displayName: recommendedElasticPools
                  get:
                    (oas-x-ms-pageable): {}
                    displayName: RecommendedElasticPools_List
                    description: Returns information about Azure SQL Recommended Elastic Pools.
                    is:
                      - apiVersionParameter
                    responses:
                      '200':
                        body:
                          application/json:
                            type: RecommendedElasticPoolListResult
                        description: OK
                    (oas-tags):
                      - RecommendedElasticPools
                  '/{recommendedElasticPoolName}':
                    displayName: '{recommendedElasticPoolName}'
                    uriParameters:
                      recommendedElasticPoolName:
                        description: The name of the Azure SQL Recommended Elastic Pool to be retrieved.
                        required: true
                        type: string
                    get:
                      displayName: RecommendedElasticPools_Get
                      description: Gets information about an Azure SQL Recommended Elastic Pool.
                      is:
                        - apiVersionParameter
                      responses:
                        '200':
                          body:
                            application/json:
                              type: RecommendedElasticPool
                          description: OK
                      (oas-tags):
                        - RecommendedElasticPools
                    /databases:
                      displayName: databases
                      get:
                        (oas-x-ms-pageable): {}
                        displayName: RecommendedElasticPools_ListDatabases
                        description: Returns information about an Azure SQL database inside of an Azure SQL Recommended Elastic Pool.
                        is:
                          - apiVersionParameter
                        responses:
                          '200':
                            body:
                              application/json:
                                type: DatabaseListResult
                            description: OK
                        (oas-tags):
                          - RecommendedElasticPools
                      '/{databaseName}':
                        displayName: '{databaseName}'
                        uriParameters:
                          databaseName:
                            description: The name of the Azure SQL database to be retrieved.
                            required: true
                            type: string
                        get:
                          displayName: RecommendedElasticPools_GetDatabases
                          description: Gets information about an Azure SQL database inside of an Azure SQL Recommended Elastic Pool.
                          is:
                            - apiVersionParameter
                          responses:
                            '200':
                              body:
                                application/json:
                                  type: Database
                              description: OK
                          (oas-tags):
                            - RecommendedElasticPools
                    /metrics:
                      displayName: metrics
                      get:
                        (oas-x-ms-pageable): {}
                        displayName: RecommendedElasticPools_ListMetrics
                        description: Returns information about an recommended elastic pool metrics.
                        is:
                          - apiVersionParameter
                        responses:
                          '200':
                            body:
                              application/json:
                                type: RecommendedElasticPoolListMetricsResult
                            description: OK
                        (oas-tags):
                          - RecommendedElasticPools
                get:
                  displayName: Servers_GetByResourceGroup
                  description: Gets information about an Azure SQL server.
                  is:
                    - apiVersionParameter
                  responses:
                    '200':
                      body:
                        application/json:
