    /resourceGroups:
      displayName: resourceGroups
      '/{resourceGroupName}':
        displayName: '{resourceGroupName}'
        /providers:
          displayName: providers
          /Microsoft.Sql:
            displayName: Microsoft.Sql
            /servers:
              displayName: servers
              '/{serverName}':
                displayName: '{serverName}'
                /databases:
                  displayName: databases
                  '/{databaseName}':
                    displayName: '{databaseName}'
                    /usages:
                      displayName: usages
                      get:
                        (oas-x-ms-pageable): {}
