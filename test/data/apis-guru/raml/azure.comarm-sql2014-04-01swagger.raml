                          - apiVersionParameter
                        responses:
                          '200':
                            body:
                              application/json:
                                type: RestorePointListResult
                            description: OK
                        (oas-tags):
                          - DatabaseBackup
                    /serviceTierAdvisors:
                      displayName: serviceTierAdvisors
