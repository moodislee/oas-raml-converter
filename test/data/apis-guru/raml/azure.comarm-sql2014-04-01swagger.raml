                            - apiVersionParameter
                          responses:
                            '200':
                              body:
                                application/json:
                                  type: ServiceTierAdvisor
                              description: OK
                          (oas-tags):
                            - ServiceTierAdvisors
                    /transparentDataEncryption:
                      displayName: transparentDataEncryption
