                            - apiVersionParameter
                          body:
                            application/json:
                              type: TransparentDataEncryption
                              (oas-body-name): parameters
                              description: The required parameters for creating or updating transparent data encryption.
                          responses:
                            '200':
                              body:
                                application/json:
                                  type: TransparentDataEncryption
                              description: OK
                            '201':
                              body:
                                application/json:
                                  type: TransparentDataEncryption
                              description: Created
                          (oas-tags):
                            - TransparentDataEncryption
                    get:
                      displayName: Databases_Get
