                      serviceObjectiveName:
                        description: The name of the service objective to retrieve.
                        required: true
                        type: string
                    get:
                      displayName: Servers_GetServiceObjective
                      description: Gets information about an Azure SQL database Service Objective.
                      is:
                        - apiVersionParameter
                      responses:
                        '200':
                          body:
                            application/json:
                              type: ServiceObjective
                          description: OK
                      (oas-tags):
                        - ServiceObjectives
                  get:
                    (oas-x-ms-pageable): {}
                    displayName: Servers_ListServiceObjectives
                    description: Returns information about Azure SQL database Service Objectives.
