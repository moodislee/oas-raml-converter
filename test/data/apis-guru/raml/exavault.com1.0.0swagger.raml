#%RAML 1.0
title: ExaVault
version: 1.0.0
baseUri: 'https://api.exavault.com/'
protocols:
  - HTTPS
description: |
  ExaVault's API allows you to incorporate ExaVault's suite of file transfer and user management tools into your own application.

  ExaVault supports both POST (recommended when requesting large data sets) and GET operations, and requires an API key in order to use.
(oas-tags-definition):
  - description: 'operations for files, notifications, sharing and users'
    name: v1
annotationTypes:
  oas-tags-definition:
    type: array
    items:
      properties:
        name: string
        description?: string
        externalDocs?:
          properties:
            url: string
            description?: string
    allowedTargets: API
  oas-x-logo: any
  oas-x-origin: any
  oas-x-preferred: any
  oas-x-providerName: any
  oas-tags:
    type: 'string[]'
    allowedTargets: Method
  oas-summary:
    type: string
    allowedTargets: Method
  oas-externalDocs:
    properties:
      description?: string
      url: string
    allowedTargets:
      - API
      - Method
      - TypeDeclaration
  oas-info:
    properties:
      termsOfService?: string
      contact?:
        properties:
          name?: string
          url?: string
          email?: string
      license?:
        properties:
          name?: string
          url?: string
    allowedTargets: API
(oas-info):
  (oas-x-logo):
    backgroundColor: '#222222'
    url: 'https://api.apis.guru/v2/cache/logo/https_www.exavault.com_common_images_110xNxlogo220x40.png.pagespeed.ic.N1chK3UTV9.png'
  (oas-x-origin):
    format: swagger
    url: 'https://www.exavault.com/api/docs'
    version: '1.2'
  (oas-x-preferred): true
  (oas-x-providerName): exavault.com
(oas-externalDocs):
  url: 'https://www.exavault.com/developer/'
/v1:
  displayName: v1
  /getNotificationActivity:
    displayName: getNotificationActivity
    get:
      displayName: getNotificationActivity
      description: Requires access token obtained via the authenticateUser operation. Available to users with the 'notification' permission
      (oas-summary): Returns all notification activity for the current user
      responses:
        '200':
          body:
            application/json:
              type: NotificationActivityResponse
          description: No response was specified
      queryParameters:
        access_token:
          description: Access token required to make the API call
          required: true
          type: string
      (oas-tags):
        - v1
  /userAvailable:
    displayName: userAvailable
    get:
      displayName: userAvailable
      description: Requires access token obtained via the authenticateUser operation. Available to users with the 'manage users' permission
      (oas-summary): Returns true if requested username has not already been taken in the system
      responses:
        '200':
          body:
            application/json:
              type: AvailableUserResponse
          description: No response was specified
      queryParameters:
        access_token:
          description: Access token required to make the API call
          required: true
          type: string
        username:
          description: Username to check
          required: true
          type: string
      (oas-tags):
        - v1
  /getNotifications:
    displayName: getNotifications
    get:
      displayName: getNotifications
      description: Requires access token obtained via the authenticateUser operation. Available to users with the 'notification' permission
      (oas-summary): Returns all notifications for the current user
      responses:
        '200':
          body:
            application/json:
              type: NotificationsResponse
          description: No response was specified
      queryParameters:
        access_token:
          description: Access token required to make the API call
          required: true
          type: string
        type:
          description: 'Type of notification to filter on: ''file'', ''folder'', ''shared_folder'', ''send_receipt'', ''share_receipt'', ''file_drop'''
          required: true
          type: string
        sortBy:
          description: 'Sort by one of the following: ''sort_notifications_folder_name'', ''sort_notifications_path'', ''sort_notifications_date'''
          required: false
          type: string
        sortOrder:
          description: Sort by 'asc' or 'desc' order.
          required: false
          type: string
        filter:
          description: Filter by the provided search terms.
          required: false
          type: string
      (oas-tags):
        - v1
  /authenticateUser:
    displayName: authenticateUser
    get:
      displayName: authenticateUser
      description: Returns access token in the Response object
      (oas-summary): Authenticates a user into the API
      responses:
        '200':
          body:
            application/json:
              type: AuthResponse
          description: No response was specified
      queryParameters:
        username:
          description: Name of of user to authenticate
          required: true
          type: string
        password:
          description: User's password
          required: true
          type: string
      (oas-tags):
        - v1
  /getResourceList:
    displayName: getResourceList
    get:
      displayName: getResourceList
      description: Requires access token obtained via the authenticateUser operation. Available to users with 'list files' permission.
      (oas-summary): Get a listing of files/folders for the specified path
      responses:
        '200':
          body:
            application/json:
              type: ResourceResponse
          description: No response was specified
      queryParameters:
        access_token:
          description: Access token required to make the API call
          required: true
          type: string
        path:
          description: The remote file path
          required: true
          type: string
        sortBy:
          description: 'Sort according to attribute: sort_files_name, sort_files_size, sort_files_date, sort_files_type, sort_files_timeline'
          required: true
          type: string
        sortOrder:
          description: 'Sort in either ascending or descending order: asc, desc'
          required: true
          type: string
        offset:
          description: Determines which item to start on for pagination
          format: int32
          required: true
          type: integer
        limit:
          description: The number of files to limit the result
          format: int32
          required: true
          type: integer
        detailed:
          default: false
          description: 'If true, returns sharedFolder, notifications or other objects associated with specified path'
          type: boolean
        pattern:
          description: 'Regex string. If not null, perform a search with specified pattern'
          type: string
      (oas-tags):
        - v1
  /copyResources:
    displayName: copyResources
    get:
      displayName: copyResources
      description: Requires access token obtained via the authenticateUser operation. Available to users with the 'upload' permission.
      (oas-summary): 'Copies files, folders to the destination path'
      responses:
        '200':
          body:
            application/json:
              type: ModifiedResourcesResponse
          description: No response was specified
      queryParameters:
        access_token:
          description: Access token required to make the API call
          required: true
          type: string
        filePaths:
          description: Remote paths of the files or folders to copy
          items:
            type: string
          required: true
          type: array
        destinationPath:
          description: Remote destination path to copy files/folders to
          required: true
          type: string
      (oas-tags):
        - v1
  /getResourceProperties:
    displayName: getResourceProperties
    get:
      displayName: getResourceProperties
      description: Requires access token obtained via the authenticateUser operation
      (oas-summary): Get the properties for each of the specified files/folders
      responses:
        '200':
          body:
            application/json:
              type: ResourcePropertiesResponse
          description: No response was specified
      queryParameters:
        access_token:
          description: Access token required to make the API call
          required: true
          type: string
        filePaths:
          description: Array containing paths of the files or folder to get
          items:
            type: string
          required: true
          type: array
      (oas-tags):
        - v1
  /createNotification:
    displayName: createNotification
    get:
      displayName: createNotification
      description: Requires access token obtained via the authenticateUser operation. Available to users with 'notification' permission
      (oas-summary): Creates a new Notification object
      responses:
        '200':
          body:
            application/json:
              type: Response
          description: No response was specified
      queryParameters:
        access_token:
          description: Access token required to make the API call
          required: true
          type: string
        path:
          description: Full path of file/folder where notification is set.
          required: true
          type: string
        action:
          description: 'Type of action to filter on: ''upload'', ''download'', ''delete'', ''all'''
          required: true
          type: string
        usernames:
          description: 'User type to filter on: ''notice_user_all'', ''notice_user_all_recipients'', ''notice_user_all_users'''
          required: true
          type: string
        sendEmail:
          description: Set to true if the user should be notified by email when the notification is triggered.
          required: true
          type: boolean
        emails:
          description: 'Email addresses to send notification to. If not specified, sends to owner by default.'
          items:
            type: string
          type: array
      (oas-tags):
        - v1
  /getShare:
    displayName: getShare
    get:
      displayName: getShare
      description: Requires access token obtained via the authenticateUser operation. Available to users with the 'share' permission
      (oas-summary): Returns a share by the specified ID
      responses:
        '200':
          body:
            application/json:
              type: ShareResponse
          description: No response was specified
      queryParameters:
        access_token:
          description: Access token required to make the API call
          required: true
          type: string
        id:
          description: ID of the requested Share
          format: int32
          required: true
          type: integer
      (oas-tags):
        - v1
  /createUser:
    displayName: createUser
    get:
      displayName: createUser
      description: Requires access token obtained via the authenticateUser operation. Available to users with the 'manage users' permission
      (oas-summary): Adds a new subaccount user to the current account
      responses:
        '200':
          body:
            application/json:
              type: Response
          description: No response was specified
      queryParameters:
        access_token:
          description: Access token required to make the API call
          required: true
          type: string
        username:
          description: Name of the subaccount user to create
          required: true
          type: string
        destinationFolder:
          description: The user's home folder
          required: true
          type: string
        email:
          description: The user's email address
          required: true
          type: string
        password:
          description: The user's password
          required: true
          type: string
        role:
          description: 'The user''s role, i.e: ''user'' or ''admin'''
          required: true
          type: string
        permissions:
          description: 'A CSV string of user permissions. The following values are supported: upload, download, delete, modify, list, changePassword, share, notification.'
          required: true
          type: string
        timeZone:
          default: America/Los_Angeles
          description: 'The user''s timezone, used for accurate time display within SWFT. See <a href=''https://php.net/manual/en/timezones.php'' target=''blank''>this page</a> for allowed values'
          required: true
          type: string
        nickname:
          description: The user's nickname
          type: string
        expiration:
          description: 'The date when the user should expire, formatted YYYY-mm-dd'
          type: string
        locked:
          default: false
          description: 'If true, the user''s account is locked by default'
          type: boolean
        welcomeEmail:
          default: false
          description: 'If true, send a user email upon creation'
          type: boolean
      (oas-tags):
        - v1
  /getShareActivity:
    displayName: getShareActivity
    get:
      displayName: getShareActivity
      description: Requires access token obtained via the authenticateUser operation. Available to users with the 'share' permission
      (oas-summary): Return activity log entries for the specified Share ID
      responses:
        '200':
          body:
            application/json:
              type: ShareActivityResponse
          description: No response was specified
      queryParameters:
        access_token:
          description: Access token required to make the API call
          required: true
          type: string
        id:
          description: ID of the Share
          format: int32
          required: true
          type: integer
      (oas-tags):
        - v1
  /deleteResources:
    displayName: deleteResources
    get:
      displayName: deleteResources
      description: Requires access token obtained via the authenticateUser operation. Available to users with the 'delete' permission
      (oas-summary): Delete the specified files/folders
      responses:
        '200':
          body:
            application/json:
              type: DeletedResourcesResponse
          description: No response was specified
      queryParameters:
        access_token:
          description: Access token required to make the API call
          required: true
          type: string
        filePaths:
          description: Array containing paths of the files or folder to delete
          items:
            type: string
          required: true
          type: array
      (oas-tags):
        - v1
  /getShares:
    displayName: getShares
    get:
      displayName: getShares
      description: Requires access token obtained via the authenticateUser operation. Available to users with the 'share' permission
      (oas-summary): Returns all Shares for the current user
      responses:
        '200':
          body:
            application/json:
              type: SharesResponse
          description: No response was specified
      queryParameters:
        access_token:
          description: Access token required to make the API call
          required: true
          type: string
        type:
          description: 'The type of share to return: ''shared_folder'', ''send'', or ''receive''. If no argument specified, will return all Shares types.'
          required: false
          type: string
        sortBy:
          description: 'Sort by one of the following: ''sort_shares_name'', ''sort_shares_date'', ''sort_shares_user'', ''sort_shares_access_mode''.'
          required: false
          type: string
        sortOrder:
          description: Sort by 'asc' or 'desc' order.
          required: false
          type: string
        filter:
          description: Filter by the provided search terms.
          required: false
          type: string
        include:
          description: 'Filter by all, active-only, or current user''s only.'
          required: false
          type: string
        offset:
          description: 'Start position of results to return, for pagination.'
          format: int32
          required: false
          type: integer
        limit:
          description: Maximum number of elements to return or 0 if no limit.
          format: int32
          required: false
          type: integer
      (oas-tags):
        - v1
  /deleteUser:
    displayName: deleteUser
    get:
      displayName: deleteUser
      description: Requires access token obtained via the authenticateUser operation. Available to users with 'manage users' permission
      (oas-summary): Deletes a subaccount user for the current account
      responses:
        '200':
          body:
            application/json:
              type: Response
          description: No response was specified
      queryParameters:
        access_token:
          description: Access token required to make the API call
          required: true
          type: string
        username:
          description: Name of the subaccount user to delete
          required: true
          type: string
      (oas-tags):
        - v1
  /getUploadFileUrl:
    displayName: getUploadFileUrl
    get:
      displayName: getUploadFileUrl
      description: Requires access token obtained via the authenticateUser operation. Called before an upload is started to get URL to appropriate storage server. Available to users with the 'write' permission.
      (oas-summary): Returns a unique URL for handling file uploads
      responses:
        '200':
          body:
            application/json:
              type: UrlResponse
          description: No response was specified
      queryParameters:
        access_token:
          description: Access token required to make the API call
          required: true
          type: string
        fileSize:
          description: 'Size of the file to upload, in bytes'
          format: int64
          required: true
          type: integer
        destinationPath:
          description: 'Path relative to account''s home directory, including file name'
          required: true
          type: string
        allowOverwrite:
          default: true
          description: 'True if the file should be overwritten, false if different file names should be generated'
          required: false
          type: boolean
        resume:
          default: false
          description: 'True if upload resume is supported, false if it isn''t'
          required: false
          type: boolean
      (oas-tags):
        - v1
  /getCurrentUser:
    displayName: getCurrentUser
    get:
      displayName: getCurrentUser
      description: Requires access token obtained via the authenticateUser operation
      (oas-summary): Gets the user object for the currently logged in user
      responses:
        '200':
          body:
            application/json:
              type: UserResponse
          description: No response was specified
      queryParameters:
        access_token:
          description: Access token required to make the API call
          required: true
          type: string
      (oas-tags):
        - v1
  /getUser:
    displayName: getUser
    get:
      displayName: getUser
      description: Requires access token obtained via the authenticateUser operation
      (oas-summary): Get the specified subaccount user for the current account
      responses:
        '200':
          body:
            application/json:
              type: UserResponse
          description: No response was specified
      queryParameters:
        access_token:
          description: Access token required to make the API call
          required: true
          type: string
        username:
          description: Name of the subaccount user to get
          required: true
          type: string
      (oas-tags):
        - v1
  /getFileActivityLogs:
    displayName: getFileActivityLogs
    get:
      displayName: getFileActivityLogs
      (oas-summary): Returns a list of account activity. Allows for searching the activity log.
      responses:
        '200':
          body:
            application/json:
              type: LogResponse
          description: No response was specified
      queryParameters:
        access_token:
          description: Access token required to make the API call
          required: true
          type: string
        offset:
          default: 0
          description: Starting record in the result set. Can be used for pagination.
          format: int32
          required: false
          type: integer
        sortBy:
          default: sort_logs_date
          description: 'Sort method [''sort_logs_date'' or ''sort_logs_ip_address'' or ''sort_logs_username'' or ''sort_logs_file'' or ''sort_logs_file_source'' or ''sort_logs_operation'', or ''sort_logs_duration'', or ''sort_logs_size'', or ''sort_logs_protocol'']'
          required: false
          type: string
        sortOrder:
          default: desc
          description: 'Sort in either ascending or descending order: asc, desc'
          required: false
          type: string
        filterBy:
          description: 'Field to search on [''filter_logs_date'' or ''filter_logs_ip_address'' or ''filter_logs_username'' or ''filter_logs_operation'' or ''filter_logs_file'']'
          required: false
          type: string
        filter:
          description: 'Search criteria. For date ranges, use format ''start_date::end_date'''
          required: false
          type: string
        itemLimit:
          description: Number of logs to return. Can be used for pagination.
          format: int32
          required: false
          type: integer
      (oas-tags):
        - v1
  /getUsers:
    displayName: getUsers
    get:
      displayName: getUsers
      description: Requires access token obtained via the authenticateUser operation. Available to users with the 'manage users' permission set
      (oas-summary): Gets the user object for the currently logged in user
      responses:
        '200':
          body:
            application/json:
              type: UsersResponse
          description: No response was specified
      queryParameters:
        access_token:
          description: Access token required to make the API call
          required: true
          type: string
        sortBy:
          description: 'sort method [''sort_users_username'' or ''sort_users_nickname'' or ''sort_users_email'' or ''sort_users_home_folder'']'
          required: true
          type: string
        sortOrder:
          description: 'sort order, i.e. ''asc'' or ''desc'''
          required: true
          type: string
      (oas-tags):
        - v1
  /getNotification:
    displayName: getNotification
    get:
      displayName: getNotification
      description: Requires access token obtained via the authenticateUser operation. Available to users with the 'notification' permission
      (oas-summary): Returns a notification based on the given ID
      responses:
        '200':
          body:
            application/json:
              type: NotificationResponse
          description: No response was specified
      queryParameters:
        access_token:
          description: Access token required to make the API call
          required: true
          type: string
        id:
          description: ID of the Notification
          format: int32
          required: true
          type: integer
      (oas-tags):
        - v1
  /logoutUser:
    displayName: logoutUser
    get:
      displayName: logoutUser
      description: Requires access token obtained via the authenticateUser operation
      (oas-summary): 'Removes user''s access token from database, logging them out of API'
      responses:
        '200':
          body:
            application/json:
              type: Response
          description: No response was specified
      queryParameters:
        access_token:
          description: Access token required to make the API call
          required: true
          type: string
      (oas-tags):
        - v1
  /createFolder:
    displayName: createFolder
    get:
      displayName: createFolder
      description: Requires access token obtained via the authenticateUser operation. Available to users with the 'upload' permission.
      (oas-summary): Create a folder at a specified path
      responses:
        '200':
          body:
            application/json:
              type: Response
          description: No response was specified
      queryParameters:
        access_token:
          description: Access token required to make the API call
          required: true
          type: string
        folderName:
          description: Name of the folder to create
          required: true
          type: string
        path:
          description: Where to create the folder
          required: true
          type: string
      (oas-tags):
        - v1
  /moveResources:
    displayName: moveResources
    get:
      displayName: moveResources
      description: Requires access token obtained via the authenticateUser operation. Available to users with the 'upload' permission
      (oas-summary): 'Moves files, folders to the destination path'
      responses:
        '200':
          body:
            application/json:
              type: ModifiedResourcesResponse
          description: No response was specified
      queryParameters:
        access_token:
          description: Access token required to make the API call
          required: true
          type: string
        filePaths:
          description: Remote paths of the files or folders to move
          items:
            type: string
          required: true
          type: array
        destinationPath:
          description: Remote destination path to move files/folders to
          required: true
          type: string
      (oas-tags):
        - v1
  /deleteNotification:
    displayName: deleteNotification
    get:
      displayName: deleteNotification
      description: Requires access token obtained via the authenticateUser operation. Available to users with 'notification' permission
      (oas-summary): Deletes a Notification by ID
      responses:
        '200':
          body:
            application/json:
              type: Response
          description: No response was specified
      queryParameters:
        access_token:
          description: Access token required to make the API call
          required: true
          type: string
        id:
          description: ID of the Notification to delete.
          format: int32
          required: true
          type: integer
      (oas-tags):
        - v1
  /previewFile:
    displayName: previewFile
    get:
      displayName: previewFile
      description: Requires access token obtained via authenticateUser operation
      (oas-summary): Returns a resized image of the specified document for support file types
      responses:
        '200':
          body:
            application/json:
              type: PreviewFileResponse
          description: No response was specified
      queryParameters:
        access_token:
          description: Access token required to make the API call
          required: true
          type: string
        path:
          description: Path of the image relative to the user's home directory
          required: true
          type: string
        size:
          description: 'The size of the image: small, medium, large'
          required: true
          type: string
        width:
          description: Overrides sizes. Sets to a specific width
          format: int32
          type: integer
        height:
          description: Overrides sizes. Sets to a specific height
          format: int32
          type: integer
        page:
          description: Page number for the document
          format: int32
          type: integer
      (oas-tags):
        - v1
  /getAccount:
    displayName: getAccount
    get:
      displayName: getAccount
      description: Requires access token obtained via the authenticateUser operation
      (oas-summary): Gets the account object for the currently logged in user
      responses:
        '200':
          body:
            application/json:
              type: AccountResponse
          description: No response was specified
      queryParameters:
        access_token:
          description: Access token required to make the API call
          required: true
          type: string
      (oas-tags):
        - v1
  /getFolders:
    displayName: getFolders
    get:
      displayName: getFolders
      description: Requires access token obtained via the authenticateUser operation
      (oas-summary): Get folders for a specified path
      responses:
        '200':
          body:
            application/json:
              type: ResourcePropertiesResponse
          description: No response was specified
      queryParameters:
        access_token:
          description: Access token required to make the API call
          required: true
          type: string
        path:
          description: The remote file path
          required: true
          type: string
      (oas-tags):
        - v1
  /updateUser:
    displayName: updateUser
    get:
      displayName: updateUser
      description: Requires access token obtained via the authenticateUser operation. Available to users with 'manage users' permission
      (oas-summary): Updates a subaccount user for the current account
      responses:
        '200':
          body:
            application/json:
              type: Response
          description: No response was specified
      queryParameters:
        access_token:
          description: Access token required to make the API call
          required: true
          type: string
        userId:
          description: 'The user ID, must be obtained from getUser method first'
          format: int32
          required: true
          type: integer
        username:
          description: Name of the subaccount user to modify
          type: string
        nickname:
          description: The user's nickname
          type: string
        expiration:
          description: 'The date when use should expire in format: YYYY-MM-DD'
          type: string
        email:
          description: The user's email
          type: string
        destinationFolder:
          description: The user's home folder
          type: string
