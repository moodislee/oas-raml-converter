securitySchemes:
  azure_auth:
    type: OAuth 2.0
    settings:
      authorizationUri: 'https://login.microsoftonline.com/common/oauth2/authorize'
      accessTokenUri: ''
      authorizationGrants:
        - implicit
      scopes:
        - user_impersonation
    description: Azure Active Directory OAuth2 Flow
/subscriptions:
  displayName: subscriptions
  '/{subscriptionId}':
    displayName: '{subscriptionId}'
    /resourceGroups:
      displayName: resourceGroups
      '/{resourceGroupName}':
        displayName: '{resourceGroupName}'
        /providers:
          displayName: providers
          /Microsoft.NotificationHubs:
            displayName: Microsoft.NotificationHubs
            /namespaces:
              displayName: namespaces
              '/{namespaceName}':
                displayName: '{namespaceName}'
                /AuthorizationRules:
                  displayName: AuthorizationRules
                  '/{authorizationRuleName}':
                    displayName: '{authorizationRuleName}'
                    /regenerateKeys:
                      displayName: regenerateKeys
                      post:
                        displayName: Namespaces_RegenerateKeys
                        description: Regenerates the Primary/Secondary Keys to the Namespace Authorization Rule
                        is:
                          - apiVersionParameter
                        body:
                          application/json:
                            type: PolicykeyResource
                            (oas-body-name): parameters
                            description: Parameters supplied to regenerate the Namespace Authorization Rule Key.
                        responses:
                          '200':
                            body:
                              application/json:
                                type: ResourceListKeys
                            description: Regenerate the Primary/Secondary Key and returns the Keys and the connection string
                        (oas-tags):
                          - Namespaces
                        (oas-externalDocs):
                          url: 'http://msdn.microsoft.com/en-us/library/windowsazure/jj873988.aspx'
                    uriParameters:
                      authorizationRuleName:
                        description: Authorization rule name.
                        required: true
                        type: string
                    delete:
                      displayName: Namespaces_DeleteAuthorizationRule
                      description: Deletes a namespace authorization rule
                      is:
                        - apiVersionParameter
                      responses:
                        '200':
                          description: Namespace AuthorizationRule is deleted
                        '204':
                          description: Namespace AuthorizationRule is already deleted
                      (oas-tags):
                        - Namespaces
                    put:
                      displayName: Namespaces_CreateOrUpdateAuthorizationRule
                      description: Creates an authorization rule for a namespace
                      is:
                        - apiVersionParameter
                      body:
                        application/json:
                          type: SharedAccessAuthorizationRuleCreateOrUpdateParameters
                          (oas-body-name): parameters
                          description: The shared access authorization rule.
                      responses:
                        '200':
                          body:
                            application/json:
                              type: SharedAccessAuthorizationRuleResource
                          description: Namespace Authorization Rule is Created/Updated
                      (oas-tags):
                        - Namespaces
                    /listKeys:
                      displayName: listKeys
                      post:
                        displayName: Namespaces_ListKeys
                        description: 'Gets the Primary and Secondary ConnectionStrings to the namespace '
                        is:
                          - apiVersionParameter
                        responses:
                          '200':
                            body:
                              application/json:
                                type: ResourceListKeys
                            description: Gets the Namespace AuthorizationRule's Primary keys and Secondary keys along with the ConnectionStrings
                        (oas-tags):
                          - Namespaces
                        (oas-externalDocs):
                          url: 'http://msdn.microsoft.com/en-us/library/windowsazure/jj873988.aspx'
                    get:
                      displayName: Namespaces_GetAuthorizationRule
                      description: Gets an authorization rule for a namespace by name.
                      is:
                        - apiVersionParameter
                      responses:
                        '200':
                          body:
                            application/json:
                              type: SharedAccessAuthorizationRuleResource
                          description: Get the Namespace AuthorizationRule
                      (oas-tags):
                        - Namespaces
                  get:
                    (oas-x-ms-pageable):
                      nextLinkName: nextLink
                    displayName: Namespaces_ListAuthorizationRules
                    description: Gets the authorization rules for a namespace.
                    is:
                      - apiVersionParameter
                    responses:
                      '200':
                        body:
                          application/json:
                            type: SharedAccessAuthorizationRuleListResult
                        description: Get the Namespace AuthorizationRule
                    (oas-tags):
                      - Namespaces
                /notificationHubs:
                  displayName: notificationHubs
                  '/{notificationHubName}':
                    displayName: '{notificationHubName}'
                    /pnsCredentials:
                      displayName: pnsCredentials
                      post:
                        displayName: NotificationHubs_GetPnsCredentials
                        description: Lists the PNS Credentials associated with a notification hub .
                        is:
                          - apiVersionParameter
                        responses:
                          '200':
                            body:
                              application/json:
                                type: PnsCredentialsResource
                            description: Gets all the NotificationHub PNS credentials
                        (oas-tags):
                          - NotificationHubs
                    uriParameters:
                      notificationHubName:
                        description: The notification hub name.
                        required: true
                        type: string
                    delete:
                      displayName: NotificationHubs_Delete
                      description: Deletes a notification hub associated with a namespace.
                      is:
                        - apiVersionParameter
                      responses:
                        '200':
                          description: Namespace is deleted
                      (oas-tags):
                        - NotificationHubs
                    get:
                      displayName: NotificationHubs_Get
                      description: Lists the notification hubs associated with a namespace.
                      is:
                        - apiVersionParameter
                      responses:
                        '200':
                          body:
                            application/json:
                              type: NotificationHubResource
                          description: Get the NotificationHub
                      (oas-tags):
                        - NotificationHubs
                    put:
                      displayName: NotificationHubs_CreateOrUpdate
                      description: Creates/Update a NotificationHub in a namespace.
                      is:
                        - apiVersionParameter
                      body:
                        application/json:
                          type: NotificationHubCreateOrUpdateParameters
                          (oas-body-name): parameters
                          description: Parameters supplied to the create/update a NotificationHub Resource.
                      responses:
                        '200':
                          body:
                            application/json:
                              type: NotificationHubResource
                          description: NotificationHub is Created/Updated
                        '201':
                          body:
                            application/json:
                              type: NotificationHubResource
                          description: NotificationHub is Created/Updated
                      (oas-tags):
                        - NotificationHubs
                      (oas-externalDocs):
                        url: 'http://msdn.microsoft.com/en-us/library/windowsazure/jj856303.aspx'
                    /AuthorizationRules:
                      displayName: AuthorizationRules
                      get:
                        (oas-x-ms-pageable):
                          nextLinkName: nextLink
                        displayName: NotificationHubs_ListAuthorizationRules
                        description: Gets the authorization rules for a NotificationHub.
                        is:
                          - apiVersionParameter
                        responses:
                          '200':
                            body:
                              application/json:
                                type: SharedAccessAuthorizationRuleListResult
                            description: Get all NotificationHub AuthorizationRules
                        (oas-tags):
                          - NotificationHubs
                      '/{authorizationRuleName}':
                        displayName: '{authorizationRuleName}'
                        uriParameters:
                          authorizationRuleName:
                            description: authorization rule name.
                            required: true
                            type: string
                        delete:
                          displayName: NotificationHubs_DeleteAuthorizationRule
                          description: Deletes a notificationHub authorization rule
                          is:
                            - apiVersionParameter
                          responses:
                            '200':
                              description: NotificationHub AuthorizationRule is deleted
                            '204':
                              description: NotificationHub AuthorizationRule is already deleted
                          (oas-tags):
                            - NotificationHubs
                        /regenerateKeys:
                          displayName: regenerateKeys
                          post:
                            displayName: NotificationHubs_RegenerateKeys
                            description: Regenerates the Primary/Secondary Keys to the NotificationHub Authorization Rule
                            is:
                              - apiVersionParameter
                            body:
                              application/json:
                                type: PolicykeyResource
                                (oas-body-name): parameters
                                description: Parameters supplied to regenerate the NotificationHub Authorization Rule Key.
                            responses:
                              '200':
                                body:
                                  application/json:
                                    type: ResourceListKeys
                                description: Regenerate the Primary/Secondary Key and returns the Keys and the connection string
                            (oas-tags):
                              - NotificationHubs
                            (oas-externalDocs):
                              url: 'http://msdn.microsoft.com/en-us/library/windowsazure/jj873988.aspx'
                        /listKeys:
                          displayName: listKeys
                          post:
                            displayName: NotificationHubs_ListKeys
                            description: 'Gets the Primary and Secondary ConnectionStrings to the NotificationHub '
                            is:
                              - apiVersionParameter
                            responses:
                              '200':
                                body:
                                  application/json:
                                    type: ResourceListKeys
                                description: Gets the Namespace AuthorizationRule's Primary keys and Secondary keys along with the ConnectionStrings
                            (oas-tags):
                              - NotificationHubs
                            (oas-externalDocs):
                              url: 'http://msdn.microsoft.com/en-us/library/windowsazure/jj873988.aspx'
                        put:
                          displayName: NotificationHubs_CreateOrUpdateAuthorizationRule
                          description: Creates/Updates an authorization rule for a NotificationHub
                          is:
                            - apiVersionParameter
                          body:
                            application/json:
                              type: SharedAccessAuthorizationRuleCreateOrUpdateParameters
                              (oas-body-name): parameters
                              description: The shared access authorization rule.
                          responses:
                            '200':
                              body:
                                application/json:
                                  type: SharedAccessAuthorizationRuleResource
                              description: NotificationHub AuthorizationRule is Created/Updated
                          (oas-tags):
                            - NotificationHubs
                        get:
                          displayName: NotificationHubs_GetAuthorizationRule
                          description: Gets an authorization rule for a NotificationHub by name.
                          is:
                            - apiVersionParameter
                          responses:
                            '200':
                              body:
                                application/json:
                                  type: SharedAccessAuthorizationRuleResource
                              description: Get NotificationHub AuthorizationRule
                          (oas-tags):
                            - NotificationHubs
                  get:
                    (oas-x-ms-pageable):
                      nextLinkName: nextLink
                    displayName: NotificationHubs_List
                    description: Lists the notification hubs associated with a namespace.
                    is:
                      - apiVersionParameter
                    responses:
                      '200':
                        body:
                          application/json:
                            type: NotificationHubListResult
                        description: Get all NotificationHubs within a Namespace
                    (oas-tags):
                      - NotificationHubs
                /checkNotificationHubAvailability:
                  displayName: checkNotificationHubAvailability
                  post:
                    displayName: NotificationHubs_CheckAvailability
                    description: Checks the availability of the given notificationHub in a namespace.
                    is:
                      - apiVersionParameter
                    body:
                      application/json:
                        type: CheckAvailabilityParameters
                        (oas-body-name): parameters
                        description: The notificationHub name.
                    responses:
                      '200':
                        body:
                          application/json:
                            type: CheckAvailabilityResult
                        description: Checks if the NotificationHub name is a valid availabile name
                    (oas-tags):
                      - NotificationHubs
                    (oas-externalDocs):
                      url: 'http://msdn.microsoft.com/en-us/library/windowsazure/jj870968.aspx'
                uriParameters:
                  namespaceName:
                    description: The namespace name.
                    required: true
                    type: string
                get:
                  displayName: Namespaces_Get
                  description: Returns the description for the specified namespace.
                  is:
                    - apiVersionParameter
                  responses:
                    '200':
                      body:
                        application/json:
                          type: NamespaceResource
                      description: Get Namespace
                  (oas-tags):
                    - Namespaces
                  (oas-externalDocs):
                    url: 'http://msdn.microsoft.com/library/azure/dn140232.aspx'
                put:
                  displayName: Namespaces_CreateOrUpdate
                  description: 'Creates/Updates a service namespace. Once created, this namespace''s resource manifest is immutable. This operation is idempotent.'
                  is:
                    - apiVersionParameter
                  body:
                    application/json:
                      type: NamespaceCreateOrUpdateParameters
                      (oas-body-name): parameters
                      description: Parameters supplied to create a Namespace Resource.
                  responses:
                    '200':
                      body:
                        application/json:
                          type: NamespaceResource
                      description: Namespace is Create/Update
                    '201':
                      body:
                        application/json:
                          type: NamespaceResource
                      description: Namespace is Create/Update
                  (oas-tags):
                    - Namespaces
                  (oas-externalDocs):
                    url: 'http://msdn.microsoft.com/en-us/library/windowsazure/jj856303.aspx'
                patch:
                  displayName: Namespaces_Patch
                  description: Patches the existing namespace
                  is:
                    - apiVersionParameter
                  body:
                    application/json:
                      type: NamespacePatchParameters
                      (oas-body-name): parameters
                      description: Parameters supplied to patch a Namespace Resource.
                  responses:
                    '200':
                      body:
                        application/json:
                          type: NamespaceResource
                      description: Namespace is Updated
                  (oas-tags):
                    - Namespaces
                delete:
                  (oas-x-ms-long-running-operation): true
                  displayName: Namespaces_Delete
                  description: Deletes an existing namespace. This operation also removes all associated notificationHubs under the namespace.
                  is:
                    - apiVersionParameter
                  responses:
                    '200':
                      description: Namespace is deleted
                    '202':
                      description: Namespace delete request has been accepted
                    '204':
                      description: Namespace is already Deleted
                  (oas-tags):
                    - Namespaces
                  (oas-externalDocs):
                    url: 'http://msdn.microsoft.com/en-us/library/windowsazure/jj856296.aspx'
              get:
                (oas-x-ms-pageable):
                  nextLinkName: nextLink
                displayName: Namespaces_List
                description: Lists the available namespaces within a resourceGroup.
                is:
                  - apiVersionParameter
                responses:
                  '200':
                    body:
                      application/json:
                        type: NamespaceListResult
                    description: Lists all Namespaces with the ResourceGroup
                (oas-tags):
                  - Namespaces
                (oas-externalDocs):
                  url: 'http://msdn.microsoft.com/en-us/library/azure/hh780759.aspx'
    /providers:
      displayName: providers
      /Microsoft.NotificationHubs:
        displayName: Microsoft.NotificationHubs
        /checkNamespaceAvailability:
          displayName: checkNamespaceAvailability
          post:
            displayName: Namespaces_CheckAvailability
            description: Checks the availability of the given service namespace across all Azure subscriptions. This is useful because the domain name is created based on the service namespace name.
            is:
              - apiVersionParameter
            body:
              application/json:
                type: CheckAvailabilityParameters
                (oas-body-name): parameters
                description: The namespace name.
            responses:
              '200':
                body:
                  application/json:
                    type: CheckAvailabilityResult
                description: Checks if the Namespace name is a valid availabile name
            (oas-tags):
              - Namespaces
            (oas-externalDocs):
              url: 'http://msdn.microsoft.com/en-us/library/windowsazure/jj870968.aspx'
        /namespaces:
          displayName: namespaces
          get:
            (oas-x-ms-pageable):
              nextLinkName: nextLink
            displayName: Namespaces_ListAll
            description: Lists all the available namespaces within the subscription irrespective of the resourceGroups.
            is:
              - apiVersionParameter
            responses:
              '200':
                body:
                  application/json:
                    type: NamespaceListResult
                description: ' Gets all the Namespaces within the subscription irrespective of the ResourceGroup'
            (oas-tags):
              - Namespaces
            (oas-externalDocs):
              url: 'http://msdn.microsoft.com/en-us/library/azure/hh780759.aspx'
types:
