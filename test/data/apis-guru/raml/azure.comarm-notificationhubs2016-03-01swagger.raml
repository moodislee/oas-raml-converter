      clientId:
        description: The client identifier.
        type: string
        required: false
      clientSecret:
        description: The credential secret access key.
        type: string
        required: false
  ApnsCredential:
    description: Description of a NotificationHub ApnsCredential.
    properties:
      properties:
        description: Properties of NotificationHub ApnsCredential.
        type: ApnsCredentialProperties
        required: false
        (oas-x-ms-client-flatten): true
  ApnsCredentialProperties:
    description: Description of a NotificationHub ApnsCredential.
