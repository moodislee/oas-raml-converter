        description: Resource location
        type: string
      name:
        description: Resource name
        type: string
      sku:
        description: The sku of the created namespace
        type: Sku
        required: false
      tags:
        description: Resource tags
        type: object
        required: false
        properties:
          //:
            type: string
      type:
        description: Resource type
        readOnly: true
        type: string
        facets:
          readOnly?: boolean
        required: false
  CheckAvailabilityResult:
    type: Resource
  GcmCredential:
