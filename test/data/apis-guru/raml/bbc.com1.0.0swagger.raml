        description: filter for specified linear service
        items:
          type: string
        required: false
        type: array
        displayName: filter for specified linear service
        facets:
          collectionFormat: string
      start_from:
        description: Return services that start on or later than the specified datetime
        format: rfc3339
        required: false
        type: datetime
        displayName: Return services that start on or later than the specified datetime
      start_to:
        description: Return services that start earlier than the specified datetime
        format: rfc3339
        required: false
        type: datetime
        displayName: Return services that start earlier than the specified datetime
    (oas-tags):
      - feeds
