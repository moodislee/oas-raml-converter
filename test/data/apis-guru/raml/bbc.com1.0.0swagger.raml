    (oas-responses-default):
      body:
        application/json:
          type: ErrorModel
      description: Unexpected error
    responses:
      '200':
        body:
          application/json:
            type: nitro
        description: Nitro response
    queryParameters:
      end_from:
        description: Return services that end on or later than the specified datetime
        format: rfc3339
        required: false
        type: datetime
        displayName: Return services that end on or later than the specified datetime
      end_to:
        description: filter for subset of broadcasts that end on or earlier than the specified datetime
        format: rfc3339
        required: false
        type: datetime
        displayName: filter for subset of broadcasts that end on or earlier than the specified datetime
      mid:
        collectionFormat: multi
