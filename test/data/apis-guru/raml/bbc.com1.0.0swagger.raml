        required: false
        type: array
        displayName: filter for services by masterbrand MID
        facets:
          collectionFormat: string
      page:
        default: 1
        description: which page of results to return
        minimum: 1
        required: false
        type: integer
        displayName: which page of results to return
      page_size:
        default: 10
        description: number of results in each page
        maximum: 300
        required: false
        type: integer
        displayName: number of results in each page
      partner_id:
        collectionFormat: multi
        description: filter for services by partner ID
        items:
          minLength: 8
          pattern: '^([0-9,a-d,f-h,j-n,p-t,v-z]){8,}$'
          type: string
        required: false
        type: array
        displayName: filter for services by partner ID
        facets:
          collectionFormat: string
      partner_pid:
        collectionFormat: multi
        description: filter for services by partner PID
        items:
          minLength: 8
          pattern: '^([0-9,a-d,f-h,j-n,p-t,v-z]){8,}$'
          type: string
        required: false
        type: array
        displayName: filter for services by partner PID
        facets:
          collectionFormat: string
      q:
        description: filter for subset of services matching supplied keyword/phrase (boolean operators permitted)
        required: false
        type: string
        displayName: filter for subset of services matching supplied keyword/phrase (boolean operators permitted)
      service_type:
        collectionFormat: multi
        description: filter for specified type of linear services
        items:
          type: string
        required: false
        type: array
        displayName: filter for specified type of linear services
        facets:
          collectionFormat: string
      sid:
