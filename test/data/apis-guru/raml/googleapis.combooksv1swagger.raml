#%RAML 1.0
title: Books
version: v1
baseUri: 'https://www.googleapis.com/books/v1'
protocols:
  - HTTPS
description: Searches for books and manages your Google Books library.
(oas-tags-definition):
  - name: bookshelves
  - name: cloudloading
  - name: dictionary
  - name: layers
  - name: myconfig
  - name: mylibrary
  - name: notification
  - name: onboarding
  - name: personalizedstream
  - name: promooffer
  - name: series
  - name: volumes
annotationTypes:
  oas-tags-definition:
    type: array
    items:
      properties:
        name: string
        description?: string
        externalDocs?:
          properties:
            url: string
            description?: string
    allowedTargets: API
  oas-x-apiClientRegistration: any
  oas-x-logo: any
  oas-x-origin: any
  oas-x-preferred: any
  oas-x-providerName: any
  oas-x-serviceName: any
  oas-tags:
    type: 'string[]'
    allowedTargets: Method
  oas-externalDocs:
    properties:
      description?: string
      url: string
    allowedTargets:
      - API
      - Method
      - TypeDeclaration
  oas-info:
    properties:
      termsOfService?: string
      contact?:
        properties:
          name?: string
          url?: string
          email?: string
      license?:
        properties:
          name?: string
          url?: string
    allowedTargets: API
  oas-body-name:
    type: string
    allowedTargets: TypeDeclaration
(oas-info):
  (oas-x-apiClientRegistration):
    url: 'https://console.developers.google.com'
  (oas-x-logo):
    url: 'https://api.apis.guru/v2/cache/logo/https_www.google.com_intl_en_images_logos_books_logo_lg.png'
  (oas-x-origin):
    format: google
    url: 'https://www.googleapis.com/discovery/v1/apis/books/v1/rest'
    version: v1
  (oas-x-preferred): true
  (oas-x-providerName): googleapis.com
  (oas-x-serviceName): books
  contact:
    name: Google
    url: 'https://google.com'
(oas-externalDocs):
  url: 'https://developers.google.com/books/docs/v1/getting_started'
securitySchemes:
  Oauth2:
    type: OAuth 2.0
    settings:
      authorizationUri: 'https://accounts.google.com/o/oauth2/auth'
      accessTokenUri: ''
      authorizationGrants:
        - implicit
      scopes:
        - 'https://www.googleapis.com/auth/books'
    description: Oauth 2.0 authentication
/onboarding:
  displayName: onboarding
  /listCategories:
    displayName: listCategories
    get:
      displayName: books.onboarding.listCategories
      description: List categories for onboarding experience.
      responses:
        '200':
          body:
            application/json:
              type: Category
          description: Successful response
      queryParameters:
        locale:
          description: ISO-639-1 language and ISO-3166-1 country code. Default is en-US if unset.
          type: string
      securedBy:
        - Oauth2:
            scopes:
              - 'https://www.googleapis.com/auth/books'
      (oas-tags):
        - onboarding
    is:
      - alt
      - fields
      - key
      - oauthToken
      - prettyPrint
      - quotaUser
      - userIp
  /listCategoryVolumes:
    displayName: listCategoryVolumes
    get:
      displayName: books.onboarding.listCategoryVolumes
      description: List available volumes under categories for onboarding experience.
      responses:
        '200':
          body:
            application/json:
              type: Volume2
          description: Successful response
      queryParameters:
        categoryId:
          collectionFormat: multi
          description: List of category ids requested.
          items:
            type: string
          type: array
          facets:
            collectionFormat: string
        locale:
          description: ISO-639-1 language and ISO-3166-1 country code. Default is en-US if unset.
          type: string
        maxAllowedMaturityRating:
          description: The maximum allowed maturity rating of returned volumes. Books with a higher maturity rating are filtered out.
          enum:
            - mature
            - not-mature
          type: string
        pageSize:
          description: Number of maximum results per page to be included in the response.
          type: integer
        pageToken:
          description: The value of the nextToken from the previous page.
          type: string
      securedBy:
        - Oauth2:
            scopes:
              - 'https://www.googleapis.com/auth/books'
      (oas-tags):
        - onboarding
    is:
      - alt
      - fields
      - key
      - oauthToken
      - prettyPrint
      - quotaUser
      - userIp
/volumes:
  displayName: volumes
  '/{volumeId}':
    displayName: '{volumeId}'
    /layersummary:
      displayName: layersummary
      '/{summaryId}':
        displayName: '{summaryId}'
        uriParameters:
          summaryId:
            description: The ID for the layer to get the summary for.
            required: true
            type: string
        get:
          displayName: books.layers.get
          description: Gets the layer summary for a volume.
          responses:
            '200':
              body:
                application/json:
                  type: Layersummary
              description: Successful response
          queryParameters:
            contentVersion:
              description: The content version for the requested volume.
              type: string
            source:
              description: String to identify the originator of this request.
              type: string
          securedBy:
            - Oauth2:
                scopes:
                  - 'https://www.googleapis.com/auth/books'
          (oas-tags):
            - layers
        is:
          - alt
          - fields
          - key
          - oauthToken
          - prettyPrint
          - quotaUser
          - userIp
      get:
        displayName: books.layers.list
        description: List the layer summaries for a volume.
        responses:
          '200':
            body:
              application/json:
                type: Layersummaries
            description: Successful response
        queryParameters:
          contentVersion:
            description: The content version for the requested volume.
            type: string
          maxResults:
            description: Maximum number of results to return
            maximum: 200
            minimum: 0
            type: integer
          pageToken:
            description: The value of the nextToken from the previous page.
            type: string
          source:
            description: String to identify the originator of this request.
            type: string
        securedBy:
          - Oauth2:
              scopes:
                - 'https://www.googleapis.com/auth/books'
        (oas-tags):
          - layers
      is:
        - alt
        - fields
        - key
        - oauthToken
        - prettyPrint
        - quotaUser
        - userIp
    uriParameters:
      volumeId:
        description: ID of volume to retrieve.
        required: true
        type: string
    get:
      displayName: books.volumes.get
      description: Gets volume information for a single volume.
      responses:
        '200':
          body:
            application/json:
              type: Volume
          description: Successful response
      queryParameters:
        country:
          description: ISO-3166-1 code to override the IP-based location.
          type: string
        includeNonComicsSeries:
          description: Set to true to include non-comics series. Defaults to false.
          type: boolean
        partner:
          description: Brand results for partner ID.
          type: string
        projection:
          description: Restrict information returned to a set of selected fields.
          enum:
            - full
            - lite
          type: string
        source:
          description: String to identify the originator of this request.
          type: string
        user_library_consistent_read:
          type: boolean
      securedBy:
        - Oauth2:
            scopes:
              - 'https://www.googleapis.com/auth/books'
      (oas-tags):
        - volumes
    is:
      - alt
      - fields
      - key
      - oauthToken
      - prettyPrint
      - quotaUser
      - userIp
    /associated:
      displayName: associated
      get:
        displayName: books.volumes.associated.list
        description: Return a list of associated books.
        responses:
          '200':
            body:
              application/json:
                type: Volumes
            description: Successful response
        queryParameters:
          association:
            description: Association type.
            enum:
              - end-of-sample
              - end-of-volume
              - related-for-play
            type: string
          locale:
            description: 'ISO-639-1 language and ISO-3166-1 country code. Ex: ''en_US''. Used for generating recommendations.'
            type: string
          maxAllowedMaturityRating:
            description: The maximum allowed maturity rating of returned recommendations. Books with a higher maturity rating are filtered out.
            enum:
              - mature
              - not-mature
            type: string
          source:
            description: String to identify the originator of this request.
            type: string
        securedBy:
          - Oauth2:
              scopes:
                - 'https://www.googleapis.com/auth/books'
        (oas-tags):
          - volumes
      is:
        - alt
        - fields
        - key
        - oauthToken
        - prettyPrint
        - quotaUser
        - userIp
    /layers:
      displayName: layers
      '/{layerId}':
        displayName: '{layerId}'
        uriParameters:
          layerId:
            description: The ID for the layer to get the annotations.
            required: true
            type: string
        get:
          displayName: books.layers.volumeAnnotations.list
          description: Gets the volume annotations for a volume and layer.
          responses:
            '200':
              body:
                application/json:
                  type: Volumeannotations
              description: Successful response
          queryParameters:
            contentVersion:
              description: The content version for the requested volume.
              required: true
              type: string
            endOffset:
              description: The end offset to end retrieving data from.
              type: string
            endPosition:
              description: The end position to end retrieving data from.
              type: string
            locale:
              description: 'The locale information for the data. ISO-639-1 language and ISO-3166-1 country code. Ex: ''en_US''.'
              type: string
            maxResults:
              description: Maximum number of results to return
              maximum: 200
              minimum: 0
              type: integer
            pageToken:
              description: The value of the nextToken from the previous page.
              type: string
            showDeleted:
              description: Set to true to return deleted annotations. updatedMin must be in the request to use this. Defaults to false.
              type: boolean
            source:
              description: String to identify the originator of this request.
              type: string
            startOffset:
              description: The start offset to start retrieving data from.
              type: string
            startPosition:
              description: The start position to start retrieving data from.
              type: string
            updatedMax:
              description: RFC 3339 timestamp to restrict to items updated prior to this timestamp (exclusive).
              type: string
            updatedMin:
              description: RFC 3339 timestamp to restrict to items updated since this timestamp (inclusive).
              type: string
            volumeAnnotationsVersion:
              description: The version of the volume annotations that you are requesting.
              type: string
          securedBy:
            - Oauth2:
                scopes:
                  - 'https://www.googleapis.com/auth/books'
          (oas-tags):
            - layers
        is:
          - alt
          - fields
          - key
          - oauthToken
          - prettyPrint
          - quotaUser
          - userIp
        /data:
          displayName: data
          '/{annotationDataId}':
            displayName: '{annotationDataId}'
            uriParameters:
              annotationDataId:
                description: The ID of the annotation data to retrieve.
                required: true
                type: string
            get:
              displayName: books.layers.annotationData.get
              description: Gets the annotation data.
              responses:
                '200':
                  body:
                    application/json:
                      type: Annotationdata
                  description: Successful response
              queryParameters:
                contentVersion:
                  description: The content version for the volume you are trying to retrieve.
                  required: true
                  type: string
                allowWebDefinitions:
                  description: For the dictionary layer. Whether or not to allow web definitions.
                  type: boolean
                h:
                  description: The requested pixel height for any images. If height is provided width must also be provided.
                  type: integer
                locale:
                  description: 'The locale information for the data. ISO-639-1 language and ISO-3166-1 country code. Ex: ''en_US''.'
                  type: string
                scale:
                  description: The requested scale for the image.
                  minimum: 0
                  type: integer
                source:
                  description: String to identify the originator of this request.
                  type: string
                w:
                  description: The requested pixel width for any images. If width is provided height must also be provided.
                  type: integer
              securedBy:
                - Oauth2:
                    scopes:
                      - 'https://www.googleapis.com/auth/books'
              (oas-tags):
                - layers
            is:
              - alt
              - fields
              - key
              - oauthToken
              - prettyPrint
              - quotaUser
              - userIp
          get:
            displayName: books.layers.annotationData.list
            description: Gets the annotation data for a volume and layer.
            responses:
              '200':
                body:
                  application/json:
                    type: Annotationsdata
                description: Successful response
            queryParameters:
              contentVersion:
                description: The content version for the requested volume.
                required: true
                type: string
              annotationDataId:
                collectionFormat: multi
                description: The list of Annotation Data Ids to retrieve. Pagination is ignored if this is set.
                items:
                  type: string
                type: array
                facets:
                  collectionFormat: string
              h:
                description: The requested pixel height for any images. If height is provided width must also be provided.
                type: integer
              locale:
                description: 'The locale information for the data. ISO-639-1 language and ISO-3166-1 country code. Ex: ''en_US''.'
                type: string
              maxResults:
                description: Maximum number of results to return
                maximum: 200
                minimum: 0
                type: integer
              pageToken:
                description: The value of the nextToken from the previous page.
                type: string
              scale:
                description: The requested scale for the image.
                minimum: 0
                type: integer
              source:
                description: String to identify the originator of this request.
                type: string
              updatedMax:
                description: RFC 3339 timestamp to restrict to items updated prior to this timestamp (exclusive).
                type: string
              updatedMin:
                description: RFC 3339 timestamp to restrict to items updated since this timestamp (inclusive).
                type: string
              w:
                description: The requested pixel width for any images. If width is provided height must also be provided.
                type: integer
            securedBy:
              - Oauth2:
                  scopes:
                    - 'https://www.googleapis.com/auth/books'
            (oas-tags):
              - layers
          is:
            - alt
            - fields
            - key
            - oauthToken
            - prettyPrint
            - quotaUser
            - userIp
        /annotations:
          displayName: annotations
          '/{annotationId}':
            displayName: '{annotationId}'
            uriParameters:
              annotationId:
                description: The ID of the volume annotation to retrieve.
                required: true
                type: string
            get:
              displayName: books.layers.volumeAnnotations.get
              description: Gets the volume annotation.
              responses:
                '200':
                  body:
                    application/json:
                      type: Volumeannotation
                  description: Successful response
              queryParameters:
                locale:
                  description: 'The locale information for the data. ISO-639-1 language and ISO-3166-1 country code. Ex: ''en_US''.'
                  type: string
                source:
                  description: String to identify the originator of this request.
                  type: string
              securedBy:
                - Oauth2:
                    scopes:
                      - 'https://www.googleapis.com/auth/books'
              (oas-tags):
                - layers
            is:
              - alt
              - fields
              - key
              - oauthToken
              - prettyPrint
              - quotaUser
              - userIp
  get:
    displayName: books.volumes.list
    description: Performs a book search.
    responses:
      '200':
        body:
          application/json:
            type: Volumes
        description: Successful response
    queryParameters:
      q:
        description: Full-text search query string.
        required: true
        type: string
      download:
        description: Restrict to volumes by download availability.
        enum:
          - epub
        type: string
      filter:
        description: Filter search results.
        enum:
          - ebooks
          - free-ebooks
          - full
          - paid-ebooks
          - partial
        type: string
      langRestrict:
        description: Restrict results to books with this language code.
        type: string
      libraryRestrict:
        description: Restrict search to this user's library.
        enum:
          - my-library
          - no-restrict
        type: string
      maxAllowedMaturityRating:
        description: The maximum allowed maturity rating of returned recommendations. Books with a higher maturity rating are filtered out.
        enum:
          - mature
          - not-mature
        type: string
      maxResults:
        description: Maximum number of results to return.
        maximum: 40
        minimum: 0
        type: integer
      orderBy:
        description: Sort search results.
        enum:
          - newest
          - relevance
        type: string
      partner:
        description: Restrict and brand results for partner ID.
        type: string
      printType:
        description: Restrict to books or magazines.
        enum:
          - all
          - books
          - magazines
        type: string
      projection:
        description: Restrict information returned to a set of selected fields.
        enum:
          - full
          - lite
        type: string
      showPreorders:
        description: Set to true to show books available for preorder. Defaults to false.
        type: boolean
      source:
        description: String to identify the originator of this request.
        type: string
      startIndex:
        description: Index of the first result to return (starts at 0)
        minimum: 0
        type: integer
    securedBy:
      - Oauth2:
          scopes:
            - 'https://www.googleapis.com/auth/books'
    (oas-tags):
      - volumes
  is:
    - alt
    - fields
    - key
    - oauthToken
    - prettyPrint
    - quotaUser
    - userIp
  /mybooks:
    displayName: mybooks
    get:
      displayName: books.volumes.mybooks.list
      description: Return a list of books in My Library.
      responses:
        '200':
          body:
            application/json:
              type: Volumes
          description: Successful response
      queryParameters:
        acquireMethod:
          collectionFormat: multi
          description: How the book was acquired
          items:
            enum:
              - FAMILY_SHARED
              - PREORDERED
              - PREVIOUSLY_RENTED
              - PUBLIC_DOMAIN
              - PURCHASED
              - RENTED
              - SAMPLE
              - UPLOADED
            type: string
          type: array
          facets:
            collectionFormat: string
        country:
          description: ISO-3166-1 code to override the IP-based location.
          type: string
        locale:
          description: 'ISO-639-1 language and ISO-3166-1 country code. Ex:''en_US''. Used for generating recommendations.'
          type: string
        maxResults:
          description: Maximum number of results to return.
          maximum: 100
          minimum: 0
          type: integer
        processingState:
          collectionFormat: multi
          description: The processing state of the user uploaded volumes to be returned. Applicable only if the UPLOADED is specified in the acquireMethod.
          items:
            enum:
              - COMPLETED_FAILED
              - COMPLETED_SUCCESS
              - RUNNING
            type: string
          type: array
          facets:
            collectionFormat: string
        source:
          description: String to identify the originator of this request.
          type: string
        startIndex:
          description: Index of the first result to return (starts at 0)
          minimum: 0
          type: integer
      securedBy:
        - Oauth2:
            scopes:
              - 'https://www.googleapis.com/auth/books'
      (oas-tags):
        - volumes
    is:
      - alt
      - fields
      - key
      - oauthToken
      - prettyPrint
      - quotaUser
      - userIp
  /recommended:
    displayName: recommended
    get:
      displayName: books.volumes.recommended.list
      description: Return a list of recommended books for the current user.
      responses:
        '200':
          body:
            application/json:
              type: Volumes
          description: Successful response
      queryParameters:
        locale:
          description: 'ISO-639-1 language and ISO-3166-1 country code. Ex: ''en_US''. Used for generating recommendations.'
          type: string
        maxAllowedMaturityRating:
          description: The maximum allowed maturity rating of returned recommendations. Books with a higher maturity rating are filtered out.
          enum:
            - mature
            - not-mature
          type: string
        source:
          description: String to identify the originator of this request.
          type: string
      securedBy:
        - Oauth2:
            scopes:
              - 'https://www.googleapis.com/auth/books'
      (oas-tags):
        - volumes
    is:
      - alt
      - fields
      - key
      - oauthToken
      - prettyPrint
      - quotaUser
      - userIp
    /rate:
      displayName: rate
      post:
        displayName: books.volumes.recommended.rate
        description: Rate a recommended book for the current user.
        responses:
          '200':
            body:
              application/json:
                type: BooksVolumesRecommendedRateResponse
            description: Successful response
        queryParameters:
          rating:
            description: Rating to be given to the volume.
            enum:
              - HAVE_IT
              - NOT_INTERESTED
            required: true
            type: string
          volumeId:
            description: ID of the source volume.
            required: true
            type: string
          locale:
            description: 'ISO-639-1 language and ISO-3166-1 country code. Ex: ''en_US''. Used for generating recommendations.'
            type: string
          source:
            description: String to identify the originator of this request.
            type: string
        securedBy:
          - Oauth2:
              scopes:
                - 'https://www.googleapis.com/auth/books'
        (oas-tags):
          - volumes
      is:
        - alt
        - fields
        - key
        - oauthToken
        - prettyPrint
        - quotaUser
        - userIp
  /useruploaded:
    displayName: useruploaded
    get:
      displayName: books.volumes.useruploaded.list
      description: Return a list of books uploaded by the current user.
      responses:
        '200':
          body:
            application/json:
              type: Volumes
          description: Successful response
      queryParameters:
        locale:
          description: 'ISO-639-1 language and ISO-3166-1 country code. Ex: ''en_US''. Used for generating recommendations.'
          type: string
        maxResults:
          description: Maximum number of results to return.
          maximum: 40
          minimum: 0
          type: integer
        processingState:
          collectionFormat: multi
          description: The processing state of the user uploaded volumes to be returned.
          items:
            enum:
              - COMPLETED_FAILED
              - COMPLETED_SUCCESS
              - RUNNING
            type: string
          type: array
          facets:
            collectionFormat: string
        source:
          description: String to identify the originator of this request.
          type: string
        startIndex:
          description: Index of the first result to return (starts at 0)
          minimum: 0
          type: integer
        volumeId:
          collectionFormat: multi
          description: The ids of the volumes to be returned. If not specified all that match the processingState are returned.
          items:
            type: string
          type: array
          facets:
            collectionFormat: string
      securedBy:
        - Oauth2:
            scopes:
              - 'https://www.googleapis.com/auth/books'
      (oas-tags):
        - volumes
    is:
      - alt
      - fields
      - key
      - oauthToken
      - prettyPrint
      - quotaUser
      - userIp
/cloudloading:
  displayName: cloudloading
  /addBook:
    displayName: addBook
    post:
      displayName: books.cloudloading.addBook
      responses:
        '200':
          body:
            application/json:
              type: BooksCloudloadingResource
          description: Successful response
      queryParameters:
        drive_document_id:
          description: A drive document id. The upload_client_token must not be set.
          type: string
        mime_type:
          description: The document MIME type. It can be set only if the drive_document_id is set.
          type: string
        name:
          description: The document name. It can be set only if the drive_document_id is set.
          type: string
        upload_client_token:
          type: string
      securedBy:
        - Oauth2:
            scopes:
              - 'https://www.googleapis.com/auth/books'
      (oas-tags):
        - cloudloading
    is:
      - alt
      - fields
      - key
      - oauthToken
      - prettyPrint
      - quotaUser
      - userIp
  /updateBook:
    displayName: updateBook
    post:
      displayName: books.cloudloading.updateBook
      body:
        application/json:
          type: BooksCloudloadingResource
          (oas-body-name): body
      responses:
        '200':
          body:
            application/json:
              type: BooksCloudloadingResource
          description: Successful response
      securedBy:
        - Oauth2:
            scopes:
              - 'https://www.googleapis.com/auth/books'
      (oas-tags):
        - cloudloading
    is:
      - alt
      - fields
      - key
      - oauthToken
      - prettyPrint
      - quotaUser
      - userIp
  /deleteBook:
    displayName: deleteBook
    post:
      displayName: books.cloudloading.deleteBook
      description: Remove the book and its contents
      responses:
        '200':
          description: Successful response
      queryParameters:
        volumeId:
          description: The id of the book to be removed.
          required: true
          type: string
      securedBy:
        - Oauth2:
            scopes:
              - 'https://www.googleapis.com/auth/books'
      (oas-tags):
        - cloudloading
    is:
      - alt
      - fields
      - key
      - oauthToken
      - prettyPrint
      - quotaUser
      - userIp
/personalizedstream:
  displayName: personalizedstream
  /get:
    displayName: get
    get:
      displayName: books.personalizedstream.get
      description: Returns a stream of personalized book clusters
      responses:
        '200':
          body:
