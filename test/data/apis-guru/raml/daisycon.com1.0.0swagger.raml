                  description: No response was specified
                '400':
                  description: Disapproved media subscriptions cannot be unsubscribed
                '404':
                  description: No subscription found
                '405':
                  description: Invalid input
              (oas-tags):
                - programs
            post:
              displayName: POST_publishers-publisher_id-programs-program_id-subscriptions-media_id
