                type: array
            description: No response was specified
          '204':
            description: No content
          '404':
            description: Publisher not found
        queryParameters:
          page:
            default: 1
            description: Paginator page
            format: int64
            required: false
            type: integer
          per_page:
            default: 100
            description: Paginator results per page (max 1000)
            format: int64
            required: false
            type: integer
          start:
            description: 'Start date (YYYY-MM-DD HH:II:SS)'
            format: rfc3339
            required: false
            type: datetime
          end:
            description: 'End date (YYYY-MM-DD HH:II:SS)'
            format: rfc3339
            required: false
            type: datetime
          date_click_start:
            description: 'Click date (YYYY-MM-DD HH:II:SS)'
            items:
              format: rfc3339
              type: datetime
            required: false
            type: array
          date_click_end:
            description: 'Click date end (YYYY-MM-DD HH:II:SS)'
            items:
              format: rfc3339
              type: datetime
            required: false
            type: array
          date_approval_start:
            description: 'Approval date (YYYY-MM-DD HH:II:SS)'
            items:
              format: rfc3339
              type: datetime
            required: false
            type: array
          date_approval_end:
            description: 'Approval date end (YYYY-MM-DD HH:II:SS)'
            items:
              format: rfc3339
              type: datetime
            required: false
            type: array
          date_modified_start:
            description: 'Last modification date (YYYY-MM-DD HH:II:SS)'
            items:
              format: rfc3339
              type: datetime
            required: false
            type: array
          date_modified_end:
            description: 'Last modification date end (YYYY-MM-DD HH:II:SS)'
            items:
              format: rfc3339
              type: datetime
            required: false
            type: array
          affiliatemarketing_id:
            description: Affiliatemarketing IDs
            items:
              type: string
            required: false
            type: array
          advertiser_id:
            description: Advertiser IDs
            items:
              format: int64
              type: integer
            required: false
            type: array
          media_id:
            description: Media IDs
            items:
              format: int64
              type: integer
            required: false
            type: array
          program_id:
            description: Program IDs
            items:
              format: int64
              type: integer
            required: false
            type: array
          ad_id:
            description: Ad IDs
            items:
              format: int64
              type: integer
            required: false
            type: array
          subid:
            description: Subids
            items:
              type: string
            required: false
            type: array
          description:
            description: Transaction description
            items:
              type: string
            required: false
            type: array
          status:
            description: 'Status (''approved'', ''disapproved'', ''open'')'
            items:
              enum:
                - approved
                - disapproved
                - open
              type: string
            required: false
            type: array
