#%RAML 1.0
title: Daisycon
version: 1.0.0
baseUri: 'https://services.daisycon.com/'
protocols:
  - HTTPS
description: 'The Daisycon API allows you to retrieve statistics and link materials, update transactions, subscribe to affiliate program, and a whole lot more!'
(oas-tags-definition):
  - description: Publisher operations
    name: publishers
  - name: account
  - name: clicks
  - name: financial
  - name: material
  - name: media
  - name: news
  - name: productfeeds
  - name: programs
  - name: statistics
  - name: transactions
annotationTypes:
  oas-tags-definition:
    type: array
    items:
      properties:
        name: string
        description?: string
        externalDocs?:
          properties:
            url: string
            description?: string
    allowedTargets: API
  oas-x-logo: any
  oas-x-origin: any
  oas-x-preferred: any
  oas-x-providerName: any
  oas-tags:
    type: 'string[]'
    allowedTargets: Method
  oas-summary:
    type: string
    allowedTargets: Method
  oas-info:
    properties:
      termsOfService?: string
      contact?:
        properties:
          name?: string
          url?: string
          email?: string
      license?:
        properties:
          name?: string
          url?: string
    allowedTargets: API
(oas-info):
  (oas-x-logo):
    backgroundColor: '#4D4D4D'
    url: 'https://api.apis.guru/v2/cache/logo/https_www.daisycon.com_shared_daisyconwebsite_img_layout_logo_small_white.png'
  (oas-x-origin):
    format: swagger
    url: 'https://services.daisycon.com/docs/api/?resources=publishers'
    version: '1.2'
  (oas-x-preferred): true
  (oas-x-providerName): daisycon.com
/publishers:
  displayName: publishers
  '/{publisher_id}':
    displayName: '{publisher_id}'
    /productfeeds:
      displayName: productfeeds
      get:
        displayName: GET_publishers-publisher_id-productfeeds
        description: Returns a set of productfeeds
        (oas-summary): Get all productfeeds
        responses:
          '200':
            body:
              application/json:
                items:
                  type: productfeed
                type: array
            description: No response was specified
          '204':
            description: No content
          '404':
            description: Publisher not found
        queryParameters:
          page:
            default: 1
            description: Paginator page
            format: int64
            required: false
            type: integer
          per_page:
            default: 100
            description: Paginator results per page (max 1000)
            format: int64
            required: false
            type: integer
          program_id:
            description: Program IDs
            items:
              format: int64
              type: integer
            required: false
            type: array
          name:
            description: Name (wildcards * allowed)
            items:
              type: string
            required: false
            type: array
          media_id:
            description: Returns programs this media is subscribed to
            format: int64
            required: false
            type: integer
          placeholder_media_id:
            description: 'replace url placeholder #MEDIA_ID# value'
            format: string
            required: false
            type: string
            facets:
              format: string
          placeholder_subid:
            description: 'replace url placeholder #SUB_ID# value'
            format: string
            required: false
            type: string
            facets:
              format: string
        (oas-tags):
          - productfeeds
      '/{productfeed_id}':
        displayName: '{productfeed_id}'
        uriParameters:
