            description: Productfeed ID
            format: int64
            minimum: 1
            required: true
            type: integer
        get:
          displayName: GET_publishers-publisher_id-productfeeds-productfeed_id
          description: Returns a productfeed
          (oas-summary): Get productfeed
          responses:
            '200':
              body:
                application/json:
                  type: productfeed
              description: No response was specified
            '204':
              description: No content
            '404':
              description: Productfeed not found
          queryParameters:
            placeholder_media_id:
              description: 'replace url placeholder #MEDIA_ID# value'
              format: string
              required: false
              type: string
              facets:
                format: string
            placeholder_subid:
              description: 'replace url placeholder #SUB_ID# value'
