#%RAML 1.0
title: Getty Images
version: '3.0'
baseUri: 'https://api.gettyimages.com/'
protocols:
  - HTTPS
description: Build applications using the world's most powerful imagery
(oas-tags-definition):
  - name: AssetChanges
  - name: AssetRegistration
  - name: Boards
  - name: Collections
  - name: Countries
  - name: Downloads
  - name: Events
  - name: Images
  - name: Products
  - name: Purchases
  - name: Search
  - name: Usage
  - name: Videos
annotationTypes:
  oas-tags-definition:
    type: array
    items:
      properties:
        name: string
        description?: string
        externalDocs?:
          properties:
            url: string
            description?: string
    allowedTargets: API
  oas-x-logo: any
  oas-x-origin: any
  oas-x-preferred: any
  oas-x-providerName: any
  oas-tags:
    type: 'string[]'
    allowedTargets: Method
  oas-summary:
    type: string
    allowedTargets: Method
  oas-info:
    properties:
      termsOfService?: string
      contact?:
        properties:
          name?: string
          url?: string
          email?: string
      license?:
        properties:
          name?: string
          url?: string
    allowedTargets: API
  oas-body-name:
    type: string
    allowedTargets: TypeDeclaration
  oas-definition-name:
    type: string
    allowedTargets: TypeDeclaration
(oas-info):
  (oas-x-logo):
    backgroundColor: '#333333'
    url: 'https://api.apis.guru/v2/cache/logo/http_www.gettyimages.com_landing_assets_unisporkal_engine_images_getty-logo-negative-b117d413642b91fba020901fbd8949ba.svg'
  (oas-x-origin):
    format: swagger
    url: 'https://api.gettyimages.com/swagger/api-docs'
    version: '1.2'
  (oas-x-preferred): true
  (oas-x-providerName): gettyimages.com
securitySchemes:
  Api-Key:
    type: Pass Through
    describedBy:
      headers:
        Api-Key:
          type: string
    description: Provide your API key.
/v3:
  displayName: v3
  /events:
    displayName: events
    get:
      displayName: GET_v3-events
      description: Returns an array of event metadata
      (oas-summary): Get metadata for multiple events
      headers:
        Accept-Language:
          type: string
          description: Specifies the language of result values.
          required: false
        Authorization:
          type: string
          description: 'Provide access token in the format of ''Bearer {token}''.'
          required: false
      responses:
        '200':
          body:
            application/json:
              type: events_result
          description: OK
        '400':
          description: MalformedRequest
        '401':
          description: Unauthorized
        '404':
          description: EventNotFound
      queryParameters:
        fields:
          description: A comma separated list of fields to return in the response.
          items:
            enum:
              - id
              - child_event_count
              - editorial_segments
              - hero_image
              - image_count
              - location
              - name
              - start_date
              - type
            type: string
          required: false
          type: array
        ids:
          description: A comma separated list of event ids.
          items:
            format: int32
            type: integer
          required: false
          type: array
      (oas-tags):
        - Events
    '/{id}':
      displayName: '{id}'
      uriParameters:
        id:
          description: An event id.
          format: int32
          required: true
          type: integer
      get:
        displayName: GET_v3-events-id
        description: Returns an array of event metadata
        (oas-summary): Get metadata for a single event
        headers:
          Accept-Language:
            type: string
            description: Specifies the language of result values.
            required: false
          Authorization:
            type: string
            description: 'Provide access token in the format of ''Bearer {token}''.'
            required: false
        responses:
          '200':
            body:
              application/json:
                type: event
            description: OK
          '400':
            description: MalformedRequest
          '401':
            description: Unauthorized
          '404':
            description: EventNotFound
        queryParameters:
          fields:
            description: A comma separated list of fields to return in the response.
            items:
              enum:
                - id
                - child_event_count
                - editorial_segments
                - hero_image
                - image_count
                - location
                - name
                - start_date
                - type
              type: string
            required: false
            type: array
        (oas-tags):
          - Events
