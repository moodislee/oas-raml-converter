        responses:
          '200':
            body:
              application/json:
                type: report_usage_batch_response
            description: Success - All usages reported were successfully recorded.
          '400':
            description: InvalidAssets - Invalid assets found in the request.
          '401':
            description: UnauthorizedToReportUsage - User was not authorized to report usages. Contact your Getty Images Account Manager to discuss options for submitting your usage data.
