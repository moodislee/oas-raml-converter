            required: false
            type: array
        (oas-tags):
          - Videos
    get:
      displayName: GET_v3-videos
      (oas-summary): Get metadata for multiple videos by supplying multiple video ids
      headers:
        Accept-Language:
          type: string
          description: Specifies the language of result values.
          required: false
        Authorization:
          type: string
          description: 'Provide access token in the format of ''Bearer {token}''.'
          required: false
      responses:
        '200':
          body:
            application/json:
              type: videos_detail
          description: OK
        '400':
          description: MalformedRequest
        '401':
          description: Unauthorized
        '403':
          description: UnauthorizedDisplaySize
        '404':
          description: VideosNotFound
      queryParameters:
        ids:
          description: Specifies one or more video ids to return. Use comma delimiter when requesting multiple ids.
          items:
            type: string
          required: true
          type: array
        fields:
          description: Specifies fields to return. Defaults to 'summary_set'.
          items:
            enum:
              - id
              - allowed_use
              - artist
              - asset_family
              - caption
              - clip_length
              - collection_code
              - collection_id
              - collection_name
              - color_type
              - copyright
              - video_comp
              - date_created
              - detail_set
              - display_set
              - download_sizes
              - downloads
              - era
              - editorial_segments
              - keywords
              - license_model
              - mastered_to
              - originally_shot_on
              - video_preview
              - product_types
              - shot_speed
              - source
              - summary_set
              - video_thumb
              - title
            type: string
          required: false
          type: array
      (oas-tags):
        - Videos
  /asset-changes:
    displayName: asset-changes
    /change-sets:
      displayName: change-sets
      put:
        displayName: PUT_v3-asset-changes-change-sets
        (oas-summary): Generates asset changes.
        headers:
          Accept-Language:
            type: string
            description: Accept-Language parameter optional. Used to restrict the set of natural languages that are preferred as a response to the request.
            required: false
          Authorization:
            type: string
            description: Authorization token required. Used for applying content access restrictions. Use one of the OAuth2 grants to auto-populate this value.
            required: true
        responses:
          '200':
            body:
              application/json:
                type: AssetChanges
            description: No response was specified
          '403':
            description: Your access token does not authorize access to this resource
          '404':
            description: The channel you specified does not exist
        queryParameters:
          channel_id:
            description: Specifies the id of the channel for the asset data. Valid channel ids can be found in the results of the Get Partner Channel query.
            format: int32
            required: true
            type: integer
          batch_size:
            description: Specifies the number of assets to return. The default is 10; maximum is 500.
            format: int32
            required: false
            type: integer
        (oas-tags):
          - AssetChanges
      '/{change-set-id}':
        displayName: '{change-set-id}'
        uriParameters:
          change-set-id:
            description: Specify the change-set-id associated with a transaction resource whose receipt you want to confirm.
            format: int64
            required: true
            type: integer
        delete:
          displayName: DELETE_v3-asset-changes-change-sets-change-set-id
          (oas-summary): Confirm asset changes acknowledges receipt of asset changes.
          headers:
            Accept-Language:
              type: string
              description: Accept-Language parameter optional. Used to restrict the set of natural languages that are preferred as a response to the request.
              required: false
            Authorization:
              type: string
              description: Authorization token required. Used for applying content access restrictions. Use one of the OAuth2 grants to auto-populate this value.
              required: true
          responses:
            '200':
              body:
                application/json:
                  type: string
              description: No response was specified
            '403':
              description: Your access token does not authorize access to this resource
            '404':
              description: Transaction was not found
          (oas-tags):
            - AssetChanges
    /channels:
      displayName: channels
      get:
        displayName: GET_v3-asset-changes-channels
        (oas-summary): Retrieves the channel data for the partner. This data can be used to populate the channel_id parameter in the Put Asset Changes query.
