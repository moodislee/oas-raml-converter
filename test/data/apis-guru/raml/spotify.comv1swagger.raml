#%RAML 1.0
title: Spotify
version: v1
baseUri: 'https://api.spotify.com/v1'
mediaType: application/json
protocols:
  - HTTPS
(oas-info):
  (oas-x-origin):
    format: raml
    url: 'https://raw.githubusercontent.com/spotify/web-api/master/specifications/raml/api.raml'
    version: '0.8'
  (oas-x-preferred): true
  (oas-x-providerName): spotify.com
annotationTypes:
  oas-x-origin: any
  oas-x-preferred: any
  oas-x-providerName: any
  oas-externalDocs:
    properties:
      description?: string
      url: string
    allowedTargets:
      - API
      - Method
      - TypeDeclaration
  oas-info:
    properties:
      termsOfService?: string
      contact?:
        properties:
          name?: string
          url?: string
          email?: string
      license?:
        properties:
          name?: string
          url?: string
    allowedTargets: API
  oas-body-name:
    type: string
    allowedTargets: TypeDeclaration
(oas-externalDocs):
  url: 'https://developer.spotify.com/'
securitySchemes:
  oauth_2_0_accessCode:
    type: OAuth 2.0
    settings:
      authorizationUri: 'https://accounts.spotify.com/authorize'
      accessTokenUri: 'https://accounts.spotify.com/api/token'
      authorizationGrants:
        - authorization_code
      scopes:
        - playlist-modify-private
        - playlist-modify-public
        - playlist-read-collaborative
        - playlist-read-private
        - user-follow-modify
        - user-follow-read
        - user-library-modify
        - user-library-read
        - user-read-birthdate
        - user-read-email
        - user-read-private
    description: |
      Spotify supports OAuth 2.0 for authenticating all API requests.
  oauth_2_0_implicit:
    type: OAuth 2.0
    settings:
      authorizationUri: 'https://accounts.spotify.com/authorize'
      accessTokenUri: ''
      authorizationGrants:
        - implicit
      scopes:
        - playlist-modify-private
        - playlist-modify-public
        - playlist-read-collaborative
        - playlist-read-private
        - user-follow-modify
        - user-follow-read
        - user-library-modify
        - user-library-read
        - user-read-birthdate
        - user-read-email
        - user-read-private
    description: |
      Spotify supports OAuth 2.0 for authenticating all API requests.
/me:
  displayName: me
  /tracks:
    displayName: tracks
    delete:
      displayName: DELETE_me-tracks
      description: |
        [Remove Tracks for Current User](https://developer.spotify.com/web-api/remove-tracks-user/)
      headers:
        Accept:
          type: string
          description: It is used to set specified media type.
      responses:
        '200':
          description: OK
        '429':
          description: |
            API rate limit exceeded. See http://developer.spotify.com/web-api/user-guide/#rate-limiting for details.
      queryParameters:
        ids:
          description: A comma-separated list of IDs
          required: true
          type: string
          displayName: A comma-separated list of IDs
    get:
      displayName: GET_me-tracks
      description: |
        [Get Current User's Saved Tracks](https://developer.spotify.com/web-api/get-users-saved-tracks/)
      headers:
        Accept:
          type: string
          description: It is used to set specified media type.
      responses:
        '200':
          body:
            application/json:
              type: saved-track-page
          description: OK
        '429':
          description: |
            API rate limit exceeded. See http://developer.spotify.com/web-api/user-guide/#rate-limiting for details.
      queryParameters:
        limit:
          default: 20
          description: The maximum number of items to return
          maximum: 50
          minimum: 0
          required: false
          type: integer
          displayName: The maximum number of items to return
        offset:
          default: 0
          description: The index of the first item to return
          required: false
          type: integer
          displayName: The index of the first item to return
        market:
          description: The market (an ISO 3166-1 alpha-2 country code)
          required: false
          type: string
          displayName: The market (an ISO 3166-1 alpha-2 country code)
    put:
      displayName: PUT_me-tracks
      description: |
        [Save Tracks for Current User](https://developer.spotify.com/web-api/save-tracks-user/)
      headers:
        Accept:
          type: string
          description: It is used to set specified media type.
      responses:
        '200':
          description: OK
        '429':
          description: |
            API rate limit exceeded. See http://developer.spotify.com/web-api/user-guide/#rate-limiting for details.
      queryParameters:
        ids:
          description: A comma-separated list of IDs
          required: true
          type: string
          displayName: A comma-separated list of IDs
    /contains:
      displayName: contains
      get:
        displayName: GET_me-tracks-contains
        description: |
          [Check Current User's Saved Tracks](https://developer.spotify.com/web-api/check-users-saved-tracks/)
        responses:
          '200':
            body:
              application/json:
                items:
                  description: 'True if the track is in user''s Your Music library, false otherwise'
                  type: boolean
                type: array
            description: OK
        queryParameters:
          ids:
            description: A comma-separated list of IDs
            required: true
            type: string
            displayName: A comma-separated list of IDs
  /following:
    displayName: following
    delete:
      displayName: DELETE_me-following
      description: |
        [Unfollow Artists or Users](https://developer.spotify.com/web-api/unfollow-artists-users/)
      responses:
        '204':
          description: No Content
      queryParameters:
        type:
          description: The type to unfollow.
          enum:
            - artist
            - user
          required: true
          type: string
          displayName: The type to unfollow.
        ids:
          description: A comma-separated list of the artists or users ids
          required: true
          type: string
          displayName: A comma-separated list of the artists or users ids
    put:
      displayName: PUT_me-following
      description: |
        [Follow Artists or Users](https://developer.spotify.com/web-api/follow-artists-users/)
      responses:
        '204':
          description: No Content
      queryParameters:
        type:
          description: The type to follow.
          enum:
            - artist
            - user
          required: true
          type: string
          displayName: The type to follow.
        ids:
          description: A comma-separated list of the artists or users ids
          required: true
          type: string
          displayName: A comma-separated list of the artists or users ids
    /contains:
      displayName: contains
      get:
        displayName: GET_me-following-contains
        description: |
          [Check if Current User Follows Artists or Users](https://developer.spotify.com/web-api/check-current-user-follows/)
        responses:
          '200':
            body:
              application/json:
                items:
                  description: 'True if the user follows the artist/user, false otherwise'
                  type: boolean
                type: array
            description: OK
        queryParameters:
          type:
            description: The type to follow.
            enum:
              - artist
              - user
            required: true
            type: string
            displayName: The type to follow.
          ids:
            description: A comma-separated string of the artists or users ids.
            required: true
            type: string
            displayName: A comma-separated string of the artists or users ids.
    get:
      displayName: GET_me-following
      description: |
        [Get User's Followed Artists](https://developer.spotify.com/web-api/get-followed-artists/)
      responses:
        '200':
          body:
            application/json:
              type: user-followed
          description: OK
      queryParameters:
        type:
          description: 'The ID type, currently only artist is supported.'
          enum:
            - artist
          required: true
          type: string
          displayName: 'The ID type, currently only artist is supported.'
        limit:
          default: 20
          description: The maximum number of items to return
          maximum: 50
          minimum: 0
          required: false
          type: integer
          displayName: The maximum number of items to return
        after:
          description: The last artist ID retrieved from the previous request.
          required: false
          type: string
          displayName: The last artist ID retrieved from the previous request.
  get:
    displayName: GET_me
    description: |
      [Get Current User's Profile](https://developer.spotify.com/web-api/get-current-users-profile/)
    responses:
      '200':
        body:
          application/json:
            type: current-user-profile
        description: OK
