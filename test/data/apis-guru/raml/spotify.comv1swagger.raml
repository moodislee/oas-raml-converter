              '200':
                description: OK
          put:
            displayName: PUT_users-user_id-playlists-playlist_id-followers
            description: |
              [Follow a Playlist](https://developer.spotify.com/web-api/follow-playlist/)
            body:
              application/json:
                properties:
                  public:
                    type: boolean
                    required: false
                type: object
                (oas-body-name): body
            responses:
              '200':
                description: OK
          /contains:
            displayName: contains
            get:
              displayName: GET_users-user_id-playlists-playlist_id-followers-contains
