              [Reorder or replace a Playlist's Tracks](https://developer.spotify.com/web-api/reorder-playlists-tracks/)
            body:
              application/json:
                properties:
                  uris:
                    items: {}
                    type: array
                type: object
                (oas-body-name): body
            headers:
              Accept:
                type: string
                description: It is used to set specified media type.
            responses:
              '200':
                body:
                  application/json:
                    type: playlist-snapshot
                description: OK
              '429':
                description: |
                  API rate limit exceeded. See http://developer.spotify.com/web-api/user-guide/#rate-limiting for details.
          post:
            displayName: POST_users-user_id-playlists-playlist_id-tracks
            description: |
              [Add Tracks to a Playlist](https://developer.spotify.com/web-api/add-tracks-to-playlist/)
            headers:
              Accept:
                type: string
                description: It is used to set specified media type.
            responses:
              '200':
                body:
                  application/json:
                    type: playlist-snapshot
                description: OK
              '429':
                description: |
                  API rate limit exceeded. See http://developer.spotify.com/web-api/user-guide/#rate-limiting for details.
            queryParameters:
              position:
                description: 'The position to insert the tracks, a zero-based index'
                required: false
                type: integer
                displayName: 'The position to insert the tracks, a zero-based index'
              uris:
                description: A comma-separated list of Spotify track URIs to add. A maximum of 100 tracks can be added in one request.
                required: true
                type: string
                displayName: A comma-separated list of Spotify track URIs to add. A maximum of 100 tracks can be added in one request.
          get:
            displayName: GET_users-user_id-playlists-playlist_id-tracks
            description: |
              [Get a Playlist's Tracks](https://developer.spotify.com/web-api/get-playlists-tracks/)
            headers:
              Accept:
                type: string
                description: It is used to set specified media type.
            responses:
              '200':
