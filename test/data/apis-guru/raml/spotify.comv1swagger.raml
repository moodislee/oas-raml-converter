                    application/json:
                      items:
                        description: 'True if the user follows the playlist, false otherwise'
                        type: boolean
                      type: array
                  description: OK
              queryParameters:
                ids:
                  description: A comma-separated list of users ids
                  required: true
                  type: string
                  displayName: A comma-separated list of users ids
      get:
        displayName: GET_users-user_id-playlists
        description: |
          [Get a List of a User's Playlists](https://developer.spotify.com/web-api/get-list-users-playlists/)
        headers:
          Accept:
            type: string
            description: It is used to set specified media type.
        responses:
          '200':
            body:
              application/json:
                type: playlist-simple-page
            description: OK
          '429':
            description: |
              API rate limit exceeded. See http://developer.spotify.com/web-api/user-guide/#rate-limiting for details.
        queryParameters:
          limit:
            default: 20
            description: The maximum number of items to return
            maximum: 50
            minimum: 0
            required: false
            type: integer
            displayName: The maximum number of items to return
          offset:
            default: 0
            description: The index of the first item to return
            required: false
            type: integer
            displayName: The index of the first item to return
      post:
        displayName: POST_users-user_id-playlists
        description: |
          [Create a Playlist](https://developer.spotify.com/web-api/create-playlist/)
        body:
          application/json:
            properties:
              name:
                type: string
              public:
                type: boolean
                required: false
            type: object
            (oas-body-name): body
        headers:
          Accept:
            type: string
            description: It is used to set specified media type.
        responses:
          '201':
            body:
              application/json:
                type: playlist
            description: Created
          '429':
            description: |
              API rate limit exceeded. See http://developer.spotify.com/web-api/user-guide/#rate-limiting for details.
    uriParameters:
      user_id:
        description: The user's Spotify user ID.
        required: true
        type: string
        displayName: The user's Spotify user ID.
    get:
      displayName: GET_users-user_id
      description: |
        [Get a User's Profile](https://developer.spotify.com/web-api/get-users-profile/)
      responses:
        '200':
          body:
            application/json:
              type: user-profile
          description: OK
