          (oas-summary): Delete card
          responses:
            '200':
              body:
                application/json:
                  type: ProfileResponse
              description: The Profile.
            '400':
              body:
                application/json:
                  type: BeanstreamException
              description: Bad Request
            '401':
              body:
                application/json:
                  type: BeanstreamException
              description: Authentication Failure
            '402':
              body:
                application/json:
                  type: BeanstreamException
              description: Business Rule Violation or Decline
            '403':
              body:
                application/json:
                  type: BeanstreamException
              description: Authorization Failure
            '405':
              body:
                application/json:
                  type: BeanstreamException
              description: Invalid Request Method
            '500':
              body:
                application/json:
                  type: BeanstreamException
              description: Internal Server Error
          (oas-tags):
            - Profiles
      post:
        displayName: POST_profiles-profileId-cards
        description: Add a card to the profile. Note that there is a default limit of 1 card per profile. You can change this in your Profiles settings in the back office.
        (oas-summary): Add card
        body:
          application/json:
            type: ProfileCard
            (oas-body-name): card
            description: The card that will be added to the profile.
        responses:
          '200':
            body:
              application/json:
                type: ProfileResponse
            description: The Profile.
          '400':
            body:
              application/json:
                type: BeanstreamException
            description: Bad Request
          '401':
            body:
              application/json:
                type: BeanstreamException
