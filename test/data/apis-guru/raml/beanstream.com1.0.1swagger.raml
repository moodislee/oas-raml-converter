/scripts:
  displayName: scripts
  /tokenization:
    displayName: tokenization
    /tokens:
      displayName: tokens
      post:
        displayName: POST_scripts-tokenization-tokens
        description: 'NOTE that the full URL for this API call is https://www.beanstream.com/scripts/tokenization/tokens. Turn a credit card into a token using this service. This helps lessen your PCI scope by not passing the credit card information to your server. Instead you turn the card number into a token in the client app, then send the token to the server. When you send the token to Beanstream to make a payment, Beanstream then looks up the card number from that token and makes the payment. Tokens can also be used to create payment profiles.'
        (oas-summary): Tokenize credit card
        body:
          application/json:
            type: TokenRequest
            (oas-body-name): token_request
        responses:
          '200':
            body:
              application/json:
                type: TokenResponse
            description: Token response object. One will always be returned even if the data or card was invalid. The validity of the card is not checked with this API call.
        (oas-tags):
          - Tokenization
/payments:
  displayName: payments
  post:
    displayName: POST_payments
    description: 'Make a payment using credit card, cash, cheque, profile, or token. Each payment type has its own json definition passed in the body. For all payments you have the standard Billing, Shipping, Comments, etc. fields that are optional. Only the amount is required along with the payment data for card, cash, cheque, profile, and token. You must change the payment_method for each payment type. Credit Card - "card", Payment Profile - "payment_profile", Legato Token - "token", Cash - "cash", Cheque - "cheque", Interac - "interac"'
    (oas-summary): Make Payment
    body:
      application/json:
        type: PaymentRequest
        (oas-body-name): payment_request
    responses:
      '200':
        body:
          application/json:
            type: PaymentResponse
        description: Payment response containing the payment details as well as if the payment was approved or declined.
      '400':
        body:
          application/json:
            type: BeanstreamException
        description: Bad Request
      '401':
        body:
          application/json:
            type: BeanstreamException
        description: Authentication Failure
      '402':
        body:
          application/json:
            type: BeanstreamException
        description: Business Rule Violation or Decline
      '403':
        body:
          application/json:
            type: BeanstreamException
        description: Authorization Failure
      '405':
        body:
          application/json:
            type: BeanstreamException
        description: Invalid Request Method
      '500':
        body:
          application/json:
            type: BeanstreamException
        description: Internal Server Error
    (oas-tags):
      - Payments
  '/{transId}':
    displayName: '{transId}'
    /completions:
      displayName: completions
      post:
        displayName: POST_payments-transId-completions
        description: Complete a pre-authorized payment. The amount of the transaction to complete must be less than or equal to the original pre-auth amount. Complete must be set to true.
        (oas-summary): Complete pre-auth
        body:
          application/json:
            type: PaymentRequest
            (oas-body-name): payment_request
        responses:
          '200':
            body:
              application/json:
                type: PaymentResponse
            description: Payment response containing the payment details as well as if the transaction was approved or declined.
          '400':
            body:
              application/json:
                type: BeanstreamException
            description: Bad Request
          '401':
            body:
              application/json:
                type: BeanstreamException
            description: Authentication Failure
          '402':
            body:
              application/json:
                type: BeanstreamException
            description: Business Rule Violation or Decline
          '403':
            body:
              application/json:
                type: BeanstreamException
            description: Authorization Failure
          '405':
            body:
              application/json:
                type: BeanstreamException
            description: Invalid Request Method
          '500':
            body:
              application/json:
                type: BeanstreamException
            description: Internal Server Error
        (oas-tags):
          - Payments
    /void:
      displayName: void
      post:
        displayName: POST_payments-transId-void
        description: 'Void a transaction. You can void payments, returns, pre-auths, and completions. It will cancel that transaction.'
        (oas-summary): Void Transaction
        body:
          application/json:
            type: Void
            (oas-body-name): void
        responses:
          '200':
            body:
              application/json:
                type: PaymentResponse
            description: Payment response containing the payment details as well as if the transaction was approved or declined.
          '400':
            body:
              application/json:
                type: BeanstreamException
            description: Bad Request
          '401':
            body:
              application/json:
                type: BeanstreamException
            description: Authentication Failure
          '402':
            body:
              application/json:
                type: BeanstreamException
            description: Business Rule Violation or Decline
          '403':
            body:
              application/json:
                type: BeanstreamException
            description: Authorization Failure
          '405':
            body:
              application/json:
                type: BeanstreamException
            description: Invalid Request Method
          '500':
            body:
              application/json:
                type: BeanstreamException
            description: Internal Server Error
        (oas-tags):
          - Payments
    /returns:
      displayName: returns
      post:
        displayName: POST_payments-transId-returns
        description: Return payment.
        (oas-summary): Return payment
        body:
          application/json:
            type: Return
            (oas-body-name): return
        responses:
          '200':
            body:
              application/json:
                type: PaymentResponse
            description: Payment response containing the payment details as well as if the transaction was approved or declined.
          '400':
            body:
              application/json:
                type: BeanstreamException
            description: Bad Request
          '401':
            body:
              application/json:
                type: BeanstreamException
            description: Authentication Failure
          '402':
            body:
              application/json:
                type: BeanstreamException
            description: Business Rule Violation or Decline
          '403':
            body:
              application/json:
                type: BeanstreamException
            description: Authorization Failure
          '405':
            body:
              application/json:
                type: BeanstreamException
            description: Invalid Request Method
          '500':
            body:
              application/json:
                type: BeanstreamException
            description: Internal Server Error
        (oas-tags):
          - Payments
    uriParameters:
      transId:
        description: The transaction id.
        required: true
        type: integer
    get:
      displayName: GET_payments-transId
      description: Get a previous payment (transaction).
      (oas-summary): Get payment
      responses:
        '200':
          body:
            application/json:
              type: Transaction
          description: A transaction object.
        '400':
          body:
            application/json:
              type: BeanstreamException
          description: Bad Request
        '401':
          body:
            application/json:
              type: BeanstreamException
          description: Authentication Failure
        '402':
          body:
            application/json:
              type: BeanstreamException
          description: Business Rule Violation or Decline
        '403':
          body:
            application/json:
              type: BeanstreamException
          description: Authorization Failure
        '405':
          body:
            application/json:
              type: BeanstreamException
          description: Invalid Request Method
        '500':
          body:
            application/json:
              type: BeanstreamException
          description: Internal Server Error
      (oas-tags):
        - Payments
/reports:
  displayName: reports
  post:
    displayName: POST_reports
    description: 'Query for transactions using a date range and optional search criteria. This method allows you to page your search results if you are expecting a lot of results to be returned. The page start value begins at 1. If no records are found the API will return a 404 error message. For details on the parameters and allowed values for Criteria please visit the documentation http://developer.beanstream.com/documentation/analyze-payments/search-specific-criteria/'
    (oas-summary): Search Query
    body:
      application/json:
        type: SearchQuery
        (oas-body-name): search_query
    responses:
      '200':
        body:
          application/json:
            type: SearchResult
        description: A transaction object.
      '400':
        body:
          application/json:
            type: BeanstreamException
        description: Bad Request
      '401':
        body:
          application/json:
            type: BeanstreamException
        description: Authentication Failure
      '402':
        body:
          application/json:
            type: BeanstreamException
        description: Business Rule Violation or Decline
      '403':
        body:
          application/json:
            type: BeanstreamException
        description: Authorization Failure
      '405':
        body:
          application/json:
            type: BeanstreamException
        description: Invalid Request Method
      '500':
        body:
          application/json:
            type: BeanstreamException
        description: Internal Server Error
    (oas-tags):
      - Reporting
types:
  Address:
    properties:
      address_line1:
        description: alphanumeric (64)
        type: string
        required: false
      address_line2:
        description: alphanumeric (64)
        type: string
        required: false
      city:
        description: alphanumeric (32)
        type: string
        required: false
      country:
        description: characters (2)
        type: string
        required: false
      email_address:
        description: alphanumeric (32)
        type: string
        required: false
      name:
        description: alphanumeric (64)
        type: string
        required: false
      phone_number:
        description: alphanumeric (32)
        type: string
        required: false
      postal_code:
        description: alphanumeric (16)
        type: string
        required: false
      province:
        description: characters (2)
        type: string
        required: false
  Adjustment:
    properties:
      amount:
        format: double
        type: number
        required: false
      approval:
        type: integer
        required: false
      created:
        description: 'The date it was created ''2015-04-22T10:03:19.323-07:00'''
        type: string
        required: false
      id:
        type: integer
        required: false
      message:
        type: string
        required: false
      type:
        type: string
        required: false
      url:
        type: string
        required: false
  BeanstreamException:
    properties:
      category:
        format: int32
        type: integer
        required: false
      code:
        format: int32
        type: integer
        required: false
      details:
        items:
          type: Detail
        type: array
        required: false
      message:
        type: string
        required: false
      reference:
        type: string
        required: false
  Card:
    properties:
      complete:
        default: true
        description: 'set to false for Pre-Authorize, and true to complete a payment'
        type: boolean
        required: false
      cvd:
        default: '123'
        description: Security code on the back of the credit card. This can be set to mandatory in the back office. digits(3 or 4)
        type: string
        required: false
      expiry_month:
        default: '02'
        description: eg. 02 for February. digits(2)
        type: string
      expiry_year:
        default: '18'
        description: eg. 15 for 2015. digits(2)
        type: string
      name:
        description: Card holder name. alphanumeric(64)
        type: string
      number:
        default: '5100000010001004'
        description: Credit card number (PAN). digits(20)
        type: string
  CardGetTransactionResponse:
    properties:
      address_match:
        description: digits (1)
        type: integer
        required: false
      avs_result:
        description: characters (1)
        type: string
        required: false
      card_type:
        description: characters (2)
        type: string
        required: false
      cvd_match:
        description: digits (2)
        type: integer
        required: false
      expiry_month:
        description: characters (2)
        type: string
        required: false
      expiry_year:
        description: characters (2)
        type: string
        required: false
      last_four:
        description: digits (4)
        type: string
        required: false
  CardPurchaseResponse:
    properties:
      address_match:
        description: digits (1)
        type: integer
        required: false
      card_type:
        description: characters (2)
        type: string
        required: false
      cvd_match:
        description: digits (2)
        type: integer
        required: false
      last_four:
        description: digits (4)
        type: string
        required: false
      postal_result:
        description: digits (1)
        type: integer
        required: false
  CreateProfileBody:
    properties:
      billing:
        type: Address
        required: false
      card:
        type: ProfileFromCard
        required: false
      comment:
        description: alphanumeric(256)
        type: string
        required: false
      custom:
        type: Custom
        required: false
      language:
        description: characters(2)
        type: string
        required: false
      token:
        type: ProfileFromToken
        required: false
  Criteria:
    properties:
      field:
        description: 'Refer to the table on this page for the searchable fields http://developer.beanstream.com/documentation/analyze-payments/search-specific-criteria/'
        type: integer
        required: false
      operator:
        description: 'URL encoded comparators such as less than, greater than, equals... Refer to the table on this page for the full list of comparators http://developer.beanstream.com/documentation/analyze-payments/search-specific-criteria/'
        type: string
        required: false
      value:
        description: The value you want to match against.
        type: string
        required: false
  Custom:
    properties:
      ref1:
        description: alphanumeric (256)
        type: string
        required: false
      ref2:
        description: alphanumeric (256)
        type: string
        required: false
      ref3:
        description: alphanumeric (256)
        type: string
        required: false
      ref4:
        description: alphanumeric (256)
        type: string
        required: false
      ref5:
        description: alphanumeric (256)
        type: string
        required: false
  Detail:
    properties:
      field:
        type: string
        required: false
      message:
        type: string
        required: false
  GetProfileDefaultCard:
    properties:
      card_type:
        description: 'MC, VI etc. characters(2)'
        type: string
        required: false
      expiry_month:
        description: digits(2)
        type: string
        required: false
      expiry_year:
        description: digits(2)
        type: string
        required: false
      name:
        description: cardholder name alphanumeric(64)
        type: string
        required: false
      number:
        description: masked credit card number alphanumeric(20)
        type: string
        required: false
  Link:
    properties:
      href:
        description: 'alphanumeric (256). Url to perform the void, return, completion etc.'
        type: string
        required: false
      method:
        description: 'characters (16). Http method. post, put, get etc.'
        type: string
        required: false
      ref:
        description: characters (16). Eg. "void" or "return"
        type: string
        required: false
  PaymentProfile:
    properties:
      account_ref:
        type: string
        required: false
      billing:
        type: Address
        required: false
      card:
        type: GetProfileDefaultCard
        required: false
      code:
        description: digits(2)
        format: int32
        type: integer
        required: false
      custom:
        type: Custom
        required: false
      customer_code:
