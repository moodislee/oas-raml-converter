          '402':
            body:
              application/json:
                type: BeanstreamException
            description: Business Rule Violation or Decline
          '403':
            body:
              application/json:
                type: BeanstreamException
            description: Authorization Failure
          '405':
            body:
              application/json:
                type: BeanstreamException
            description: Invalid Request Method
          '500':
            body:
              application/json:
                type: BeanstreamException
            description: Internal Server Error
        (oas-tags):
          - Profiles
    delete:
      displayName: DELETE_profiles-profileId
      description: 'Delete a Payment Profile using the profile ID, also known as the Customer Code.'
      (oas-summary): Delete profile
      responses:
        '200':
          body:
            application/json:
              type: ProfileResponse
          description: The Profile.
        '400':
          body:
            application/json:
              type: BeanstreamException
          description: Bad Request
        '401':
          body:
            application/json:
              type: BeanstreamException
          description: Authentication Failure
        '402':
          body:
            application/json:
              type: BeanstreamException
          description: Business Rule Violation or Decline
        '403':
          body:
            application/json:
              type: BeanstreamException
          description: Authorization Failure
        '405':
          body:
            application/json:
              type: BeanstreamException
          description: Invalid Request Method
        '500':
          body:
            application/json:
              type: BeanstreamException
          description: Internal Server Error
      (oas-tags):
        - Profiles
  post:
    displayName: POST_profiles
    description: Create a new Payment Profile using either a card or a Legato token. You must supply one of them.
    (oas-summary): Create Profile
    body:
      application/json:
        type: CreateProfileBody
        (oas-body-name): createProfileBody
    responses:
      '200':
        body:
          application/json:
            type: ProfileResponse
        description: The Profile.
      '400':
        body:
          application/json:
            type: BeanstreamException
        description: Bad Request
      '401':
        body:
          application/json:
            type: BeanstreamException
        description: Authentication Failure
      '402':
        body:
          application/json:
            type: BeanstreamException
        description: Business Rule Violation or Decline
      '403':
        body:
          application/json:
            type: BeanstreamException
        description: Authorization Failure
      '405':
        body:
          application/json:
            type: BeanstreamException
        description: Invalid Request Method
      '500':
        body:
          application/json:
            type: BeanstreamException
        description: Internal Server Error
    (oas-tags):
