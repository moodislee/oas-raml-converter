/profiles:
  displayName: profiles
  '/{profileId}':
    displayName: '{profileId}'
    uriParameters:
      profileId:
        description: The profile id. (aka CustomerCode)
        required: true
        type: string
    get:
      displayName: GET_profiles-profileId
      description: 'Get a Payment Profile using the profile ID, also known as the Customer Code.'
      (oas-summary): Get profile
      responses:
        '200':
          body:
            application/json:
              type: PaymentProfile
          description: The Profile.
        '400':
          body:
            application/json:
              type: BeanstreamException
          description: Bad Request
        '401':
          body:
            application/json:
              type: BeanstreamException
          description: Authentication Failure
        '402':
          body:
            application/json:
              type: BeanstreamException
          description: Business Rule Violation or Decline
        '403':
          body:
            application/json:
              type: BeanstreamException
          description: Authorization Failure
        '405':
          body:
            application/json:
              type: BeanstreamException
          description: Invalid Request Method
        '500':
          body:
            application/json:
              type: BeanstreamException
          description: Internal Server Error
      (oas-tags):
        - Profiles
    put:
      displayName: PUT_profiles-profileId
      description: Create a new Payment Profile using either a card or a Legato token. You must supply one of them.
      (oas-summary): Update Profile
      body:
        application/json:
          type: UpdateProfileBody
          (oas-body-name): updateProfileBody
      responses:
        '200':
          body:
            application/json:
              type: ProfileResponse
          description: The Profile.
        '400':
          body:
            application/json:
              type: BeanstreamException
          description: Bad Request
        '401':
          body:
            application/json:
              type: BeanstreamException
          description: Authentication Failure
        '402':
          body:
            application/json:
              type: BeanstreamException
          description: Business Rule Violation or Decline
        '403':
          body:
            application/json:
              type: BeanstreamException
          description: Authorization Failure
        '405':
          body:
            application/json:
              type: BeanstreamException
          description: Invalid Request Method
        '500':
