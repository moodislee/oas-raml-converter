            body:
              application/json:
                type: BeanstreamException
            description: Authorization Failure
          '405':
            body:
              application/json:
                type: BeanstreamException
            description: Invalid Request Method
          '500':
            body:
              application/json:
                type: BeanstreamException
            description: Internal Server Error
        (oas-tags):
          - Profiles
      '/{cardId}':
        displayName: '{cardId}'
        uriParameters:
          cardId:
            description: The card id.
            required: true
            type: integer
        put:
          displayName: PUT_profiles-profileId-cards-cardId
          description: Update the details of a card on the profile.
          (oas-summary): Update card
