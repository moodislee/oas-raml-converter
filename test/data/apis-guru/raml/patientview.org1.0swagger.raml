#%RAML 1.0
title: PatientView
version: '1.0'
baseUri: 'https://www.patientview.org/'
mediaType: application/json
protocols:
  - HTTPS
description: The recommended REST API endpoints to be used when integrating with PatientView
(oas-tags-definition):
  - description: Auth Controller
    name: auth-controller
  - description: Observation Controller
    name: observation-controller
  - description: Observation Heading Controller
    name: observation-heading-controller
  - description: Patient Controller
    name: patient-controller
  - description: Patient Management Controller
    name: patient-management-controller
annotationTypes:
  oas-tags-definition:
    type: array
    items:
      properties:
        name: string
        description?: string
        externalDocs?:
          properties:
            url: string
            description?: string
    allowedTargets: API
  oas-x-logo: any
  oas-x-origin: any
  oas-x-preferred: any
  oas-x-providerName: any
  oas-tags:
    type: 'string[]'
    allowedTargets: Method
  oas-summary:
    type: string
    allowedTargets: Method
  oas-info:
    properties:
      termsOfService?: string
      contact?:
        properties:
          name?: string
          url?: string
          email?: string
      license?:
        properties:
          name?: string
          url?: string
    allowedTargets: API
  oas-body-name:
    type: string
    allowedTargets: TypeDeclaration
  oas-definition-name:
    type: string
    allowedTargets: TypeDeclaration
(oas-info):
  (oas-x-logo):
    url: 'https://api.apis.guru/v2/cache/logo/https_www.patientview.org_images_pv-logo.png'
  (oas-x-origin):
    format: swagger
    url: 'https://www.patientview.org/api/api-docs'
    version: '1.2'
  (oas-x-preferred): true
  (oas-x-providerName): patientview.org
  termsOfService: 'https://www.patientview.org/#/terms'
/patientmanagement:
  displayName: patientmanagement
  '/{userId}':
    displayName: '{userId}'
    /group:
      displayName: group
      '/{groupId}':
        displayName: '{groupId}'
        /identifier:
          displayName: identifier
          '/{identifierId}':
            displayName: '{identifierId}'
            uriParameters:
              identifierId:
                description: identifierId
                format: int64
                required: true
                type: integer
            get:
              displayName: getPatientManagement
              description: getPatientManagement
              (oas-summary): getPatientManagement
              responses:
                '200':
                  body:
                    application/json:
                      type: PatientManagement
                  description: OK
                '401':
                  description: Unauthorized
                '403':
                  description: Forbidden
                '404':
                  description: Not Found
              (oas-tags):
                - patient-management-controller
            post:
              displayName: savePatientManagement
              description: savePatientManagement
              (oas-summary): savePatientManagement
              body:
                application/json:
                  type: PatientManagement
                  (oas-body-name): body
                  description: patientManagement
              responses:
                '200':
                  description: Description was not specified
                '201':
                  description: Created
                '401':
                  description: Unauthorized
                '403':
                  description: Forbidden
                '404':
                  description: Not Found
              (oas-tags):
                - patient-management-controller
            /surgeries:
              displayName: surgeries
              post:
                displayName: savePatientManagementSurgeries
                description: savePatientManagementSurgeries
                (oas-summary): savePatientManagementSurgeries
                body:
                  application/json:
                    type: PatientManagement
                    (oas-body-name): body
                    description: patientManagement
                responses:
                  '200':
                    description: Description was not specified
                  '201':
                    description: Created
                  '401':
                    description: Unauthorized
                  '403':
                    description: Forbidden
                  '404':
                    description: Not Found
                (oas-tags):
                  - patient-management-controller
  /diagnoses:
    displayName: diagnoses
    get:
      displayName: getPatientManagementDiagnoses
      description: getPatientManagementDiagnoses
      (oas-summary): getPatientManagementDiagnoses
      responses:
        '200':
          body:
            application/json:
              items:
                type: Code
              type: array
              uniqueItems: false
          description: OK
        '401':
          description: Unauthorized
        '403':
          description: Forbidden
        '404':
          description: Not Found
      (oas-tags):
        - patient-management-controller
  /lookuptypes:
    displayName: lookuptypes
    get:
      displayName: getPatientManagementLookupTypes
      description: getPatientManagementLookupTypes
      (oas-summary): getPatientManagementLookupTypes
      responses:
        '200':
          body:
            application/json:
              items:
                type: LookupType
              type: array
              uniqueItems: false
          description: OK
        '401':
          description: Unauthorized
        '403':
          description: Forbidden
        '404':
          description: Not Found
      (oas-tags):
        - patient-management-controller
  /validate:
    displayName: validate
    post:
      displayName: validatePatientManagement
      description: validatePatientManagement
      (oas-summary): validatePatientManagement
      body:
        application/json:
          type: PatientManagement
          (oas-body-name): body
          description: patientManagement
      responses:
        '200':
          description: Description was not specified
        '201':
          description: Created
        '401':
          description: Unauthorized
        '403':
          description: Forbidden
        '404':
          description: Not Found
      (oas-tags):
        - patient-management-controller
/user:
  displayName: user
  '/{userId}':
    displayName: '{userId}'
    /patiententeredobservationheadings:
      displayName: patiententeredobservationheadings
      get:
        displayName: getPatientEnteredObservationHeadings
        description: Given a User ID retrieve a list of available observation types for that user (where they have patient entered observation data).
        (oas-summary): Get Available Patient Entered Observations Types For a User
        responses:
          '200':
            body:
              application/json:
                items:
                  type: ObservationHeading
                type: array
                uniqueItems: false
            description: OK
          '401':
            description: Unauthorized
          '403':
            description: Forbidden
          '404':
            description: Not Found
        (oas-tags):
          - observation-heading-controller
    /observations:
      displayName: observations
      '/{code}':
        displayName: '{code}'
        /patiententered:
          displayName: patiententered
          get:
            displayName: getPatientEnteredObservationsByCode
            description: 'Given a User ID and observation code, retrieve patient entered observations.'
            (oas-summary): Get patient entered Observations of a Certain Type For a User
            responses:
              '200':
                body:
                  '*/*':
                    items:
                      type: FhirObservation
                    type: array
                    uniqueItems: false
                description: OK
              '401':
                description: Unauthorized
              '403':
                description: Forbidden
              '404':
                description: Not Found
            (oas-tags):
              - observation-controller
        uriParameters:
          code:
            description: code
            required: true
            type: string
        get:
          displayName: getObservationsByCode
          description: 'Given a User ID and observation code, retrieve all observations.'
          (oas-summary): Get Observations of a Certain Type For a User
          responses:
            '200':
              body:
                '*/*':
                  items:
                    type: FhirObservation
                  type: array
                  uniqueItems: false
              description: OK
            '401':
              description: Unauthorized
            '403':
              description: Forbidden
            '404':
              description: Not Found
          (oas-tags):
            - observation-controller
      get:
        displayName: getObservationsByCodes
        description: 'Given a User ID and search parameters, retrieve a page of observations.'
        (oas-summary): Get Observations of Multiple Types For a User
        responses:
          '200':
            body:
              '*/*':
                type: FhirObservationPage
            description: OK
          '401':
            description: Unauthorized
          '403':
            description: Forbidden
          '404':
            description: Not Found
        queryParameters:
          code:
            description: code
            items:
              type: string
            required: true
            type: array
            uniqueItems: false
          limit:
            description: limit
            format: int64
            required: true
            type: integer
          offset:
            description: offset
            format: int64
            required: true
            type: integer
          orderDirection:
            description: orderDirection
            required: true
            type: string
        (oas-tags):
          - observation-controller
    /availableobservationheadings:
      displayName: availableobservationheadings
      get:
        displayName: getAvailableObservationHeadings
        description: Given a User ID retrieve a list of available observation types for that user (where they have observation data).
        (oas-summary): Get Available Observations Types For a User
        responses:
          '200':
            body:
              application/json:
                items:
                  type: ObservationHeading
                type: array
                uniqueItems: false
            description: OK
          '401':
            description: Unauthorized
          '403':
            description: Forbidden
          '404':
            description: Not Found
        (oas-tags):
          - observation-heading-controller
/auth:
  displayName: auth
  /login:
    displayName: login
    post:
      displayName: logIn
      description: 'Authenticate using username and password, returns token, which must be added to X-Auth-Token in header of all future requests'
      (oas-summary): Log In
      body:
        application/json:
          type: Credentials
          (oas-body-name): body
          description: credentials
      responses:
        '200':
          body:
            '*/*':
              type: UserToken
          description: Description was not specified
        '201':
          description: Created
        '401':
          description: Unauthorized
        '403':
          description: Forbidden
        '404':
          description: Not Found
      (oas-tags):
        - auth-controller
  '/{token}':
    displayName: '{token}'
    /basicuserinformation:
      displayName: basicuserinformation
      get:
        displayName: getBasicUserInformation
        description: 'Once logged in and have a token, get basic user information including group role membership'
        (oas-summary): Get Basic User Information
        responses:
          '200':
            body:
              '*/*':
                type: User
            description: OK
          '401':
            description: Unauthorized
          '403':
            description: Forbidden
          '404':
            description: Not Found
        (oas-tags):
          - auth-controller
  /logout:
    displayName: logout
    '/{token}':
      displayName: '{token}'
      uriParameters:
        token:
          description: token
          required: true
          type: string
      delete:
        displayName: logOut
        description: Log Out
        (oas-summary): Log Out
        responses:
          '200':
            description: Description was not specified
          '204':
            description: No Content
          '401':
            description: Unauthorized
          '403':
            description: Forbidden
        (oas-tags):
          - auth-controller
/patient:
  displayName: patient
  '/{userId}':
    displayName: '{userId}'
    /basic:
      displayName: basic
      get:
        displayName: getBasicPatientDetails
        description: 'Given a User ID, get basic patient information for a user from clinical data stored in FHIR'
        (oas-summary): Get Basic Patient Information
        responses:
          '200':
            body:
              '*/*':
                items:
                  type: Patient
                type: array
                uniqueItems: false
            description: OK
          '401':
            description: Unauthorized
          '403':
            description: Forbidden
          '404':
            description: Not Found
        (oas-tags):
          - patient-controller
