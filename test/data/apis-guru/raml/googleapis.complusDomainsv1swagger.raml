#%RAML 1.0
title: Google+ Domains
version: v1
baseUri: 'https://www.googleapis.com/plusDomains/v1'
protocols:
  - HTTPS
description: Builds on top of the Google+ platform for Google Apps Domains.
(oas-tags-definition):
  - name: activities
  - name: audiences
  - name: circles
  - name: comments
  - name: media
  - name: people
annotationTypes:
  oas-tags-definition:
    type: array
    items:
      properties:
        name: string
        description?: string
        externalDocs?:
          properties:
            url: string
            description?: string
    allowedTargets: API
  oas-x-apiClientRegistration: any
  oas-x-logo: any
  oas-x-origin: any
  oas-x-preferred: any
  oas-x-providerName: any
  oas-x-serviceName: any
  oas-tags:
    type: 'string[]'
    allowedTargets: Method
  oas-externalDocs:
    properties:
      description?: string
      url: string
    allowedTargets:
      - API
      - Method
      - TypeDeclaration
  oas-info:
    properties:
      termsOfService?: string
      contact?:
        properties:
          name?: string
          url?: string
          email?: string
      license?:
        properties:
          name?: string
          url?: string
    allowedTargets: API
  oas-body-name:
    type: string
    allowedTargets: TypeDeclaration
(oas-info):
  (oas-x-apiClientRegistration):
    url: 'https://console.developers.google.com'
  (oas-x-logo):
    url: 'https://api.apis.guru/v2/cache/logo/https_developers.google.com_+_images_branding_g+128.png'
  (oas-x-origin):
    format: google
    url: 'https://www.googleapis.com/discovery/v1/apis/plusDomains/v1/rest'
    version: v1
  (oas-x-preferred): true
  (oas-x-providerName): googleapis.com
  (oas-x-serviceName): plusDomains
  contact:
    name: Google
    url: 'https://google.com'
(oas-externalDocs):
  url: 'https://developers.google.com/+/domains/'
securitySchemes:
  Oauth2:
    type: OAuth 2.0
    settings:
      authorizationUri: 'https://accounts.google.com/o/oauth2/auth'
      accessTokenUri: ''
      authorizationGrants:
        - implicit
      scopes:
        - 'https://www.googleapis.com/auth/plus.circles.read'
        - 'https://www.googleapis.com/auth/plus.circles.write'
        - 'https://www.googleapis.com/auth/plus.login'
        - 'https://www.googleapis.com/auth/plus.me'
        - 'https://www.googleapis.com/auth/plus.media.upload'
        - 'https://www.googleapis.com/auth/plus.profiles.read'
        - 'https://www.googleapis.com/auth/plus.stream.read'
        - 'https://www.googleapis.com/auth/plus.stream.write'
        - 'https://www.googleapis.com/auth/userinfo.email'
        - 'https://www.googleapis.com/auth/userinfo.profile'
    description: Oauth 2.0 authentication
/circles:
  displayName: circles
  '/{circleId}':
    displayName: '{circleId}'
    /people:
      displayName: people
      put:
        displayName: plusDomains.circles.addPeople
        description: 'Add a person to a circle. Google+ limits certain circle operations, including the number of circle adds. Learn More.'
        responses:
          '200':
            body:
              application/json:
                type: Circle
            description: Successful response
        queryParameters:
          email:
            collectionFormat: multi
            description: 'Email of the people to add to the circle. Optional, can be repeated.'
            items:
              type: string
            type: array
            facets:
              collectionFormat: string
          userId:
            collectionFormat: multi
            description: 'IDs of the people to add to the circle. Optional, can be repeated.'
            items:
              type: string
            type: array
            facets:
              collectionFormat: string
        securedBy:
          - Oauth2:
              scopes:
                - 'https://www.googleapis.com/auth/plus.login'
        (oas-tags):
          - circles
      is:
        - alt
        - fields
        - key
        - oauthToken
        - prettyPrint
        - quotaUser
        - userIp
      delete:
        displayName: plusDomains.circles.removePeople
        description: Remove a person from a circle.
        responses:
          '200':
            description: Successful response
        queryParameters:
          email:
            collectionFormat: multi
            description: 'Email of the people to add to the circle. Optional, can be repeated.'
            items:
              type: string
            type: array
            facets:
              collectionFormat: string
          userId:
            collectionFormat: multi
            description: 'IDs of the people to remove from the circle. Optional, can be repeated.'
            items:
              type: string
            type: array
            facets:
              collectionFormat: string
        securedBy:
          - Oauth2:
              scopes:
                - 'https://www.googleapis.com/auth/plus.login'
        (oas-tags):
          - circles
      get:
        displayName: plusDomains.people.listByCircle
        description: List all of the people who are members of a circle.
        responses:
          '200':
            body:
              application/json:
                type: PeopleFeed
            description: Successful response
        queryParameters:
          maxResults:
            default: 20
            description: 'The maximum number of people to include in the response, which is used for paging. For any response, the actual number returned might be less than the specified maxResults.'
            maximum: 100
            minimum: 1
            type: integer
          pageToken:
            description: 'The continuation token, which is used to page through large result sets. To get the next page of results, set this parameter to the value of "nextPageToken" from the previous response.'
            type: string
        securedBy:
          - Oauth2:
              scopes:
                - 'https://www.googleapis.com/auth/plus.login'
        (oas-tags):
          - people
    uriParameters:
      circleId:
        description: The ID of the circle to get.
        required: true
        type: string
    delete:
      displayName: plusDomains.circles.remove
      description: Delete a circle.
      responses:
        '200':
          description: Successful response
      securedBy:
        - Oauth2:
            scopes:
              - 'https://www.googleapis.com/auth/plus.login'
      (oas-tags):
        - circles
    is:
      - alt
      - fields
      - key
      - oauthToken
      - prettyPrint
      - quotaUser
      - userIp
    patch:
      displayName: plusDomains.circles.patch
      description: Update a circle's description. This method supports patch semantics.
      body:
        application/json:
          type: Circle
          (oas-body-name): body
      responses:
        '200':
          body:
            application/json:
              type: Circle
          description: Successful response
      securedBy:
        - Oauth2:
            scopes:
              - 'https://www.googleapis.com/auth/plus.login'
      (oas-tags):
        - circles
    put:
      displayName: plusDomains.circles.update
      description: Update a circle's description.
      body:
        application/json:
          type: Circle
          (oas-body-name): body
      responses:
        '200':
          body:
            application/json:
              type: Circle
          description: Successful response
      securedBy:
        - Oauth2:
            scopes:
              - 'https://www.googleapis.com/auth/plus.login'
      (oas-tags):
        - circles
    get:
      displayName: plusDomains.circles.get
      description: Get a circle.
      responses:
        '200':
          body:
            application/json:
              type: Circle
          description: Successful response
      securedBy:
        - Oauth2:
            scopes:
              - 'https://www.googleapis.com/auth/plus.login'
      (oas-tags):
        - circles
/people:
  displayName: people
  '/{userId}':
    displayName: '{userId}'
    /people:
      displayName: people
      '/{collection}':
        displayName: '{collection}'
        uriParameters:
          collection:
            description: The collection of people to list.
            enum:
              - circled
            required: true
            type: string
        get:
          displayName: plusDomains.people.list
          description: List all of the people in the specified collection.
          responses:
            '200':
              body:
                application/json:
                  type: PeopleFeed
              description: Successful response
          queryParameters:
            maxResults:
              default: 100
              description: 'The maximum number of people to include in the response, which is used for paging. For any response, the actual number returned might be less than the specified maxResults.'
              maximum: 100
              minimum: 1
              type: integer
            orderBy:
              description: The order to return people in.
              enum:
                - alphabetical
                - best
              type: string
            pageToken:
              description: 'The continuation token, which is used to page through large result sets. To get the next page of results, set this parameter to the value of "nextPageToken" from the previous response.'
              type: string
          securedBy:
            - Oauth2:
                scopes:
                  - 'https://www.googleapis.com/auth/plus.me'
          (oas-tags):
            - people
        is:
          - alt
          - fields
          - key
          - oauthToken
          - prettyPrint
          - quotaUser
          - userIp
    uriParameters:
      userId:
        description: The ID of the person to get the profile for. The special value "me" can be used to indicate the authenticated user.
        required: true
        type: string
    get:
      displayName: plusDomains.people.get
      description: Get a person's profile.
      responses:
        '200':
          body:
            application/json:
              type: Person
          description: Successful response
      securedBy:
        - Oauth2:
            scopes:
              - 'https://www.googleapis.com/auth/userinfo.profile'
      (oas-tags):
        - people
    is:
      - alt
      - fields
      - key
      - oauthToken
      - prettyPrint
      - quotaUser
      - userIp
    /activities:
      displayName: activities
      post:
        displayName: plusDomains.activities.insert
        description: Create a new activity for the authenticated user.
        body:
          application/json:
            type: Activity
            (oas-body-name): body
        responses:
          '200':
            body:
              application/json:
                type: Activity
            description: Successful response
        queryParameters:
          preview:
            description: 'If "true", extract the potential media attachments for a URL. The response will include all possible attachments for a URL, including video, photos, and articles based on the content of the page.'
            type: boolean
        securedBy:
          - Oauth2:
              scopes:
                - 'https://www.googleapis.com/auth/plus.stream.write'
        (oas-tags):
          - activities
      is:
        - alt
        - fields
        - key
        - oauthToken
        - prettyPrint
        - quotaUser
        - userIp
      '/{collection}':
        displayName: '{collection}'
        uriParameters:
          collection:
            description: The collection of activities to list.
            enum:
              - user
            required: true
            type: string
        get:
          displayName: plusDomains.activities.list
          description: List all of the activities in the specified collection for a particular user.
          responses:
            '200':
              body:
                application/json:
                  type: ActivityFeed
              description: Successful response
          queryParameters:
            maxResults:
              default: 20
              description: 'The maximum number of activities to include in the response, which is used for paging. For any response, the actual number returned might be less than the specified maxResults.'
              maximum: 100
              minimum: 1
              type: integer
            pageToken:
              description: 'The continuation token, which is used to page through large result sets. To get the next page of results, set this parameter to the value of "nextPageToken" from the previous response.'
              type: string
          securedBy:
            - Oauth2:
                scopes:
                  - 'https://www.googleapis.com/auth/plus.stream.read'
          (oas-tags):
            - activities
        is:
          - alt
          - fields
          - key
          - oauthToken
          - prettyPrint
          - quotaUser
          - userIp
    /media:
      displayName: media
      '/{collection}':
        displayName: '{collection}'
        uriParameters:
          collection:
            enum:
              - cloud
            required: true
            type: string
        post:
          displayName: plusDomains.media.insert
          description: Add a new media item to an album. The current upload size limitations are 36MB for a photo and 1GB for a video. Uploads do not count against quota if photos are less than 2048 pixels on their longest side or videos are less than 15 minutes in length.
          body:
            application/json:
              type: Media
              (oas-body-name): body
          responses:
            '200':
              body:
                application/json:
                  type: Media
              description: Successful response
          securedBy:
            - Oauth2:
                scopes:
                  - 'https://www.googleapis.com/auth/plus.media.upload'
          (oas-tags):
            - media
        is:
          - alt
          - fields
          - key
          - oauthToken
          - prettyPrint
          - quotaUser
          - userIp
    /circles:
      displayName: circles
      post:
        displayName: plusDomains.circles.insert
        description: Create a new circle for the authenticated user.
        body:
          application/json:
            type: Circle
            (oas-body-name): body
        responses:
          '200':
            body:
              application/json:
                type: Circle
            description: Successful response
        securedBy:
          - Oauth2:
              scopes:
                - 'https://www.googleapis.com/auth/plus.me'
        (oas-tags):
          - circles
      is:
        - alt
        - fields
        - key
        - oauthToken
        - prettyPrint
        - quotaUser
        - userIp
      get:
        displayName: plusDomains.circles.list
        description: List all of the circles for a user.
        responses:
          '200':
            body:
              application/json:
                type: CircleFeed
            description: Successful response
        queryParameters:
          maxResults:
            default: 20
            description: 'The maximum number of circles to include in the response, which is used for paging. For any response, the actual number returned might be less than the specified maxResults.'
            maximum: 100
            minimum: 1
            type: integer
          pageToken:
            description: 'The continuation token, which is used to page through large result sets. To get the next page of results, set this parameter to the value of "nextPageToken" from the previous response.'
            type: string
        securedBy:
          - Oauth2:
              scopes:
                - 'https://www.googleapis.com/auth/plus.me'
        (oas-tags):
          - circles
    /audiences:
      displayName: audiences
      get:
        displayName: plusDomains.audiences.list
        description: List all of the audiences to which a user can share.
        responses:
          '200':
            body:
              application/json:
                type: AudiencesFeed
            description: Successful response
        queryParameters:
          maxResults:
            default: 20
            description: 'The maximum number of circles to include in the response, which is used for paging. For any response, the actual number returned might be less than the specified maxResults.'
            maximum: 100
            minimum: 1
            type: integer
          pageToken:
            description: 'The continuation token, which is used to page through large result sets. To get the next page of results, set this parameter to the value of "nextPageToken" from the previous response.'
            type: string
        securedBy:
          - Oauth2:
              scopes:
                - 'https://www.googleapis.com/auth/plus.me'
        (oas-tags):
          - audiences
      is:
        - alt
        - fields
        - key
        - oauthToken
        - prettyPrint
        - quotaUser
        - userIp
/comments:
  displayName: comments
  '/{commentId}':
    displayName: '{commentId}'
    uriParameters:
      commentId:
        description: The ID of the comment to get.
        required: true
        type: string
    get:
      displayName: plusDomains.comments.get
