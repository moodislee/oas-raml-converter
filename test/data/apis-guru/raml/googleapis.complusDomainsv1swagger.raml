      - prettyPrint
      - quotaUser
      - userIp
/activities:
  displayName: activities
  '/{activityId}':
    displayName: '{activityId}'
    uriParameters:
      activityId:
        description: The ID of the activity to get.
        required: true
        type: string
    get:
      displayName: plusDomains.activities.get
      description: Get an activity.
      responses:
        '200':
          body:
            application/json:
              type: Activity
          description: Successful response
      securedBy:
        - Oauth2:
            scopes:
              - 'https://www.googleapis.com/auth/plus.stream.read'
      (oas-tags):
        - activities
    is:
      - alt
      - fields
      - key
      - oauthToken
      - prettyPrint
      - quotaUser
      - userIp
    /comments:
      displayName: comments
      post:
        displayName: plusDomains.comments.insert
        description: Create a new comment in reply to an activity.
        body:
          application/json:
            type: Comment
            (oas-body-name): body
        responses:
          '200':
            body:
              application/json:
                type: Comment
            description: Successful response
        securedBy:
          - Oauth2:
              scopes:
                - 'https://www.googleapis.com/auth/plus.stream.write'
        (oas-tags):
          - comments
      is:
        - alt
        - fields
        - key
        - oauthToken
        - prettyPrint
        - quotaUser
        - userIp
      get:
        displayName: plusDomains.comments.list
        description: List all of the comments for an activity.
        responses:
          '200':
            body:
              application/json:
                type: CommentFeed
            description: Successful response
        queryParameters:
          maxResults:
            default: 20
            description: 'The maximum number of comments to include in the response, which is used for paging. For any response, the actual number returned might be less than the specified maxResults.'
            maximum: 500
            minimum: 0
            type: integer
          pageToken:
            description: 'The continuation token, which is used to page through large result sets. To get the next page of results, set this parameter to the value of "nextPageToken" from the previous response.'
            type: string
          sortOrder:
            default: ascending
            description: The order in which to sort the list of comments.
            enum:
              - ascending
              - descending
            type: string
        securedBy:
          - Oauth2:
              scopes:
                - 'https://www.googleapis.com/auth/plus.stream.read'
        (oas-tags):
          - comments
    /people:
      displayName: people
      '/{collection}':
        displayName: '{collection}'
        uriParameters:
          collection:
            description: The collection of people to list.
            enum:
              - plusoners
              - resharers
              - sharedto
            required: true
            type: string
        get:
          displayName: plusDomains.people.listByActivity
          description: List all of the people in the specified collection for a particular activity.
          responses:
            '200':
              body:
                application/json:
                  type: PeopleFeed
              description: Successful response
          queryParameters:
            maxResults:
              default: 20
