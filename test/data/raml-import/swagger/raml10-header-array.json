{
  "swagger": "2.0",
  "info": {
    "title": "headers example",
    "version": "1"
  },
  "paths": {
    "/jobs": {
      "post": {
        "description": "Create a job",
        "operationId": "POST_jobs",
        "parameters": [
          {
            "description": "The API key needed to create a new job",
            "in": "header",
            "name": "Zencoder-Api-Key",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "default": {
            "description": "",
            "schema": {}
          }
        }
      }
    },
    "/jobs2": {
      "get": {
        "operationId": "GET_jobs2",
        "parameters": [
          {
            "name": "X-Dept",
            "in": "header",
            "description": "A department code to be charged.\nMultiple of such headers are allowed.\n",
            "required": true,
            "type": "array",
            "items": {
              "x-raml-pattern": "^\\d+\\-\\w+$",
              "x-raml-example": "230-OCTO"
            }
          }
        ],
        "responses": {
          "default": {
            "description": "",
            "schema": {}
          }
        }
      }
    }
  }
}