{
  "swagger": "2.0",
  "info": {
    "version": "",
    "title": "API Platform V2 API"
  },
  "paths": {},
  "definitions": {
    "Mixed": {
      "type": "object",
      "properties": {
        "Email": {
          "type": "string",
          "pattern": "[0-9|-]+",
          "minLength": 2,
          "maxLength": 6
        },
        "Weight": {
          "type": "number",
          "minimum": 3,
          "maximum": 5,
          "format": "int64",
          "multipleOf": 4
        },
        "Age": {
          "type": "integer",
          "minimum": 3,
          "maximum": 5,
          "format": "int8",
          "multipleOf": 1
        },
        "IsMarried": {
          "type": "boolean"
        },
        "birthday": {
          "type": "string",
          "format": "date",
          "example": "2015-05-23"
        },
        "lunchtime": {
          "type": "string",
          "x-raml-format": "time-only",
          "example": "12:30:00"
        },
        "fireworks": {
          "type": "string",
          "x-raml-format": "datetime-only",
          "example": "2015-07-04T21:00:00"
        },
        "created": {
          "type": "string",
          "format": "date-time",
          "example": "2016-02-28T16:41:41.090Z"
        },
        "If-Modified-Since": {
          "type": "string",
          "x-raml-format": "rfc2616",
          "example": "Sun, 28 Feb 2016 16:41:41 GMT"
        }
      },
      "required": [
        "Email",
        "Weight",
        "Age",
        "IsMarried",
        "birthday",
        "lunchtime",
        "fireworks",
        "created",
        "If-Modified-Since"
      ]
    }
  }
}